# description : IO::All of it to Graham and Damian!
# depends : perl.1 perl-io-string

name=perl-io-all
version=0.87
release=3

source="https://cpan.metacpan.org/authors/id/F/FR/FREW/IO-All-$version.tar.gz"
options="!checksum"

build() {
   cd IO-All-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
