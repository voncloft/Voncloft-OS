# description : Perl/CPAN Module String::CRC32 : ZMODEMlike CRC32 generation

name=perl-string-crc32
version=2
release=1

source="https://cpan.metacpan.org/authors/id/L/LE/LEEJO/String-CRC32-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
