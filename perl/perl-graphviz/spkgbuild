# description : GraphViz - Interface to AT&Ts GraphViz. Deprecated. See GraphViz2
# depends : graphviz perl-file-which perl-ipc-run perl-libwww perl-parse-recdescent

name=perl-graphviz
version=2.24
release=6

source="https://cpan.metacpan.org/authors/id/R/RS/RSAVAGE/GraphViz-$version.source.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
