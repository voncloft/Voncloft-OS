# description : Perl/CPAN Module Locale::PO: PO parser

name=perl-locale-po
version=0.27
release=6

source="https://search.cpan.org/CPAN/authors/id/C/CO/COSIMO/Locale-PO-$version.tar.gz"
options="!checksum"

build() {
   cd Locale-PO-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
