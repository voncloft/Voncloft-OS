# description : Perl/CPAN Module DateTime::Event::ICal
# depends : perl-datetime-event-recurrence

name=perl-datetime-event-ical
version=0.13
release=6

source="https://www.cpan.org/authors/id/F/FG/FGLOCK/DateTime-Event-ICal-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
