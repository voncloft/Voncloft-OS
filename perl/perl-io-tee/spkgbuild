# description : Perl/CPAN Module IO::Tee : Multiplex output to multiple handles

name=perl-io-tee
version=0.65
release=3

source="https://cpan.metacpan.org/authors/id/N/NE/NEILB/IO-Tee-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
