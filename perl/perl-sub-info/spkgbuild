# description : Tool for inspecting subroutines.
# depends : perl-importer

name=perl-sub-info
version=0.002
release=7

source="https://search.cpan.org/CPAN/authors/id/E/EX/EXODIST/$_cpanname-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
