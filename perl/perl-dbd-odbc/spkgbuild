# description : ODBC Driver for DBI
# depends : perl-dbi perl unixodbc

name=perl-dbd-odbc
version=1.60
release=4

source="https://search.cpan.org/CPAN/authors/id/M/MJ/MJEVANS/DBD-ODBC-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
