# description : Perl extension for HTML::Element
# depends : perl

name=perl-html-element-extended
version=1.18
release=8

source="https://search.cpan.org/CPAN/authors/id/M/MS/MSISK/HTML-Element-Extended-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
