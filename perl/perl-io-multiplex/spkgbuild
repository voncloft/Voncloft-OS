# description : Perl/CPAN Module IO::Multiplex : Manage Input Output on many file handles

name=perl-io-multiplex
version=1.16
release=6

source="https://search.cpan.org/CPAN/authors/id/B/BB/BBB/IO-Multiplex-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
