# description : Perform diffs on files and record sets 
# depends : perl-algorithm-diff

name=perl-text-diff
version=1.45
release=2

source="https://search.cpan.org/CPAN/authors/id/N/NE/NEILB/Text-Diff-$version.tar.gz"
options="!checksum"

build() {
   cd Text-Diff-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
