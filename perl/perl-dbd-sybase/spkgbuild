# description : Sybase Driver for DBI
# depends : perl-dbi perl freetds

name=perl-dbd-sybase
version=1.16
release=5

source="https://search.cpan.org/CPAN/authors/id/M/ME/MEWP/DBD-Sybase-${version}.tar.gz"
options="!checksum"

build() {
   cd  DBD-Sybase-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
