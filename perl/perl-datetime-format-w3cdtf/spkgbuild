# description : Parse and format W3CDTF datetime strings
# depends : perl perl-datetime

name=perl-datetime-format-w3cdtf
version=0.07
release=4

source="https://search.cpan.org/CPAN/authors/id/G/GW/GWILLIAMS/DateTime-Format-W3CDTF-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
