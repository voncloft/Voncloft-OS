# description : compare perl data structures
# depends : perl-file-find-rule

name=perl-data-compare
version=1.25
release=8

source="https://search.cpan.org/CPAN/authors/id/D/DC/DCANTRELL/Data-Compare-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
