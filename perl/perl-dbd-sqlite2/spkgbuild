# description : Perl/CPAN Module DBD::SQLite2
# depends : perl-dbi

name=perl-dbd-sqlite2
version=0.38
release=4

source="https://search.cpan.org/CPAN/authors/id/R/RU/RURBAN/DBD-SQLite2-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
