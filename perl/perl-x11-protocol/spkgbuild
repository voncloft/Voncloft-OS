# description : Perl/CPAN Module X11::Protocol : Raw interface to X Window System servers
# depends : perl

name=perl-x11-protocol
version=0.56
release=11

source="https://www.cpan.org/authors/id/S/SM/SMCCAM/X11-Protocol-$version.tar.gz"
options="!checksum"

build() {
   cd X11-Protocol-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
