# description : Provide the stuff missing in List::Util
# depends : perl-exporter-tiny perl-list-moreutils-xs

name=perl-list-moreutils
version=0.430
release=1

source="https://search.cpan.org/CPAN/authors/id/R/RE/REHSACK/List-MoreUtils-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
