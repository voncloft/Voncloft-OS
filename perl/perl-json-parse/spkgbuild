# description : JSON::Parse - Read JSON into a Perl variable
# depends : perl

name=perl-json-parse
version=0.61
release=2

source="https://cpan.metacpan.org/authors/id/B/BK/BKB/JSON-Parse-$version.tar.gz"
options="!checksum"

build() {
   cd  JSON-Parse-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
