# description : Create bubble-babble fingerprints
# depends : perl

name=perl-digest-bubblebabble
version=0.02
release=6

source="https://search.cpan.org/CPAN/authors/id/B/BT/BTROTT/Digest-BubbleBabble-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
