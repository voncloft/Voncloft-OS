# description : Automates web page form & link interaction
# depends : perl-libwww perl-html-parser perl-uri perl-http-response-encoding

name=perl-www-mechanize
version=2.03
release=1

source="https://search.cpan.org/CPAN/authors/id/O/OA/OALDERS/WWW-Mechanize-$version.tar.gz"
options="!checksum"

build() {
   cd  WWW-Mechanize-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
