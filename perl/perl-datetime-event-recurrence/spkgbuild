# description : Perl/CPAN Module DateTime::Event::Recurrence

name=perl-datetime-event-recurrence
version=0.19
release=6

source="https://www.cpan.org/authors/id/F/FG/FGLOCK/DateTime-Event-Recurrence-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
