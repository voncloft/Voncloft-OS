# description : Perl/CPAN Module Algorithm::Annotate
# depends : perl-algorithm-diff

name=perl-algorithm-annotate
version=0.10
release=11

source="https://www.cpan.org/authors/id/C/CL/CLKAO/Algorithm-Annotate-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
