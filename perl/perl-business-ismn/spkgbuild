# description : Perl package to work with International Standard Music Numbers
# depends : perl perl-tie-cycle

name=perl-business-ismn
version=1.202
release=4

_dist=Business-ISMN
source="https://search.cpan.org/CPAN/authors/id/B/BD/BDFOY/${_dist}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${_dist}-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
