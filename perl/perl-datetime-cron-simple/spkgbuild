# description : Perl/CPAN Module DateTime::Cron::Simple

name=perl-datetime-cron-simple
version=0.2
release=11

source="https://www.cpan.org/authors/id/B/BI/BITS/DateTime-Cron-Simple-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
