# description : Extremely flexible deep comparison 
# depends : perl-test-nowarnings perl-test-simple

name=perl-test-deep
version=1.128
release=3

source="https://search.cpan.org/CPAN/authors/id/R/RJ/RJBS/Test-Deep-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
