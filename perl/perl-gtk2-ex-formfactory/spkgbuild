# description : Framework for perl-gtk2 development
# depends : gtk2-perl perl

name=perl-gtk2-ex-formfactory
version=0.67
release=5

source="https://www.exit1.org/packages/${_realname}/dist/${_realname}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
