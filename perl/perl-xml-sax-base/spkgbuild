# description : Base class SAX Drivers and Filters
# depends : perl

name=perl-xml-sax-base
version=1.09
release=4

source="https://www.cpan.org/authors/id/G/GR/GRANTM/XML-SAX-Base-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
