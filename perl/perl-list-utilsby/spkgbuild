# description : Higher-order list utility functions
# depends : perl

name=perl-list-utilsby
version=0.11
release=4

source="https://search.cpan.org/CPAN/authors/id/P/PE/PEVANS/$_cpanname-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
