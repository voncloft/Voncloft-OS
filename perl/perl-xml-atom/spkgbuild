# description : Atom feed and API implementation 
# depends : perl-class-data-inheritable perl-uri perl-xml-libxml

name=perl-xml-atom
version=0.42
release=2

source="https://www.cpan.org/authors/id/M/MI/MIYAGAWA/XML-Atom-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
