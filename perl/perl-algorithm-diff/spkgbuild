# description : Perl/CPAN Module Algorithm::Diff : Diff also Longest Common Subsequence
# depends : perl

name=perl-algorithm-diff
version=1.1903
release=6

source="https://search.cpan.org/CPAN/authors/id/T/TY/TYEMQ/Algorithm-Diff-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
