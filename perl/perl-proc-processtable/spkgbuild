# description : Provides a consistent interface to process table information.
# depends : perl

name=perl-proc-processtable
version=0.59
release=2

source="https://search.cpan.org/CPAN/authors/id/J/JW/JWB/Proc-ProcessTable-$version.tar.gz"
options="!checksum"

build() {
   cd Proc-ProcessTable-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
