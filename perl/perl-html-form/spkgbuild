# description : Class that represents an HTML form element
# depends : perl perl-html-parser perl-http-message perl-uri.10

name=perl-html-form
version=6.03
release=7

source="https://search.cpan.org/CPAN/authors/id/G/GA/GAAS/HTML-Form-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
