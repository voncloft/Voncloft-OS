# description : Lists of reserved barewords and symbol names
# depends : perl

name=perl-b-keywords
version=1.21
release=2

source="https://cpan.metacpan.org/authors/id/R/RU/RURBAN/B-Keywords-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
