# description : Data tables useful in parsing HTML
# depends : perl

name=perl-html-tagset
version=3.20
release=10

source="https://www.cpan.org/authors/id/P/PE/PETDANCE/HTML-Tagset-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
