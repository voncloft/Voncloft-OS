# description : Date::Manip - date manipulation routines
# depends : perl

name=perl-date-manip
version=6.83
release=1

source="https://search.cpan.org/CPAN/authors/id/S/SB/SBECK/Date-Manip-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
