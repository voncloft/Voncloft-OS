# description : Extensible, general Perl server engine
# depends : perl

name=perl-net-server
version=2.010
release=6

source="https://search.cpan.org/CPAN/authors/id/R/RH/RHANDOM/Net-Server-$version.tar.gz"
options="!checksum"

build() {
   cd Net-Server-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
