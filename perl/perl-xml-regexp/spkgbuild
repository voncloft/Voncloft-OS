# description : Regular expressions for XML tokens
# depends : perl

name=perl-xml-regexp
version=0.04
release=5

source="https://www.cpan.org/authors/id/T/TJ/TJMATHER/XML-RegExp-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
