# description : Class to compare two arrays
# depends : perl

name=perl-array-compare
version=3.0.3
release=1

source="https://search.cpan.org/CPAN/authors/id/D/DA/DAVECROSS/Array-Compare-v$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
