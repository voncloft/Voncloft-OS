# description : comma-separated values manipulator
# depends : perl

name=perl-text-csv
version=2.01
release=4

_dist=Text-CSV
source="https://cpan.metacpan.org/authors/id/I/IS/ISHIGAKI/${_dist}-${version}.tar.gz"
options="!checksum"

build() {
     cd $_dist-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
