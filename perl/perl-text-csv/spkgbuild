# description : comma-separated values manipulator
# depends : perl

name=perl-text-csv
version=1.97
release=4

source="https://cpan.metacpan.org/authors/id/I/IS/ISHIGAKI/${_dist}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
