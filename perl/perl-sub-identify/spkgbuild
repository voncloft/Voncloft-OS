# description : Retrieve names of code references
# depends : perl

name=perl-sub-identify
version=0.14
release=8

source="https://search.mcpan.org/CPAN/authors/id/R/RG/RGARCIA/Sub-Identify-$version.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
