# description : Perl module for simple Date objects
# depends : perl

name=perl-date-simple
version=3.03
release=9

source="https://search.cpan.org/CPAN/authors/id/I/IZ/IZUT/${_dist}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#perl-}-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
