# description : real big library
# depends : ruby

name=ruby-tins
version=1.26.0
release=2

source="https://rubygems.org/downloads/tins-${version}.gem"
options="!checksum"

build() {
	local _gemdir="$(ruby -e'puts Gem.default_dir')"
	gem install --ignore-dependencies --no-user-install -N -i "${PKG}"/${_gemdir} tins-${version}.gem
}
