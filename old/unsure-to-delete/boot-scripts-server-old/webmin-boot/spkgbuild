# description : webmin boot script
# depends :

name=webmin-boot
version=1.962
release=1

build()
{
	cd $name-$version
	mkdir $PKG/etc/init.d
	install $SRC/webmin $PKG/etc/init.d/
	ln -sfv /etc/init.d/webmin /etc/rc.d/rc5.d/S99webmin
	ln -sfv /etc/init.d/webmin /etc/rc.d/rc4.d/S99webmin
	ln -sfv /etc/init.d/webmin /etc/rc.d/rc3.d/S99webmin
}
