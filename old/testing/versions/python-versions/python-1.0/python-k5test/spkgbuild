
name=python-k5test
version=0.9.2
release=1
# description : Library for setting up self-contained Kerberos 5 environments 
# depends :   python   krb5  
source="https://github.com/pythongssapi/k5test/archive/v${version}/${name}-${version}.tar.gz" 

build   {
  cd "k5test-${version}"
  python setup.py build
}

package   {
  cd "k5test-${version}"
  python setup.py install -O1 --root=$PKG --skip-build
  install -Dm 644 LICENSE.txt -t "${PKG}/usr/share/licenses/${name}"
}

# vim: ts=2 sw=2 et:
