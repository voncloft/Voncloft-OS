name=python-kitchen
version=1.2.6
release=5
# description : Useful snippets of python code"
# depends :   python  
source=" $url/archive/$version.tar.gz" 

build   {
    cd "kitchen-$version"
    python setup.py build
}

package   {
    cd "kitchen-$version"
    python setup.py install --root=$PKG --optimize=1 --skip-build
}
