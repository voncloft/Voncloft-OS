
name=python-mimerender
version=0.6.0
release=6
# description : Transparently select a render function for an HTTP request handler s result"
# depends :   python-mimeparse  
source="https://github.com/martinblech/mimerender/archive/v$version.tar.gz" 

build   {
  cd mimerender-$version
  python setup.py build
}

check   {
  cd mimerender-$version
  python src/test.py
}

package   {
  cd mimerender-$version
  python setup.py install -O1 --root $PKG
}
