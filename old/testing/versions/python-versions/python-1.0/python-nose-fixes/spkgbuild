
name=python-nose-fixes
version=1.3
release=5
# description : A plugin to make nose behave better"
# depends :   python-nose  

build   {
  cd "$srcdir/nose_fixes"
  python setup.py build
}

check   {
  cd "$srcdir/nose_fixes"
  nosetests3
}

package   {
  cd nose_fixes
  python setup.py install --root=$PKG --optimize=1
}
