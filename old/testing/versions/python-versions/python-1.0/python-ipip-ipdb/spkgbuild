
name=python-ipip-ipdb
version=1.5.0
release=1
# description : IPIP.net officially supported IP database ipdb format parsing library"
# depends :   python  
conflicts=  python-ipdb  
source="https://pypi.io/packages/source/i/ipip-ipdb/ipip-ipdb-$version.tar.gz" 

build   {
  cd ipip-ipdb-$version
  python setup.py build
}

check   {
  cd ipip-ipdb-$version
  # TODO: Switch to github source afterhttps://github.com/ipipdotnet/ipdb-python/issues/7 is fixed
  # python ipdb_test.py
}

package   {
  cd ipip-ipdb-$version
  python setup.py install --root=$PKG --optimize=1
}
