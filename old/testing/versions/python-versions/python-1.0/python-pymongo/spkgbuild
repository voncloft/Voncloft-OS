
name=python-pymongo
version=3.11.3
release=1
# description : Python module for using MongoDB 
# depends :   python  
source="https://pypi.io/packages/source/p/pymongo/pymongo-$version.tar.gz" 

build   {
  cd "$srcdir"/pymongo-$version
  python setup.py build
}

package   {
  cd "pymongo-$version"
  python setup.py install --root=$PKG --skip-build --optimize=1
}

# vim:set ts=2 sw=2 et:
