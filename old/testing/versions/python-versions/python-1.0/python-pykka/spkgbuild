
name=python-pykka
version=2.0.3
release=1
# description : Easy to use concurrency abstractions for Python using the actor model"
# depends :   python  
source="https://pypi.python.org/packages/source/P/Pykka/Pykka-$version.tar.gz" 

build   {
  cd "Pykka-$version"
  python setup.py build
}

package   {
  cd "Pykka-$version"
  python setup.py install --prefix=/usr --root=$PKG/ --optimize=1 --skip-build
  install -Dm644 LICENSE "$PKG/usr/share/licenses/$name/LICENSE"
}
