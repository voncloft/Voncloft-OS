
name=python-baron
version=0.9
release=5
# description : Full Syntax Tree for python to make writing refactoring code a realist task"
# depends :   python-rply  
source="https://github.com/PyCQA/baron/archive/$version.tar.gz" 

build   {
  cd baron-$version
  python setup.py build
}

check   {
  cd baron-$version
  python setup.py pytest
}

package   {
  cd baron-$version
  python setup.py install --root=$PKG --optimize=1
}
