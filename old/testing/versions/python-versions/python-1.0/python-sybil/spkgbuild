
name=python-sybil
version=2.0.1
release=1
# description : Automated testing for the examples in your documentation. 
# depends :   python  
source="https://github.com/cjw296/sybil/archive/$version.tar.gz" 

build   {
  cd sybil-$version
  python setup.py build
}

check   {
  cd sybil-$version
  python -m pytest
}

package   {
  cd sybil-$version
  python setup.py install --root=$PKG --optimize=1
}

# vim:set ts=2 sw=2 et:
