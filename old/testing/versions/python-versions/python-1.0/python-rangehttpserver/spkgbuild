
_name=RangeHTTPServer
name=python-rangehttpserver
version=1.2.0
release=6
# description : SimpleHTTPServer with support for Range requests 
# depends :   python  
source= ${name}-${version}.tar.gz::https://github.com/danvk/RangeHTTPServer//archive/${version}.tar.gz 

build   {
   cd ${_name}-${version}
    python setup.py build
   
}

check   {
   cd ${_name}-${version}
    nosetests
   
}

package   {
  cd ${_name}-${version}
  python setup.py install --root=$PKG --optimize=1 --skip-build
}
