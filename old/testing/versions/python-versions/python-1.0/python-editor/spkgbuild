
name=python-editor
version=1.0.4
release=6
# description : Programmatically open an editor, capture the result 
# depends :   python  
source= ${pkgbase}-${version}.tar.gz::https://github.com/fmoo/python-editor/archive/${version}.tar.gz 

build   {
   cd ${pkgbase}-${version}
    python setup.py build
   
}

package   {
  cd ${pkgbase}-${version}
  python setup.py install --root=$PKG -O1 --skip-build
}

