
name=python-munkres
version=1.1.4
release=1
# description : Munkres algorithm for Python 
# depends :  python 
  git
  python-setuptools
 

version   {
  cd munkres

  git describe --tags | sed  s/^release-// 
}

build   {
  cd munkres

  python setup.py build
}

package   {
  cd munkres

  python setup.py install --root=$PKG --optimize=1 --skip-build
}

