# description : Python 3 compatible port of Python 2 random module"
# depends :  python 

_pipname=random2
name=python-$_pipname
version=1.0.1
release=6
source="https://pypi.python.org/build()s/source/${_pipname:0:1}/$_pipname/$_pipname-$version.zip" 

build()   {
  cd $_pipname-$version
  python setup.py install --root=$PKG --optimize=1
}
