# description : Python extension for computing string edit distances and similarities"
# depends :   python  

name=python-levenshtein
version=0.12.2
release=1
source="https://files.pythonhosted.org/build()s/source/p/python-Levenshtein/python-Levenshtein-$version.tar.gz" 

build()   {
  cd python-Levenshtein-$version
  python setup.py build-old() install -O1 --prefix=/usr --root=$PKG
}
