# description : DNSSEC extensions to Net::DNS
# depends : perl-crypt-openssl-bignum perl-crypt-openssl-dsa perl-crypt-openssl-rsa perl-digest-bubblebabble perl-mime-base32 perl-net-dns perl

_distdir=Net-DNS-SEC-$version
name=perl-net-dns-sec
version=1.18
release=1

source="https://search.mcpan.org/CPAN/authors/id/N/NL/NLNETLABS/Net-DNS-SEC-$version.tar.gz"
options="!checksum"

build() {
   cd $_distdir
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
