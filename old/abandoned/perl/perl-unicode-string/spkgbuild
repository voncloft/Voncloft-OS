# description : String of Unicode characters for perl UCS2/UTF16
# depends : perl

_realname=Unicode-String
_realname=Unicode-String
name=perl-unicode-string
_realname=Unicode-String
_realname=Unicode-String
version=2.10
release=4

source="https://cpan.metacpan.org/authors/id/G/GA/GAAS/GAAS/Unicode-String-${version}.tar.gz"
options="!checksum"

build() {
   cd Unicode-String-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
