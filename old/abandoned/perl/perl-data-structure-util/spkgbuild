# description : Perl/CPAN Module Data::Structure::Util

name=perl-data-structure-util
version=0.16
release=10

source="https://search.cpan.org/CPAN/authors/id/A/AN/ANDYA/Data-Structure-Util-$version.tar.gz"
options="!checksum"

build() {
   cd Data-Structure-Util-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
