# description : Date conversion routines
# depends : perl-timedate

name=perl-http-date
version=6.05
release=3

source="https://search.cpan.org/CPAN/authors/id/O/OA/OALDERS/HTTP-Date-$version.tar.gz"
options="!checksum"

build() {
   cd HTTP-Date-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
