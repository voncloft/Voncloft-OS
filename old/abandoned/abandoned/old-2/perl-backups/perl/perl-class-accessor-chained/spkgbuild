# description : make chained accessors
# depends : perl perl-class-accessor

name=perl-class-accessor-chained
version=0.01
release=9

source="https://www.cpan.org/authors/id/R/RC/RCLAMP/Class-Accessor-Chained-$version.tar.gz"
options="!checksum"

build() {
   cd  Class-Accessor-Chained-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
