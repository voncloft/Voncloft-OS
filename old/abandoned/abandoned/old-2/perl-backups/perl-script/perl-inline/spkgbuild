# description : Write Perl subroutines in other languages
# depends : perl perl-parse-recdescent

name=perl-inline
version=0.83
release=2

source="https://cpan.metacpan.org/authors/id/T/TI/TINITA/Inline-$version.tar.gz"
options="!checksum"

build() {
   cd  Inline-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
