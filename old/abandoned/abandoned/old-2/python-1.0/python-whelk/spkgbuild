# description : Easy access to shell commands from python
# depends : python

url=https://pypi.python.org/pypi/whelk
name=python-whelk
version=3.0
release=2

source="https://github.com/seveas/whelk/archive/$version.tar.gz"
options="checksum"

build() {
   cd whelk-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
