# description : Sphinx extension which outputs QtHelp document

_name=sphinxcontrib-qthelp
url=https://github.com/sphinx-doc/sphinxcontrib-qthelp
_name=sphinxcontrib-qthelp
name=python-sphinxcontrib-qthelp
version=1.0.3
release=3

source="https://files.pythonhosted.org/packages/source/${_name::1}/$_name/$_name-$version.tar.gz"
options="checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
