name=python-ply python2-ply
version=3.11
release=7

source="https://pypi.io/packages/source/p/ply/ply-$version.tar.gz"

build() {
   cd ply-$version-py2/test
   cd ply-$version
   cd ply-$version-py2
   cd ply-$version/test
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
