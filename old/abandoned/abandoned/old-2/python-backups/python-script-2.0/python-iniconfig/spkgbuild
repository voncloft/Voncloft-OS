# description : brain-dead simple config-ini parsing
  # depends : python
  # depends : python2

url=https://github.com/RonnyPfannschmidt/iniconfig
name=python-iniconfig python2-iniconfig
version=1.1.1
release=1

source="https://github.com/RonnyPfannschmidt/iniconfig/archive/v$version.tar.gz"

build() {
   cd iniconfig-$version
   cd iniconfig-$version-py2
   cd iniconfig-$version
   cd iniconfig-$version-py2
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
