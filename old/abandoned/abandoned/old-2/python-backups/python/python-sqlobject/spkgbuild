# description : "SQLObject, an object-relational mapper for Python"
# depends : python-pydispatcher

name=python-sqlobject
version=3.9.0
release=1
url="http://sqlobject.org/"
make# depends : python-setuptools
source="${name}-${version}.tar.gz::https://github.com/sqlobject/sqlobject/archive/${version}.tar.gz"
sha256sums=dd720d636e4d098ff2b826628c75ef014fc561935d8711cc0bcd83465bba4c8a

build()-delete{
   cd "sqlobject-$version"
  python setup.py build
}

package_python-sqlobject {
   cd "sqlobject-$version"
  python setup.py build
  python setup.py install --root="$PKG/" --optimize=1 
}
