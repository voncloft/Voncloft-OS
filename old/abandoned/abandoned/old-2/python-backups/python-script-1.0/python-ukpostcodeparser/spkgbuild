# description : UK Postcode parser
# depends : python

url=https://github.com/hamstah/ukpostcodeparser
name=python-ukpostcodeparser
version=1.1.2
release=8

source="https://github.com/hamstah/ukpostcodeparser/archive/v$version.tar.gz"
options="checksum"

build() {
   cd $srcdir/ukpostcodeparser-$version
   cd ukpostcodeparser-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
