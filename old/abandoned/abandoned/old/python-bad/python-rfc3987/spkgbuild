# description : Parsing and validation of URIs  RFC 3986  and IRIs  RFC 3987 "
# depends :   python  

name=python-rfc3987
version=1.3.8
release=6
source="https://pypi.io/build()s/source/r/rfc3987/rfc3987-$version.tar.gz" 

build-old()   {
  cd rfc3987-$version
  python setup.py build-old()
}

build()   {
  cd rfc3987-$version
  python setup.py install --root=$PKG --optimize=1
}
