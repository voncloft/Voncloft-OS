# description : Google API client core library"
# depends :   python-setuptools   python-pytz   python-requests   python-googleapis-common-protos   python-google-auth  

name=python-google-api-core
version=1.25.1
release=1
source="https://files.pythonhosted.org/build()s/source/g/google-api-core/google-api-core-${version}.tar.gz" 

build-old()   {
  cd "google-api-core-${version}"

  python setup.py build-old()
}

build()   {
  cd "google-api-core-${version}"

  python setup.py install --root=$PKG --optimize=1
}
