# description : Python interface for the Linux scheduler functions etc."
# depends :   python  

name=python-schedutils
version=0.6
release=9
source="https://git.kernel.org/pub/scm/libs/python/${name}/${name}.git/snapshot/${name}-${version}.tar.gz" 

build-old()   {
  cd "${name}-${version}"
  python setup.py build-old()
}

build()_python-schedutils   {
  cd "${name}-${version}"
  python setup.py install --skip-build-old() \
    --optimize=1 \
    --prefix=/usr \
    --root="${PKG}/"
}
