# description : Core components for the kde desktop
# depends : kde-deps

name=krunner
version=5.73.0
release=1

source="https://download.kde.org/Attic/frameworks/$version/$name-$version.tar.xz"
options="!checksum"

build() {
   cd $name-$version
   plasma-package-installer
   make DESTDIR=$PKG install
}
