# description : Core components for the kde desktop
# depends : kde-deps

name=ktexteditor
version=5.82.0
release=1

source="https://download.kde.org/stable/frameworks/${version%.*}/$name-$version.tar.xz"
options="!checksum"

build() {
   cd $name-$version
   kf5-package-installer
   cd build
   make DESTDIR=$PKG install
}
