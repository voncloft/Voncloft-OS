# description	: Freedesktop.org menu spec-compliant desktop menus for LXDE

name=lxmenu-data
version=0.1.5
release=1
source="https://downloads.sourceforge.net/lxde/lxmenu-data-0.1.5.tar.xz"

build() {
	cd $name-$version

	./configure --prefix=/usr --sysconfdir=/etc
	make
	make DESTDIR=$PKG install
}

