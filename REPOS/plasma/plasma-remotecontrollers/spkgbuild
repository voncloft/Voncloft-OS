# description : Core components for the kde desktop
# depends : kde-deps plasma-workspace libcec
name=plasma-remotecontrollers
version=5.90.0
release=1

source="https://download.kde.org/unstable/plasma/$version/$name-$version.tar.xz"
options="!checksum"

build() {
   cd $name-$version
   plasma-package-installer
   cd build
   make DESTDIR=$PKG install
}
