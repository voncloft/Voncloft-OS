# description : Core components for the kde desktop
# depends : kde-deps

name=kplotting
version=5.246.0.0
release=1

source="https://download.kde.org/unstable/frameworks/${version%.*}/$name-$version.tar.xz"
options="!checksum"

build() {
   cd $name-$version
   kf-package-installer
   cd build
   make DESTDIR=$PKG install
}
