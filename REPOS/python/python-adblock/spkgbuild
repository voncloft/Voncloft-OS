name=python-adblock
version=0.5.2
release=1

source="https://github.com/ArniDagur/python-adblock/archive/refs/tags/$version.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
