# description : Core components for the kde desktop
# depends : kde-deps kemoticons kitemmodels kparts kunitconversion perl kdoctools kdesignerplugin networkmanager

name=kdelibs4support
version=6.15.0
release=1

source="https://download.kde.org/stable/frameworks/${version%.*}/portingAids/$name-$version.tar.xz"
options="!checksum"

build() {
   cd $name-$version
   kf-package-installer
   cd build
   make DESTDIR=$PKG install
}
