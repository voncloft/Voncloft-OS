# description	: Library for manipulating Unicode strings and C strings

name=libunistring
version=1.1
release=1
source="https://ftp.gnu.org/gnu/$name/$name-$version.tar.xz"

build() {
	cd $name-$version
	./configure --prefix=/usr    \
	            --disable-static
	make
	make DESTDIR=$PKG install
	rm -r $PKG/usr/share
}
