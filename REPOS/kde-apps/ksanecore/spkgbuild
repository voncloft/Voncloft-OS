# description : library interface for scanner
# depends : ki18n sane

name=ksanecore
version=24.01.80
release=1

source="https://download.kde.org/unstable/release-service/$version/src/$name-$version.tar.xz"

build() { 
	cd $name-$version 
	plasma-package-installer
	 cd build
  make DESTDIR=$PKG install
}
