# description : Core components for the kde desktop
# depends : kde-deps kcrash ki18n kirigami-addons kwindowsystem python-ytmusicapi qt5-imageformats qt5-multimedia qt5-svg yt-dlp

name=audiotube
version=25.04.3
release=1

source="https://download.kde.org/stable/release-service/$version/src/$name-$version.tar.xz"
options="!checksum"

build() {
   cd $name-$version
   plasma-package-installer
  cd build
  make DESTDIR=$PKG install
}
