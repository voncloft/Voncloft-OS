# description	: Print an X window dump from xwd
# depends	: libxmu libx11 

name=xpr
version=1.0.5
release=1
source="http://ftp.x.org/pub/individual/app/xpr-$version.tar.bz2"

build() {
	cd $name-$version

  	./configure \
		--prefix=/usr \
		--sysconfdir=/etc \
		--localstatedir=/var \
		--disable-static
	make
	make DESTDIR=$PKG install
}

