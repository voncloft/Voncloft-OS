# description : in-memory daatabase
# depends : grep jemalloc shadow

name=redis
version=8.0.2
release=2

source="https://github.com/redis/redis/archive/$version.tar.gz"
options="!checksum"

build() {
	cd $name-$version
	make BUILD_TLS=yes \
       	USE_SYSTEMD=no \
	#make test
       	make PREFIX=$PKG/usr install
}
