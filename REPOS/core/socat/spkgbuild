# description : Multipurpose relay
# depends : openssl readline

name=socat
version=1.7.4.4
release=2

source="http://www.dest-unreach.org/socat/download/${name}-$version.tar.gz"
options="!checksum"

build() {
	cd $name-$version
	./configure \
		--prefix=/usr \
		--mandir=/usr/share/man \

	make
	make DESTDIR=$PKG install
}
