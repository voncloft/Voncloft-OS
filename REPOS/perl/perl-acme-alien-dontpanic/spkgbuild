# description : Test Module for Alien::Base
# depends : dontpanic perl-alien-build

name=perl-acme-alien-dontpanic
version=2.2900
release=1

source="https://cpan.metacpan.org/authors/id/P/PL/PLICEASE/Acme-Alien-DontPanic-2.2900.tar.gz"
options="!checksum"

build() {
   cd *
   cd *
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
