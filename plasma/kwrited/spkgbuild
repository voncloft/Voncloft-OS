# description : Core components for the kde desktop
# depends : kde-deps

name=kwrited
version=5.23.2
release=1

source="https://download.kde.org/stable/plasma/$version/$name-$version.tar.xz"
options="!checksum"

build() {
   cd $name-$version
   plasma-package-installer
   cd build
   make DESTDIR=$PKG install
}
