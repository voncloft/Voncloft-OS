# description	: List interned atoms defined on server
# depends	: libxcb 

name=xlsatoms
version=1.1.3
release=1
source="http://ftp.x.org/pub/individual/app/xlsatoms-$version.tar.bz2"

build() {
	cd $name-$version

  	./configure \
		--prefix=/usr \
		--sysconfdir=/etc \
		--localstatedir=/var \
		--disable-static
	make
	make DESTDIR=$PKG install
}
