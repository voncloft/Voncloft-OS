# description	: A small youtube browser
# depends	: hicolor-icon-theme qt5 qt5-webkit

name=smtube
version=20.6.0
release=1
source="https://sourceforge.net/projects/smtube/files/SMTube/$version/smtube-$version.tar.bz2"

build() {
	cd $name-$version
	make PREFIX=/usr
	make PREFIX=/usr DESTDIR=$PKG install
}
