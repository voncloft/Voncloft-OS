# description : Port of YUI CSS Compressor to Python
# depends : python

_name=csscompressor
url=https://github.com/sprymix/csscompressor
_name=csscompressor
name=python-csscompressor
version=0.9.5
release=3

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="!checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
