# description : Pure Python GeoIP API
# depends : python

url=https://pypi.python.org/pypi/pygeoip
name=python-pygeoip
version=0.3.2
release=7

source="https://pypi.python.org/packages/source/p/pygeoip/pygeoip-$version.tar.gz"
options="!checksum"

build() {
   cd pygeoip-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
