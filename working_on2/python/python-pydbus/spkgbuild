# description : Pythonic D-Bus library
# depends : python python-gobject

_name=pydbus
url=https://github.com/LEW21/${_name}
_name=pydbus
name=python-${_name}
version=0.6.0
release=7

source="${url}/archive/v${version}.tar.gz"
options="!checksum"

build() {
     cd ${_name}-${version}
#      cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
