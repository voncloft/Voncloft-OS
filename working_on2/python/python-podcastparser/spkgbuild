# description : Simplified, fast RSS parsing library in Python
# depends : python

_name=podcastparser
url=https://github.com/gpodder/podcastparser
_name=podcastparser
name=python-$_name
version=0.6.6
release=1

source="https://github.com/gpodder/$_name/archive/$version.tar.gz"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
