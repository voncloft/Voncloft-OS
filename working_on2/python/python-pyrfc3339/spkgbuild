# description : Parses and generates RFC 3339-compliant timestamps using Python datetime.datetime objects.
# depends : python-pytz

url=https://pypi.python.org/pypi/pyRFC3339
name=python-pyrfc3339
version=1.1
release=7

source="https://pypi.python.org/packages/source/p/pyRFC3339/pyRFC3339-${version}.tar.gz"
options="!checksum"

build() {
   cd pyRFC3339-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
