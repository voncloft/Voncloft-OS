# description : Sphinx themes for Pylons Project documentation
# depends : python

url=https://pylonsproject.org
name=python-pylons-sphinx-themes
version=1.0.13
release=1

source="https://github.com/Pylons/pylons-sphinx-themes/archive/$version/$name-$version.tar.gz"
options="!checksum"

build() {
   cd pylons-sphinx-themes-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
