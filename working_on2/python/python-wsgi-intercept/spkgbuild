# description : Intercept socket connection to wsgi applications for testing.
# depends : python-six

_name=wsgi_intercept
url=https://github.com/cdent/wsgi-intercept
_name=wsgi_intercept
name=python-wsgi-intercept
version=1.9.2
release=2

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"

build() {
   cd ${_name}-${version}
   cd docs
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
