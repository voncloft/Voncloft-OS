# description : Python binding for ICU
# depends : python icu

url=http://pyicu.osafoundation.org/
name=python-pyicu
version=2.6
release=3

source="https://pypi.io/packages/source/P/PyICU/PyICU-$version.tar.gz"
options="!checksum"

build() {
   cd PyICU-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
