name=perl-set-infinite
version=0.65
release=6
url=https://search.cpan.org/dist/Set-Infinite
source="https://www.cpan.org/authors/id/F/FG/FGLOCK/Set-Infinite-$version.tar.gz"
options="!checksum"
sha256sums=07bc880734492de40b4a3a8b5a331762f64e69b4629029fd9a9d357b25b87e1f

build-delete {
   cd  $sr cdir/Set-Infinite-$version
  PERL_MM_USE_DEFAULT=1 perl Makefile.PL INSTALLDIRS=vendor
  make
}

build() {
   cd  $sr cdir/Set-Infinite-$version
  make install DESTDIR=$PKG
  find $PKG -name .packlist -delete
  find $PKG -name *.pod -delete
}
build() {
    cd
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
