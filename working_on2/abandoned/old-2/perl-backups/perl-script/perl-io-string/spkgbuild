# description : IO::File interface for in-core strings
# depends : perl

name=perl-io-string
version=1.08
release=12

source="https://www.cpan.org/authors/id/G/GA/GAAS/IO-String-$version.tar.gz"
options="!checksum"

build() {
   cd  IO-String-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
