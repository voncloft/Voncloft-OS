# description : I/O on in-core objects like strings/arrays
# depends : perl

name=perl-io-stringy
_name=IO-Stringy
version=2.113
release=1

_name=IO-Stringy
source="https://cpan.metacpan.org/authors/id/C/CA/CAPOEIRAB/${_name}-${version}.tar.gz"
options="!checksum"

build() {
   cd  ${_name}-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
