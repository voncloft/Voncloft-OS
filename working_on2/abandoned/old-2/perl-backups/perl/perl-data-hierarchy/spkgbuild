# description : Perl/CPAN Module Data::Hierarchy
# depends : perl-test-exception

name=perl-data-hierarchy
version=0.34
release=11

source="https://www.cpan.org/authors/id/C/CL/CLKAO/Data-Hierarchy-$version.tar.gz"
options="!checksum"

build() {
   cd Data-Hierarchy-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
