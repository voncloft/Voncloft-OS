# description : Recursive copy of nested objects.
# depends : perl

name=perl-clone
version=0.45
release=2

source="https://search.cpan.org/CPAN/authors/id/A/AT/ATOOMIC/Clone-$version.tar.gz"
options="!checksum"

build() {
   cd Clone-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
