name=python-quart-cors
version=0.3.0
release=2

source="git+https://gitlab.com/pgjones/quart-cors.git#tag=d2cb4c71c2afab7b93c8dabdde90d88732482374
options="checksum"

build() {
   cd quart-cors
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
