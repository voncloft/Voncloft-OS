# description : A Python interface to GNU Privacy Guard GnuPG.
# depends : gnupg python

url=http://py-gnupg.sourceforge.net/
name=python-gnupginterface
version=0.3.2
release=9

source="https://downloads.sourceforge.net/sourceforge/py-gnupg/GnuPGInterface-$version.tar.gz"
options="checksum"

build() {
	 cd GnuPGInterface-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
