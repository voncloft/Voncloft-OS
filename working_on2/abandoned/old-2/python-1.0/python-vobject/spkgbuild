# description : Module for parsing and generating vCard and vCalendar files
# depends : python-dateutil python-setuptools

_name=vobject
url=https://eventable.github.io/vobject/
_name=vobject
name=python-vobject
version=0.9.6.1
release=6

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
