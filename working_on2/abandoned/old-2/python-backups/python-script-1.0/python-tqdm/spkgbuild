# description : Fast, Extensible Progress Meter
# depends : python

url=https://github.com/tqdm/tqdm
name=python-tqdm
version=4.56.0
release=1

source="https://github.com/tqdm/tqdm/archive/v$version.tar.gz"
options="checksum"

build() {
   cd tqdm-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
