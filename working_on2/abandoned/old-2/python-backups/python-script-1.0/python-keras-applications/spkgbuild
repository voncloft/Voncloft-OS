# description : Applications module of the Keras deep learning library
# depends : python python-numpy

_name=Keras_Applications
url=https://pypi.org/project/Keras-Applications/
_name=Keras_Applications
name=python-keras-applications
version=1.0.8
release=6

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="checksum"

build() {
   cd $srcdir/${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
