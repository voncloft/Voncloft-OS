# description : HTTPS CredSSP authentication with the requests library
# depends : python python-pyopenssl python-requests python-ntlm-auth

url=https://github.com/jborean93/requests-credssp
name=python-requests-credssp
version=1.2.0
release=2

source="https://github.com/jborean93/requests-credssp/archive/v${version}.tar.gz"
options="checksum"

build() {
     cd ${_module}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
