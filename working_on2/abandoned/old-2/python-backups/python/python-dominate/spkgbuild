# description : "A Python library for creating and manipulating HTML documents using an elegant DOM API"
# depends : python

name=python-dominate
version=2.6.0
release=3
url="https://github.com/Knio/dominate"
make# depends : python-setuptools
check# depends : python-pytest-runner
source="$name-$version.tar.gz::https://github.com/Knio/dominate/archive/$version.tar.gz"
sha512sums=ce32146ab105b5f53e5b1281d3da1166d5704c6972ff294cdb8ecfa69bc88ec49c62998e9e3e58f303389cb6002ae6042e956037a521b118de2d6de35ea712af

build()-delete {
   cd dominate-$version
  python setup.py build
}

check {
   cd dominate-$version
  python setup.py pytest
}

options="!checksum"

build() {
   cd dominate-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1
}
