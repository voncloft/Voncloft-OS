# description : Plot graphs with Gnuplot
# depends : python-numpy gnuplot

pkgbase=python-gnuplot
name=python-gnuplot
version=1.8
release=11
url=https://gnuplot-py.sourceforge.net/
make# depends : git python-setuptools
source="git+https://github.com/yuyichao/gnuplot-py#commit=2c2218dc67"
sha256sums=SKIP

options="!checksum"

build() {
   cd gnuplot-py

  python setup.py build
  python setup.py install   --root=$PKG --optimize=1
}

