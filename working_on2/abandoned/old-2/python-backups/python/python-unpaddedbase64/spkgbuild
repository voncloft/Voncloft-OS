# description : Unpadded Base64
# depends : python

name=python-unpaddedbase64
version=1.1.0
release=7

url=https://pypi.python.org/pypi/unpaddedbase64/

make# depends : python-setuptools

source="python-unpaddedbase64-${version}.tar.gz::https://github.com/matrix-org/python-unpaddedbase64/archive/v$version.tar.gz"

md5sums=e765e946f15e84eeb0dc512259a7d15b

build()-delete {
	 cd "$srcdir"/python-unpaddedbase64-$version
	python setup.py build
}

options="!checksum"

build() {
	 cd python-unpaddedbase64-$version
	python setup.py build
  python setup.py install --root=$PKG --optimize=1 
}

