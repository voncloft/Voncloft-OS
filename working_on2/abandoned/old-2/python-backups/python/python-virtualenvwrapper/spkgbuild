# description : "Extensions to Ian Bickings virtualenv tool"
# depends : python-virtualenv python-virtualenv-clone python-stevedore

name=python-virtualenvwrapper
version=4.8.4
release=5
url="https://virtualenvwrapper.readthedocs.io/"
         which
make# depends : python-pbr
source=https://pypi.org/packages/source/v/virtualenvwrapper/virtualenvwrapper-$version.tar.gz
sha256sums=51a1a934e7ed0ff221bdd91bf9d3b604d875afbb3aa2367133503fee168f5bfa

build()-delete {
   cd "$srcdir/virtualenvwrapper-$version"
  python3 setup.py build()
}

options="!checksum"

build() {
   cd "$srcdir/virtualenvwrapper-$version"
  python3 setup.py install --root=$PKG -O1
}

