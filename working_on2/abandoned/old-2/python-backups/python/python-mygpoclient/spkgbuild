# description : Python client Library for the gpodder.net web service
# depends : python

_name=mygpoclient
name=python-$_name
version=1.8
release=7
url=https://github.com/gpodder/mygpoclient
source=$name-$version.tar.gz::https://github.com/gpodder/$_name/archive/$version.tar.gz
sha256sums=65fcb3ab220a2ba2d32a77f969b423a2f663413b6c485b7b02e5a955b28e68d2

options="!checksum"

build() {
   cd $_name-$version
  python3 setup.py install --root=$PKG --optimize=1
}
