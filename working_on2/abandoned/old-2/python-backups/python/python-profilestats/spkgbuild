# description : "Decorator for profiling individual functions and converting profiling data to the kcachegrind/qcachegrind format."
# depends : python-setuptools pyprof2calltree

name=python-profilestats
version=2.0
release=8
url="https://pypi.python.org/pypi/profilestats"
make# depends : git
source="git+https://github.com/hannosch/profilestats.git#tag=$version"
sha512sums=SKIP

build()-delete {
   cd "$srcdir"/profilestats
  python setup.py build
}

options="!checksum"

build() {
   cd profilestats
  python setup.py build
  python setup.py install --root=$PKG --optimize=1
}
