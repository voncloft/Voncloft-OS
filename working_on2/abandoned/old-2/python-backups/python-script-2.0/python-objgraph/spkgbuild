# description : Draws Python object reference graphs with graphviz
  # depends : python
  # depends : python2

url=https://github.com/mgedmin/objgraph
name=python-objgraph python2-objgraph
version=3.5.0
release=3

source="https://github.com/mgedmin/objgraph/archive/$version.tar.gz"

build() {
   cd objgraph-$version
   cd objgraph-$version-py2
   cd objgraph-$version
   cd objgraph-$version-py2
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
