# description : Bindings to Coin3D
# depends : python soqt

url=https://github.com/coin3d/pivy
name=python-pivy
version=0.6.5
release=3

source="git+$url#commit=cde1297b0e0da2b93d261c9ec24b8ece7d167c91 # tag: 0.6.5
options="checksum"

build() {
   cd pivy
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
