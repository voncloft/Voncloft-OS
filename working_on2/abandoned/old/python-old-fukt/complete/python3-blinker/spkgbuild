# description : Object to object and broadcast signaling 
# depends : python3 

name=python3-blinker
version=1.4
release=9

source="https://pypi.io/packages/source/b/blinker/blinker-$version.tar.gz"
options="!checksum"

build()
{
	cd blinker-$version
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
