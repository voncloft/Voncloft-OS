# description : Object Oriented web development framework for python
# depends : python3

name=python3-cherrypy
version=18.6.0
release=4

source="https://github.com/$name/$name/archive/master.zip"
options="!checksum"

build()
{
	cd $name-master
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
