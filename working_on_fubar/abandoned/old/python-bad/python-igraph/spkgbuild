# description : Python bindings for the igraph library"
# depends :  igraph python-texttable 

name=python-igraph
version=0.8.3
release=2
source="https://pypi.python.org/build()s/source/p/$name/$name-$version.tar.gz" 

build-old()   {
  cd $pkgbase-$version
  python setup.py build-old()
}

build()   {
  cd $pkgbase-$version
  python setup.py install --root=$PKG --optimize=1
}
