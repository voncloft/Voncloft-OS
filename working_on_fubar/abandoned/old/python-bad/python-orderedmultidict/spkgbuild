# description : Ordered multivalue dictionary that retains method parity with dict."
# depends :   python-six  

_name=orderedmultidict
name=python-orderedmultidict
version=1.0.1
release=6
 python-pycodestyle   python-pyflakes  
source="https://files.pythonhosted.org/build()s/source/${_name::1}/${_name}/${_name}-${version}.tar.gz" 

prepare   {
  mv -v "${_name}-${version}" "${name}-${version}"
}

build-old()   {
  cd "${name}-${version}"
  python setup.py build-old()
}

check   {
  cd "${name}-${version}"
  python setup.py test
}

build()   {
  cd "${name}-${version}"
  python setup.py install --skip-build-old() \
    --optimize=1 \
    --prefix=/usr \
    --root=$PKG
}
