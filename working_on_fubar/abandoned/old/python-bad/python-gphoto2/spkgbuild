# description : Python interface to libgphoto2"
# depends :   libgphoto2   python  

name=python-gphoto2
version=2.2.4
release=2
source= https://github.com/jim-easterbrook/$name/archive/v$version.tar.gz 

build-old()   {
  cd $name-$version
  python setup.py build-old()_swig
  python setup.py build-old()
}

build()  {
  cd $name-$version
  python setup.py install --root=$PKG --optimize=1
}
