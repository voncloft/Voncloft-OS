# description : A Python interface to the gpgme GnuPG library"
# depends :   python   gpgme  

name=python-pygpgme
version=0.3
release=9
source="https://launchpad.net/pygpgme/trunk/${version}/+download/pygpgme-${version}.tar.gz" 

build()   {
	cd "${srcdir}/pygpgme-${version}"
	python setup.py install --root=$PKG --optimize=1
}
