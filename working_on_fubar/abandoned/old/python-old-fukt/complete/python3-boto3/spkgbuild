# description : aws interface for python
# depends : python3

name=python3-boto
version=2.49.0
release=5

source="https://pypi.io/packages/source/b/boto/boto-$version.tar.gz"
options="!checksum"

build()
{
	cd 
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
