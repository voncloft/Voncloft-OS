# description : datetime module for python
# depends : python3

name=python3-dateutil
version=2.8.1
release=5

source="https://pypi.io/packages/source/p/python-dateutil/python-dateutil-$version.tar.gz"
options="!checksum"

build()
{
	cd python-dateutil-$version
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
