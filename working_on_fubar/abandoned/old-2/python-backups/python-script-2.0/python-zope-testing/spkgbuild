# description : Zope testing helpers
  # depends : python-setuptools python-zope-interface python-zope-exceptions
  # depends : python2-setuptools python2-zope-interface python2-zope-exceptions

url=https://github.com/zopefoundation/zope.testing
name=python-zope-testing python2-zope-testing
version=4.9
release=1

source="https://github.com/zopefoundation/zope.testing/archive/$version.tar.gz"

build() {
   cd zope.testing-$version
   cd zope.testing-$version-py2
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
