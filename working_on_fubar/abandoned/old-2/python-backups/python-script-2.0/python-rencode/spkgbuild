# description : A Module similar to bencode from the BitTorrent project
# depends : python

url=https://github.com/aresch/rencode
name=python-rencode
version=1.0.6
release=5

source="https://github.com/aresch/rencode/archive/v$version.tar.gz"
options="checksum"

build() {
   cd rencode-$version
   cd rencode-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
