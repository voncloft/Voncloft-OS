# description : A low memory library for creating large XML files.
# depends : python-lxml

url=https://et-xmlfile.readthedocs.io/
name=python-et-xmlfile
version=1.0.1
release=9

source="https://pypi.python.org/packages/source/e/et_xmlfile/et_xmlfile-${version}.tar.gz"
options="checksum"

build() {
   cd $srcdir/et_xmlfile-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
