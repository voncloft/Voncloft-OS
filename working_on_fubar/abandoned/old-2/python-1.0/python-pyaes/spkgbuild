source="https://pypi.org/packages/source/${_pipname:0:1}/$_pipname/$_pipname-$version.tar.gz"

build() {
   cd $_pipname-$version
   cd $_pipname-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
