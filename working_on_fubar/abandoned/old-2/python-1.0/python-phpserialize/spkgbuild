# description : A port of the serialize and unserialize functions of php to python
# depends : python

_name=phpserialize
url=https://github.com/mitsuhiko/phpserialize
_name=phpserialize
name=python-phpserialize
version=1.3
release=7

source="https://github.com/mitsuhiko/${_name}/archive/${version}.tar.gz"
options="checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
