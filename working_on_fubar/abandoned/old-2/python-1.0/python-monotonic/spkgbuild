# description : An implementation of time.monotonic for Python 2.
  # depends : python

_name=monotonic
url=https://pypi.python.org/pypi/monotonic
_name=monotonic
version=1.5
release=6

source="https://github.com/atdt/monotonic/archive/$version.tar.gz"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
