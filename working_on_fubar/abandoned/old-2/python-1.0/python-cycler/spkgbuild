# description : Composable style cycles
# depends : python-six

url=https://github.com/matplotlib/cycler
name=python-cycler
version=0.10.0
release=8

source="https://github.com/matplotlib/cycler/archive/v$version/$name-$version.tar.gz"
options="checksum"

build() {
   cd cycler-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
