# description	: MATE menu editing tooldictionaries and searching files
# depends	: mate-menus

name=mozo
version=1.24.0
release=1
source="https://pub.mate-desktop.org/releases/${version%.*}/$name-$version.tar.xz"

build() {
	cd $name-$version
	./configure --prefix=/usr --disable-nls
	make
	make DESTDIR=$PKG install
}
