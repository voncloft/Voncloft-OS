# description : Concurrent appendable key-value storage
# depends : 

_pkg=partd
url=https://github.com/dask/partd
name=python-${_pkg}
version=1.1.0
release=3

source="https://files.pythonhosted.org/packages/source/${_pkg::1}/${_pkg}/${_pkg}-${version}.tar.gz"
options="checksum"

build() {
   cd ${_pkg}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
