# description : Provides both low-level and high level wrappers around the GSSAPI C libraries
# depends : python python-decorator krb5

url=https://github.com/pythongssapi/python-gssapi
name=python-gssapi
version=1.6.12
release=2

source="https://github.com/pythongssapi/python-gssapi/archive/v${version}/${name}-${version}.tar.gz"
options="checksum"

build() {
   cd ${name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
