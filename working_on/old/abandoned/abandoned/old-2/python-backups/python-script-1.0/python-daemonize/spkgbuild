# description : Library to enable your code run as a daemon process on Unix-like systems
# depends : python

url=https://pypi.python.org/pypi/daemonize/
name=python-daemonize
version=2.5.0
release=4

source="https://pypi.org/packages/source/d/daemonize/daemonize-$version.tar.gz"
options="checksum"

build() {
	 cd $srcdir/daemonize-$version
	 cd daemonize-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
