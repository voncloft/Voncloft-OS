# description : Experimental type system extensions for programs checked with the mypy typechecker
# depends : python

name=python-mypy_extensions
version=0.4.3
release=3
url="http://www.mypy-lang.org/"
make# depends : python-setuptools
source="$name-$version.tar.gz::https://pypi.org/packages/source/m/mypy_extensions/mypy_extensions-$version.tar.gz"
sha256sums=2d82818f5bb3e369420cb3c4060a7970edba416647068eb4c5343488a6c604a8

build()-delete {
     cd "mypy_extensions-$version"
    python setup.py build
}

options="!checksum"

build() {
     cd "mypy_extensions-$version"
    python setup.py build
  python setup.py install --prefix="/usr"--root=$PKG -optimize=1 
}
