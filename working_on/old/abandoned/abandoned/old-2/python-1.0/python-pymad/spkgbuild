# description : Python interface to libmad
# depends : libmad python

url=https://github.com/jaqx0r/pymad
name=python-pymad
version=0.10
release=4

source="$name::git+https://github.com/jaqx0r/pymad.git#tag=version/${version/_/-}
options="checksum"

build() {
   cd $name
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
