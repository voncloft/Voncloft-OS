# description : Provides an uniform layer to support PyQt5, PyQt4 and PySide with a single codebase
# depends : python

_name=qtpy
url=https://github.com/spyder-ide/${_name}/
name=python-qtpy
_name=qtpy
version=1.9.0
release=5

source="python-https://github.com/spyder-ide/${_name}/archive/v${version}.tar.gz"
options="checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
