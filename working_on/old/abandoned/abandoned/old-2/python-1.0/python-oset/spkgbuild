# description : Ordered sets for Python

url=https://pypi.org/project/oset/
name=python-oset
version=0.1.3
release=7

source="https://pypi.io/packages/source/o/oset/oset-$version.tar.gz"
options="checksum"

build() {
   cd oset-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
