# description : Python Xapp Library
# depends : python python-psutil

_name=python3-xapp
url=https://github.com/linuxmint/${_name}
_name=python3-xapp
name=python-xapp
version=2.0.2
release=2

source="${url}/archive/${version}.tar.gz"
options="checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
