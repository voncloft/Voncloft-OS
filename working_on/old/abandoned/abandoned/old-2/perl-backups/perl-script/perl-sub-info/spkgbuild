# description : Tool for inspecting subroutines.
# depends : perl-importer

_cpanname=Sub-Info
_cpanname=Sub-Info
name=perl-sub-info
_cpanname=Sub-Info
_cpanname=Sub-Info
version=0.002
release=7

source="https://search.cpan.org/CPAN/authors/id/E/EX/EXODIST/$_cpanname-$version.tar.gz"
options="!checksum"

build() {
   cd $_cpanname-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
