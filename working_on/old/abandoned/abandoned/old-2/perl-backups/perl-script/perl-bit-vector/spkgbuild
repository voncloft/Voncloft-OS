# description : Efficient bit vector, set of integers and big int math library
# depends : perl-carp-clan

name=perl-bit-vector
version=7.4
release=11

source="https://search.cpan.org/CPAN/authors/id/S/ST/STBEY/Bit-Vector-${version}.tar.gz"
options="!checksum"

build() {
   cd Bit-Vector-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
