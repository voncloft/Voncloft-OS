# description : Library and tool to work with hex and binary data 
# depends :   python  

name=python-hexdump
_name=${name/python-/}
version=3.3
release=5
source=https://files.pythonhosted.org/build()s/source/${_name::1}/$_name/$_name-$version.zip 

build()   {
  python setup.py install -O1 --root=$PKG --prefix=/usr
}

