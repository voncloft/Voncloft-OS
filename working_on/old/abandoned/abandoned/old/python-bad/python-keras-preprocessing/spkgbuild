# description : Preprocessing module of the Keras deep learning library"
# depends :   python   python-numpy  
_name=Keras_Preprocessing
name=python-keras-preprocessing
version=1.1.2
release=4
source="https://files.pythonhosted.org/build()s/source/${_name::1}/${_name}/${_name}-${version}.tar.gz" 

build-old()   {
  cd "$srcdir/${_name}-${version}"
  python setup.py build-old()
}

build()   {
  cd "$srcdir/${_name}-${version}"
  python setup.py install --root=$PKG/ --optimize=1
}
