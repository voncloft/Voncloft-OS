# description : Python interface to libmad 
# depends :   libmad   python  

name=python-pymad
version=0.10
release=4

build-old()   {
  cd $name
  python config_unix.py --prefix=/usr
}

build()   {
  cd $name
  python setup.py install --root=$PKG
}

