# description : Track the number of times subs are called
# depends : perl-hook-lexwrap

name=perl-test-subcalls
version=1.10
release=3

source="https://cpan.metacpan.org/authors/id/E/ET/ETHER/Test-SubCalls-$version.tar.gz"
options="!checksum"

build() {
   cd Test-SubCalls-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
