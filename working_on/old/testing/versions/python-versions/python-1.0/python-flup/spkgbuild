
name=python-flup
version=1.0.3
release=6
# description : Random assortment of WSGI servers"
# depends :   python  
source="https://pypi.io/packages/source/f/flup/flup-$version.tar.gz" 

build   {
  cd flup-$version
  python setup.py build
}

package_python-flup   {
  cd flup-$version
  python setup.py install --root=$PKG --optimize=1
}
