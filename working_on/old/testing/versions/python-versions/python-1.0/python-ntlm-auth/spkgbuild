
name=python-ntlm-auth
_module= ntlm-auth 
version=1.5.0
release=3
# description : Creates NTLM authentication structures"
# depends :   python   python-six   python-cryptography  
source="${name}-${version}.tar.gz::https://github.com/jborean93/ntlm-auth/archive/v${version}.tar.gz" 

build   {
    cd "${_module}-${version}"
    python setup.py build
}

package   {
    cd "${_module}-${version}"
    python setup.py install --root=$PKG --optimize=1 --skip-build
}
