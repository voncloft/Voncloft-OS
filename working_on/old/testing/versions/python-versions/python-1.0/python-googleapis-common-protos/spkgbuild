
name=python-googleapis-common-protos
version=1.52.0
release=3
# description : Python classes generated from the common protos in the googleapis repository"
# depends :   python-protobuf  
source="https://files.pythonhosted.org/packages/source/g/googleapis-common-protos/googleapis-common-protos-${version}.tar.gz" 

build   {
  cd "googleapis-common-protos-${version}"

  python setup.py build
}

package   {
  cd "googleapis-common-protos-${version}"

  python setup.py install --root=$PKG --optimize=1

  chmod -R +r $PKG
}
