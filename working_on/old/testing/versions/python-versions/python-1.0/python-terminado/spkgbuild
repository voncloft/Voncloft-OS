
name=python-terminado
_name=terminado
version=0.9.2
release=1
# description : Terminals served to term.js using Tornado websockets"
# depends :   python-tornado   python-ptyprocess  
source="https://files.pythonhosted.org/packages/source/t/terminado/terminado-$version.tar.gz" 

package   {
  cd "$srcdir/$_name-$version"
  python3 setup.py install --root="$PKG/" --prefix=/usr --optimize=0
  install -Dm644 LICENSE "$PKG/usr/share/licenses/python-$_name/LICENSE"
}
