
name=python-manuel
version=1.10.1
release=5
# description : Manuel lets you build tested documentation"
# depends :   python-setuptools  
source="https://github.com/benji-york/manuel/archive/$version.tar.gz" 

build   {
  cd manuel-$version
  python setup.py build
}

check   {
  cd manuel-$version
  python setup.py test
}

package   {
  cd manuel-$version
  python setup.py install --root=$PKG --optimize=1
}
