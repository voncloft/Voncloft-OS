
name=python-dockerpty
version=0.4.1
release=8
# description : Python library to use the pseudo-tty of a docker container"
# depends :   python-docker-py   python-six  

build   {
  cd "$srcdir/dockerpty"
  python setup.py build
}

check   {
  cd "$srcdir/dockerpty"
  py.test
}

package   {
  cd dockerpty
  python setup.py install --root=$PKG --optimize=1
}
