
name=python-rpy2
version=3.4.2
release=1
# description : A very simple, yet robust, Python interface to the R Programming Language."
# depends :  python-cffi python-numpy python-tzlocal r 
source="https://pypi.io/packages/source/r/rpy2/rpy2-$version.tar.gz" 

package   {
  cd rpy2-$version

  python setup.py install --prefix=/usr --install-data=/usr/share --root=$PKG --optimize=1
}
