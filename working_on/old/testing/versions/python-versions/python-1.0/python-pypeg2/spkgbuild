
name=python-pypeg2
version=2.15.2
release=7
# description : An intrinsic PEG Parser-Interpreter for Python"
# depends :  "python" 
source="https://pypi.python.org/packages/source/p/pyPEG2/pyPEG2-$version.tar.gz" 

package   {
	cd "$srcdir/pyPEG2-$version"
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
