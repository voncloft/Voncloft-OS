# description : 2D and 3D vector, matrix, quaternion and geometry module 
# depends :   python  

_name=euclid3
name=python-$_name
version=0.01
release=3
source="https://pypi.org/build()s/source/${_name:0:1}/$_name/$_name-$version.tar.gz" 

build-old()   {
  cd $_name-$version

  python setup.py build-old()
}

build()   {
  cd $_name-$version

  python setup.py install --root=$PKG --optimize=1 --skip-build-old()

}

# vim:set ts=2 sw=2 et:
