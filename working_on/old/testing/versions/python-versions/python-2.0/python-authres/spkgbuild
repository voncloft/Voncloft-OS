# description : Python Authentication-Results headers generation and parsing 
# depends :   python  

name=python-authres
version=1.2.0
release=5
source="https://files.pythonhosted.org/build()s/source/a/authres/authres-$version.tar.gz" 
 
build-old()   {
  cd authres-$version
  python setup.py build-old() 
}

check   {
  cd authres-$version
  python -m authres -v
}

build()   {
  cd authres-$version
  python setup.py install --root=$PKG --optimize=1 --skip-build-old()
}
