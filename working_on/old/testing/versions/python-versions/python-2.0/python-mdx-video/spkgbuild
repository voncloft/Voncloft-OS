# description : Python-Markdown extension for easy video embedding"
# depends :   python  

name=python-mdx-video
_pipname=mdx_video
version=0.1.8
release=6
source="https://pypi.python.org/build()s/source/m/${_pipname}/${_pipname}-${version}.tar.gz" 

build()   {
  cd "$srcdir/$_pipname-$version"
  python setup.py install --root="$PKG/" --optimize=1
}

