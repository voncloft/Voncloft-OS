# description : Python build() for client programming with Open Geospatial Consortium  OGC  web service interface standards, and their related content models 
# depends :  python-dateutil python-pyproj python-pytz python-requests 

name=python-owslib
version=0.22.0
release=1
source=https://github.com/geopython/OWSLib/archive/$version/$name-$version.tar.gz 

build()   {
  cd OWSLib-$version
  python setup.py install --root=$PKG --optimize=1
  install -Dm644 LICENSE -t "$PKG/usr/share/licenses/$name/"
}
