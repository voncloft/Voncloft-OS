# description : A Python implementation of the JSON5 data format"
# depends :  python 

name=python-json5
version=0.9.5
release=3
source= $name-$version::"https://github.com/dpranke/pyjson5/archive/v$version.tar.gz" 

build-old()   {
  cd pyjson5-$version
  
  python setup.py build-old()
}

build()   {
  cd pyjson5-$version

  python setup.py install --prefix=/usr --root=$PKG --skip-build-old() --optimize=1
}
