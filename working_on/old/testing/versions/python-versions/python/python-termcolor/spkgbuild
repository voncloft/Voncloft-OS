# description : ANSII Color formatting for output in terminal"
# depends :   python  

name=python-termcolor
version=1.1.0
release=10
source=https://pypi.python.org/build()s/source/t/termcolor/termcolor-$version.tar.gz 

build()   {
  cd "$srcdir"/termcolor-$version
  python setup.py install --root=$PKG
}
