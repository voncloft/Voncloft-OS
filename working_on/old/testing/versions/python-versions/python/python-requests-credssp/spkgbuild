# description : HTTPS CredSSP authentication with the requests library"
# depends :   python   python-pyopenssl   python-requests   python-ntlm-auth  

name=python-requests-credssp
_module= requests-credssp 
version=1.2.0
release=2
source="${name}-${version}.tar.gz::https://github.com/jborean93/requests-credssp/archive/v${version}.tar.gz" 

build-old()   {
    cd "${_module}-${version}"
    python setup.py build-old()
}

build()   {
    cd "${_module}-${version}"
    python setup.py install --root=$PKG --optimize=1 --skip-build-old()
}
