name=python-pafy
version=0.5.5
release=3



source="python-pafy-$version.tar.gz::https://github.com/mps-youtube/pafy/archive/v$version.tar.gz" 


build()_python-pafy   {
	cd pafy-$version
	python setup.py install --root=$PKG --optimize=1
}
