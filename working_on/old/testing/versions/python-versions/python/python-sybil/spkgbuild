# description : Automated testing for the examples in your documentation. 
# depends :   python  

name=python-sybil
version=2.0.1
release=1
source="https://github.com/cjw296/sybil/archive/$version.tar.gz" 

build-old()   {
  cd sybil-$version
  python setup.py build-old()
}

check   {
  cd sybil-$version
  python -m pytest
}

build()   {
  cd sybil-$version
  python setup.py install --root=$PKG --optimize=1
}

