# description : Python interface to the SANE scanner and frame grabber.
# depends : sane python

url=https://python-sane.readthedocs.io/en/latest/
name=python-sane
version=2.8.3
release=6

source="https://github.com/python-pillow/Sane/archive/v${version}.tar.gz"
options="!checksum"

build() {
   cd Sane-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
