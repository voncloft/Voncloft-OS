# description : Generate RSS2 using a Python data structure
# depends : python

_name=PyRSS2Gen
url=http://www.dalkescientific.com/Python/PyRSS2Gen.html
_name=PyRSS2Gen
name=python-pyrss2gen
version=1.1
release=12

source="http://www.dalkescientific.com/Python/${_name}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
