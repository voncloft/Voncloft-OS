# description : A semi hard Cornish cheese, also queries PyPI PyPI client
# depends : python-requests

url=https://github.com/kura/yarg
name=python-yarg
version=0.1.9
release=6

source="https://github.com/kura/yarg/archive/$version.tar.gz"
options="!checksum"

build() {
   cd yarg-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
