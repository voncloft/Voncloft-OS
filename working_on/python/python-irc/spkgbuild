# description : IRC Internet Relay Chat protocol client library for Python
# depends : python-six python-pytz python-more-itertools python-jaraco

url=https://pypi.python.org/pypi/irc
name=python-irc
version=19.0.1
release=2

source="https://files.pythonhosted.org/packages/source/i/irc/irc-$version.tar.gz"
options="!checksum"

build() {
   cd irc-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
