# description : Transparently select a render function for an HTTP request handlers result
# depends : python-mimeparse

url=https://github.com/martinblech/mimerender
name=python-mimerender
version=0.6.0
release=6

source="https://github.com/martinblech/mimerender/archive/v$version.tar.gz"
options="!checksum"

build() {
   cd mimerender-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
