# description : Manage OSS licenses: retrieve information, generate
# depends : python-attrs python-requests

_name=dephell-licenses
url=https://github.com/dephell/${_name/-/_}
name=python-dephell-licenses
version=0.1.7
release=3

source="https://files.pythonhosted.org/packages/source/${_name:0:1}/${_name}/${_name}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
