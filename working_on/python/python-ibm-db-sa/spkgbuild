# description : SQLAlchemy support for IBM Data Servers
# depends : python-sqlalchemy

url=https://pypi.python.org/pypi/ibm_db_sa
name=python-ibm-db-sa
version=0.3.5
release=4

source="https://pypi.io/packages/source/i/ibm_db_sa/ibm_db_sa-$version.tar.gz"
options="!checksum"

build() {
   cd ibm_db_sa-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
