# description : Fast HTTP parser
# depends : python http-parser

_name=httptools
url=https://github.com/MagicStack/httptools
_name=httptools
name=python-$_name
version=0.1.1
release=4

source="$url/archive/v$version.tar.gz"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
