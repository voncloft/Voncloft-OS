# depends : python-docutils python-more-itertools

url=https://github.com/jaraco/pytest-checkdocs
name=python-pytest-checkdocs
version=2.2.0
release=1

source="https://github.com/jaraco/pytest-checkdocs/archive/v$version/$name-$version.tar.gz"
options="!checksum"

build() {
   cd pytest-checkdocs-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
