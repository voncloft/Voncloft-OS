# description : Configuration manager in your pocket
# depends : python-yaml

_name=kaptan
url=https://emre.github.io/kaptan/
_name=kaptan
name=python-kaptan
version=0.5.12
release=5

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
