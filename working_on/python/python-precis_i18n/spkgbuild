# description : PRECIS framework RFC 8264, RFC 8265, RFC 8266
# depends : python

_name=precis_i18n
url=https://github.com/byllyfish/precis_i18n
_name=precis_i18n
name=python-precis_i18n
version=1.0.2
release=3

source="https://github.com/byllyfish/precis_i18n/archive/v${version}/${name}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
