# description : Zope testing frameworks
  # depends : python

_name=${pkgbase#python-}
url=https://github.com/zopefoundation/$_name
_name=${pkgbase#python-}
version=2.0
release=5

source="$url/archive/$version.tar.gz"

build() {
   cd $_name-$version
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
