name=python-pysimplesoap
version=1.16.2
release=5

source="https://github.com/pysimplesoap/pysimplesoap/archive/$_commit.tar.gz"
options="!checksum"

build() {
   cd pysimplesoap-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
