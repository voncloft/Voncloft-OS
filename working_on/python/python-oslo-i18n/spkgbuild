# description : Oslo i18n library
# depends : python-six python-pbr python-babel

url=https://pypi.python.org/pypi/oslo.i18n/$version
name=python-oslo-i18n
version=5.0.1
release=3

source="https://github.com/openstack/oslo.i18n/archive/$version.tar.gz"
options="!checksum"

build() {
   cd oslo.i18n-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
