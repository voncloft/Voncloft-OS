# description : Low-level AMQP client for Python fork of amqplib
# depends : python-vine

name=python-amqp
version=5.0.5
release=1

source="https://github.com/celery/py-amqp/archive/v$version/$name-$version.tar.gz"
options="!checksum"

build() {
  cd py-amqp-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1
}

