# description : Oslo Utility library
# depends : python-six python-pbr python-iso8601 python-oslo-i18n python-pytz

url=https://pypi.python.org/pypi/oslo.utils/$version
name=python-oslo-utils
version=4.7.0
release=3

source="https://github.com/openstack/oslo.utils/archive/$version.tar.gz"
options="!checksum"

build() {
   cd oslo.utils-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
