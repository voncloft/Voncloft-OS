name=python-js2py
version=0.70
release=3

source="https://github.com/PiotrDabkowski/Js2Py/archive/$_commit.tar.gz"
options="!checksum"

build() {
   cd Js2Py-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
