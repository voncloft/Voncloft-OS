# description : A pythonic, object-oriented web development framework
# depends : python python-six python-cheroot python-portend python-jaraco

_name=${name#python-}
url=https://cherrypy.org
name=python-cherrypy
version=18.6.0
release=4

source="https://github.com/${_name}/${_name}/archive/v$version.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
