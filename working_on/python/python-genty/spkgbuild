# description : Allows you to run a test with multiple data sets
# depends : python-six

url=https://github.com/box/genty
name=python-genty
version=1.3.2
release=7

source="https://github.com/box/genty/releases/download/v1.3.2/genty-$version.tar.gz"
options="!checksum"

build() {
   cd genty-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
