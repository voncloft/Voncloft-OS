# description : Python-based IP-XACT parser
# depends : python python-yaml

_name=ipyxact
url=https://github.com/olofk/ipyxact
_name=ipyxact
name=python-$_name
version=0.2.4
release=3

source="$url/archive/v$version.tar.gz"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
