# description : Powerful and Lightweight Python Tree Data Structure
# depends : python-six

name=python-anytree
version=2.8.0
release=3

source="https://github.com/c0fec0de/anytree/archive/$version.tar.gz" 
options="!checksum"

build() {
  cd anytree-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1 
}
