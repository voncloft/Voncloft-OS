# description : Various parsers for ECMA standards
# depends : python-ply

_name=calmjs.parse
url=https://github.com/calmjs/calmjs.parse
name=python-calmjs.parse
version=1.2.5
release=5

source="https://github.com/calmjs/$_name/archive/$version.tar.gz"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
