# description : GLib based websocket server written in python
# depends : python-gobject

_name=gwebsockets
url=https://github.com/sugarlabs/gwebsockets
name=python-gwebsockets
version=0.7
release=3

source="https://github.com/sugarlabs/$_name/archive/v$version.tar.gz"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
