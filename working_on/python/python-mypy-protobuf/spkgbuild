# description : Generate mypy stub files from protobuf specs
# depends : mypy protobuf python python-protobuf

url=https://github.com/dropbox/mypy-protobuf
name=python-mypy-protobuf
version=1.23
release=3

source="https://files.pythonhosted.org/packages/source/m/mypy-protobuf/mypy-protobuf-$version.tar.gz"
options="!checksum"

build() {
   cd mypy-protobuf-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
