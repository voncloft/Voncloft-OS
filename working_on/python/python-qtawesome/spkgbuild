# description : Iconic fonts in PyQt and PySide applications
# depends : python-six python-qtpy

_name=qtawesome
url=https://github.com/spyder-ide/${_name}/
name=python-qtawesome
_name=qtawesome
version=0.6.0
release=5

source="python-https://github.com/spyder-ide/${_name}/archive/v${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
