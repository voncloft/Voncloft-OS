# description : computing with and displaying bytes
# depends : python python-justbases

_name=justbytes
url=https://pypi.org/project/justbytes/
name=python-justbytes
_name=justbytes
version=0.14
release=3

source="https://github.com/mulkieran/${_name}/archive/v${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
