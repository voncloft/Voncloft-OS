# description : Asyncio-based multidict implementation for Python
# depends : python glibc

_name=multidict
url=https://github.com/aio-libs/multidict
_name=multidict
name=python-${_name}
version=5.1.0
release=1

source="https://github.com/aio-libs/multidict/archive/v${version}/${name}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
