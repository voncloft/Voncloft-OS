# description : A messaging library for Python
# depends : python-amqp

url=https://kombu.readthedocs.org/
name=python-kombu
version=5.0.2
release=3

source="https://github.com/celery/kombu/archive/v$version.tar.gz"
options=!emptydirs
options="!checksum"

build() {
   cd kombu-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
