# description : Python client Library for the gpodder.net web service
# depends : python

_name=mygpoclient
url=https://github.com/gpodder/mygpoclient
_name=mygpoclient
name=python-$_name
version=1.8
release=7

source="https://github.com/gpodder/$_name/archive/$version.tar.gz"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
