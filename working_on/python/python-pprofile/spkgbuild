# description : Line-granularity, thread-aware deterministic and statistic pure-python profiler
# depends : python

_name=pprofile
url=https://github.com/vpelletier/pprofile
_name=pprofile
name=python-pprofile
version=2.0.5
release=2

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="!checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
