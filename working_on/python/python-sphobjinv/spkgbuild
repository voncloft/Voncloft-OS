# description : Manipulate and inspect Sphinx objects.inv files
# depends : python python-attrs python-certifi python-fuzzywuzzy

_name=${name#python-}
url=https://github.com/bskinn/sphobjinv
name=python-sphobjinv
_name=${name#python-}
version=2.0.1
release=2

source="https://files.pythonhosted.org/packages/source/${_name::1}/$_name/$_name-$version.tar.gz"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
