# description : Helpers for better testing
# depends : python-mock

_pypiname=exam
url=https://pypi.python.org/pypi/exam
name=python-exam
version=0.10.6
release=8

source="https://github.com/Fluxx/exam/archive/$version.tar.gz"
options="!checksum"

build() {
   cd $_pypiname-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
