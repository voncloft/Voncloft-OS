# description : Rtree: spatial index for Python GIS
# depends : python spatialindex

url=https://toblerity.github.com/rtree/
name=python-rtree
version=0.9.7
release=2

source="https://github.com/Toblerity/rtree/archive/$version/$name-$version.tar.gz"
options="!checksum"

build() {
   cd rtree-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
