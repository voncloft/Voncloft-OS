# description : Mock/Stub/Spy library for Python
# depends : python

url=https://has207.github.com/flexmock
name=python-flexmock
version=0.10.4
release=6

source="https://github.com/bkabrda/flexmock/archive/$version.tar.gz"
options="!checksum"

build() {
   cd flexmock-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
