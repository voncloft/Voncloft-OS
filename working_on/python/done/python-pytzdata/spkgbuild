# description : Official timezone database for Python.
# depends : python

name=python-pytzdata
version=2020.1
  local python_version=$python -c import sys; print..joinmapstr, sys.version_info[:2]
release=3

source="https://files.pythonhosted.org/packages/source/${name::1}/${name#python-}/${name#python-}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
