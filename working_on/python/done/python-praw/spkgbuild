# description : Python Reddit API Wrapper that allows for simple access to reddits API
# depends : python-prawcore python-update-checker python-websocket-client

name=python-praw
version=7.1.3
release=1

source="https://files.pythonhosted.org/packages/source/${name::1}/${name#python-}/${name#python-}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
