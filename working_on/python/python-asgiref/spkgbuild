# description : Reference ASGI adapters and channel layers
# depends : python

url=http://github.com/django/asgiref
name=python-asgiref
version=3.3.1
release=1

source="https://pypi.io/packages/source/a/asgiref/asgiref-${version}.tar.gz"
options="!checksum"

build() {
   cd asgiref-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
