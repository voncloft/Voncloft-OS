# description : Full featured consistent hashing python library compatible with ketama
# depends : python

url=https://github.com/ultrabug/uhashring
name=python-uhashring
version=1.2
release=3

source="https://github.com/ultrabug/uhashring/archive/$version.tar.gz"

build() {
   cd uhashring-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
