# description : Making it easy to write async iterators
# depends : python

_name=async_generator
url=https://github.com/python-trio/async_generator
name=python-${_name}
version=1.10
release=5

source="https://github.com/python-trio/async_generator/archive/v${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
