# description : Python Zabbix API
# depends : python

_name=zabbix-api
url=https://pypi.org/project/zabbix-api
_name=zabbix-api
name=python-zabbix-api
version=0.5.4
release=2

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
