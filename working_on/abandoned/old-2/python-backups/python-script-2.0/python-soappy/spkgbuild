# description : A module which implements a high-level interface to SOAP functions
  # depends : python2-wstools python2-defusedxml

url=http://pywebsvcs.sourceforge.net/
name=python2-soappy
version=0.12.22
release=3

source="https://pypi.python.org/packages/source/S/SOAPpy/SOAPpy-${version}.zip

build() {
   cd SOAPpy-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
