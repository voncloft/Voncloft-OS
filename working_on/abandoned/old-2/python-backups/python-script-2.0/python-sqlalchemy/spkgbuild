name=python-sqlalchemy python2-sqlalchemy
version=1.3.23
release=1

source="https://pypi.io/packages/source/S/SQLAlchemy/SQLAlchemy-$version.tar.gz"{,.asc}

build() {
   cd SQLAlchemy-$version
   cd SQLAlchemy2-$version
   cd SQLAlchemy2-$version  
   cd SQLAlchemy-$version
   cd SQLAlchemy2-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
