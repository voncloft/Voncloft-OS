# description : Various parsers for ECMA standards
# depends : python-ply=3.11

_name=calmjs.parse
url=https://github.com/calmjs/calmjs.parse
_name=calmjs.parse
name=python-calmjs.parse
version=1.2.5
release=5

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.zip
options="checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
