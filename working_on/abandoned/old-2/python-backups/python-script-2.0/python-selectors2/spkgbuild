url=https://github.com/SethMichaelLarson/selectors2
name=python2-selectors2 python-selectors2
version=2.0.2
release=1

source="https://github.com/SethMichaelLarson/selectors2/archive/$version.tar.gz"

build() {
   cd selectors2-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
