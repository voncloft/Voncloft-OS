source="https://pypi.io/packages/source/i/idna/idna-$version.tar.gz"

build() {
    cd idna-$version
    cd idna-$version-py2
    cd idna-$version
    cd idna-$version-py2
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
