# description : Library for setting up self-contained Kerberos 5 environments
# depends : python krb5

url=https://github.com/pythongssapi/k5test
name=python-k5test
version=0.9.2
release=1

source="https://github.com/pythongssapi/k5test/archive/v${version}/${name}-${version}.tar.gz"
options="checksum"

build() {
   cd k5test-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
