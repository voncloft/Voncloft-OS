# description : A dot-accessible dictionary a la JavaScript objects
# depends : python-six

url=https://github.com/Infinidat/munch
name=python-munch
version=2.5.0
release=4

source="https://pypi.io/packages/source/m/munch/munch-${version}.tar.gz"
options="checksum"

build() {
   cd $srcdir/munch-$version
   cd munch-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
