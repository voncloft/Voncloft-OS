# description : Get the public suffix of a domain name using the Public Suffix List
# depends : python

_name=publicsuffix
url=https://publicsuffix.org/
_name=publicsuffix
name=python-publicsuffix
version=1.1.1
release=2

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
