# description : Backport of shutil.which
# depends : python

url=https://github.com/mbr/shutilwhich
name=python-shutilwhich
version=1.1.0
release=6

source="https://github.com/mbr/shutilwhich/archive/${version}.tar.gz"
options="checksum"

build() {
   cd $srcdir/shutilwhich-$version
   cd shutilwhich-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
