# description : A memory-efficient packed representation for bit arrays in pure Python
# depends : python

url=https://pypi.python.org/pypi/BitVector
name=python-bitvector
version=3.4.9
release=4

source="https://pypi.io/packages/source/B/BitVector/BitVector-$version.tar.gz"
options="checksum"

build() {
   cd BitVector-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
