# description : Python module for using MongoDB
# depends : python

url=https://pypi.python.org/pypi/pymongo/
name=python-pymongo
version=3.11.3
release=1

source="https://pypi.io/packages/source/p/pymongo/pymongo-$version.tar.gz"
options="checksum"

build() {
   cd $srcdir/pymongo-$version
   cd pymongo-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
