# description : Allows you to run a test with multiple data sets
  # depends : python-six
  # depends : python2-six

url=https://github.com/box/genty
name=python-genty python2-genty
version=1.3.2
release=7

source="git+https://github.com/box/genty.git#tag=v$version

build() {
   cd $srcdir/genty
   cd $srcdir/genty-py2
   cd genty
   cd genty-py2
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
