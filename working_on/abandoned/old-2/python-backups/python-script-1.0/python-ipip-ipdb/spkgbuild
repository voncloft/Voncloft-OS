# description : IPIP.net officially supported IP database ipdb format parsing library
# depends : python

url=https://github.com/ipipdotnet/ipdb-python
name=python-ipip-ipdb
version=1.5.0
release=1

source="https://pypi.io/packages/source/i/ipip-ipdb/ipip-ipdb-$version.tar.gz"
options="checksum"

build() {
   cd ipip-ipdb-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
