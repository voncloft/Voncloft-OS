# description : Bindings for Chromaprint acoustic fingerprinting and the Acoustid API
# depends : 

url=https://github.com/beetbox/pyacoustid
name=python-pyacoustid
version=1.2.0
release=2

source="git+https://github.com/beetbox/pyacoustid.git#tag=dd69cfd1b68ac15bf84597b6ab5ab0aae3226a83
options="checksum"

build() {
   cd pyacoustid
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
