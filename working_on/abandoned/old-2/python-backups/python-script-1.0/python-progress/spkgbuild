# description : Easy to use progress bars for Python
    # depends : python
    # depends : python2

_name=progress
url=https://github.com/verigak/${_name}
_name=progress
name=python-progress python2-progress
version=1.5
release=5

source="${url}/archive/${version}.tar.gz"

build() {
     cd ${srcdir}/${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
