# description : Low-level components of distutils2/packaging
    # depends : python
    # depends : python2

_name=distlib
url=https://bitbucket.org/pypa/${_name}
_name=distlib
name=python-distlib python2-distlib
version=0.3.1
release=2

source="https://files.pythonhosted.org/packages/source/${_name:0:1}/${_name}/${_name}-${version}.zip

build() {
     cd ${srcdir}/${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
