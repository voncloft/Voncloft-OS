name=python-pywinrm
version=0.4.1
release=4
url="https://pypi.python.org/pypi/pywinrm"
opt# depends : python-requests-kerberos: for kerberos authentication
            python-requests-credssp: for CredSSP authenication
source="https://files.pythonhosted.org/packages/source/p/pywinrm/pywinrm-${version}.tar.gz"
sha512sums=0eaeb190d37f055c10d1bd8ae1414da4a7396351fd6286b1e5ffc22b8f8ed589c03e2cbf1c287e5ea8fc00c08012a206657cecb77ec69cd373ff462c3b18e3d7

options="!checksum"

build() {
     cd pywinrm-${version}
    python setup.py build
  python setup.py install--root=$PKG / --optimize=1
}

