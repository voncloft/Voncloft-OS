# description : Sphinx extension which outputs QtHelp document

_name=sphinxcontrib-qthelp
name=python-sphinxcontrib-qthelp
version=1.0.3
release=3
url=https://github.com/sphinx-doc/sphinxcontrib-qthelp
make# depends : python-setuptools
check# depends : python-pytest python-sphinx
source="https://files.pythonhosted.org/packages/source/${_name::1}/$_name/$_name-$version.tar.gz"
sha256sums=4c33767ee058b70dba89a6fc5c1892c0d57a54be67ddd3e7875a18d14cba5a72

build()-delete {
   cd $_name-$version
  python setup.py build
}

check {
   cd $_name-$version
  pytest
}

options="!checksum"

build() {
   cd $_name-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1 
}

