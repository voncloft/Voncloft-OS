# description : "Python module that makes working with XML feel like working with JSON"
# depends : python

name=python-xmltodict
version=0.12.0
release=6
url="https://github.com/martinblech/xmltodict"
make# depends : python-setuptools
source="https://files.pythonhosted.org/packages/source/x/xmltodict/xmltodict-$version.tar.gz"
md5sums=ddb2bd078cef4f7e3021a578034ad941

options="!checksum"

build() {
   cd "$srcdir/xmltodict-$version"
  python setup.py build
  python setup.py install--root=$PKG 
}
