# description : "Python-Markdown extension for easy video embedding"
# depends : python
# AUR Maintainer: Swift Geek

name=python-mdx-video
_pipname=mdx_video
version=0.1.8
release=6
url="https://pypi.python.org/pypi/mdx_video"
make# depends : python-setuptools
source="https://pypi.python.org/packages/source/m/${_pipname}/${_pipname}-${version}.tar.gz"
md5sums=a7c557bc24d0a3fb0a6e138b049d86e8

options="!checksum"

build() {
   cd "$srcdir/$_pipname-$version"
  python setup.py build
  python setup.py install --root="$PKG/" --optimize=1
}

