# description : "Functions to split or partition sequences"
# depends : python

name=python-split
version=0.4
release=6
url="https://bitbucket.org/astanin/python-split"
source="https://pypi.io/packages/source/s/split/split-$version.zip"
md5sums=16e72d45b66b0f8da2349be0f82d5216

options="!checksum"

build() {
   cd split-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1
}
