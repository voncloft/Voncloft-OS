# description : "Import exceptions from potentially bundled packages in requests."
# depends : python-requests

name=python-requestsexceptions
version=1.4.0
release=7
url="https://github.com/openstack-infra/requestsexceptions"
make# depends : python-pbr
source="$name-$version.tar.gz::https://github.com/openstack-infra/requestsexceptions/archive/$version.tar.gz"
sha512sums=fb7cf432584963eee659b7294a11b4d0a1d1dd850bfccdac7a1d7014b6e97a5330553436df58c034088c8a2096b14bfe40ded7e128a5d8741114265c935cd02f

export PBR_VERSION=$version

build()-delete {
   cd requestsexceptions-$version
  python setup.py build
}

options="!checksum"

build() {
   cd requestsexceptions-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1
}

