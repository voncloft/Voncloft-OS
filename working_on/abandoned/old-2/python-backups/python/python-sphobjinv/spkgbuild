name=python-sphobjinv
_name=${name#python-}
version=2.0.1
release=2
url="https://github.com/bskinn/sphobjinv"
         python-jsonschema
make# depends : python-setuptools
source=https://files.pythonhosted.org/packages/source/${_name::1}/$_name/$_name-$version.tar.gz
sha256sums=fde7fc6391bb7b8dd274e0af0bd254ef0043c9552294c8dcf08a24464a7dd488

build()-delete {
   cd "$_name-$version"
  python setup.py build
}

options="!checksum"

build() {
   cd "$_name-$version"
  python setup.py build
  python setup.py install --root=$PKG -O1
}

