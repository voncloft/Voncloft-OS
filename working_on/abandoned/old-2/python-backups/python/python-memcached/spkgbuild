# description : Python interface to memcached
# depends : python-six

name=python-memcached
version=1.59
release=7
url=https://github.com/linsomniac/python-memcached
make# depends : git python-setuptools
source="git://github.com/linsomniac/python-memcached#tag=$version"
md5sums=SKIP

options="!checksum"

build() {
   cd python-memcached

  python3 setup.py install --root=$PKG --optimize=1
}

