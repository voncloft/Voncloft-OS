# description : The little ASGI framework that shines
# depends : python

_name=starlette
url=https://github.com/encode/starlette
_name=starlette
name=python-$_name
version=0.13.8
release=3

source="$url/archive/$version.tar.gz"
options="checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
