# description : Data readers extracted from the pandas codebase
# depends : python-pandas python-lxml python-requests

url=https://github.com/pydata/pandas-datareader
name=python-pandas-datareader
version=0.9.0
release=2

source="https://github.com/pydata/pandas-datareader/archive/v${version}.tar.gz"
options="checksum"

build() {
   cd pandas-datareader-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
