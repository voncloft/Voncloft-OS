# description : A CSS Cascading Style Sheets library for Python
# depends : python

_name=css-parser
url=https://github.com/ebook-utils/${_name}
_name=css-parser
name=python-css-parser
version=1.0.6
release=3

source="https://files.pythonhosted.org/packages/source/${_name:0:1}/${_name}/${_name}-${version}.tar.gz"{,.asc}
options="checksum"

build() {
     cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
