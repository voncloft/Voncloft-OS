# description : Pythonic interface to the linux prctl syscall
# depends : python libcap

url=https://github.com/seveas/python-prctl
name=python-prctl
version=1.8
release=1

source="https://github.com/seveas/python-prctl/archive/v$version.tar.gz"
options="checksum"

build() {
   cd python-prctl-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
