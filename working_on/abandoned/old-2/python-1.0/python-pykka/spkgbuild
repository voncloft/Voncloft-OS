# description : Easy to use concurrency abstractions for Python using the actor model
# depends : python

url=http://pykka.readthedocs.org/
name=python-pykka
version=2.0.3
release=1

source="https://pypi.python.org/packages/source/P/Pykka/Pykka-$version.tar.gz"
options="checksum"

build() {
   cd Pykka-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
