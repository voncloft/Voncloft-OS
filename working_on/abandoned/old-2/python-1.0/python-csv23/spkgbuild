# description : Python 3 unicode CSV compatibility layer
# depends : python

url=https://github.com/xflr6/csv23
name=python-csv23
version=0.3.2
release=4

source="https://github.com/xflr6/csv23/archive/v$version.tar.gz"
options="checksum"

build() {
   cd csv23-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
