# description : List-like type with better asymptotic performance
# depends : python

_name=blist
url=http://stutzbachenterprises.com/blist
name=python-blist
_name=blist
version=1.3.6
release=9

source="https://github.com/DanielStutzbach/blist/archive/v${version}.tar.gz"
options="checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
