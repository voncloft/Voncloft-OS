# description : Emoji for Python
# depends : python

url=https://github.com/carpedm20/emoji
name=python-emoji
version=0.6.0
release=1

source="https://pypi.io/packages/source/e/emoji/emoji-$version.tar.gz"
options="checksum"

build() {
   cd emoji-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
