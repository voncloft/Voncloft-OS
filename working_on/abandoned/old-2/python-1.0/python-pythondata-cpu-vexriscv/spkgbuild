# description : Python module containing verilog files for vexriscv cpu for use with LiteX
# depends : python

_name=pythondata-cpu-vexriscv
_pyname=${_name//-/_}
url=https://github.com/litex-hub/$_name
_name=pythondata-cpu-vexriscv
_pyname=${_name//-/_}
name=python-$_name
version=2020.08
release=1

source="$url/archive/$version.tar.gz"
options=!strip
options="checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
