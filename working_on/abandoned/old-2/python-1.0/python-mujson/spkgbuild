# description : Use the fastest JSON functions available at import time
# depends : python

_name=mujson
url=https://github.com/mattgiles/mujson
_name=mujson
name=python-mujson
version=1.4
release=3

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
