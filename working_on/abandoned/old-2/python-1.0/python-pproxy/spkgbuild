# description : Versatile TCP/UDP asynchronous tunnel proxy implemented in Python 3 asyncio
# depends : python

url=https://github.com/qwj/python-proxy
name=python-pproxy
version=2.5.6
release=1

source="https://files.pythonhosted.org/packages/source/p/pproxy/pproxy-${version}.tar.gz"
options="checksum"

build() {
   cd pproxy-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
