# depends : perl perl-data-section perl-text-template

_ddir=Software-License-$version
version=0.103014
release=3

source="https://search.cpan.org/CPAN/authors/id/L/LE/LEONT/Software-License-$version.tar.gz"
options="!checksum"

build() {
   cd $_ddir
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
