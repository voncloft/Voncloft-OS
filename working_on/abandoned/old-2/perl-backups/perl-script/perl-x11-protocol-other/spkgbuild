# description : Extra extransions for perl-x11-protocol
# depends : perl-x11-protocol

name=perl-x11-protocol-other
version=30
release=6

source="https://search.cpan.org/CPAN/authors/id/K/KR/KRYDE/X11-Protocol-Other-${version}.tar.gz"
options="!checksum"

build() {
   cd X11-Protocol-Other-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
