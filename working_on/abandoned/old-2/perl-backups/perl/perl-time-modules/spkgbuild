# description : Perl time related modules

name=perl-time-modules
version=2013.0912
release=6

source="https://search.cpan.org/CPAN/authors/id/M/MU/MUIR/modules/Time-modules-$version.tar.gz"
options="!checksum"

build() {
   cd Time-modules-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
