# description : Perl Module:  Easy manipulation of IPv4 and IPv6 addresses
# depends : perl

name=perl-net-ip
_realname=Net-IP
version=1.26
release=8

_realname=Net-IP
source="https://www.cpan.org/authors/id/M/MA/MANU/Net-IP-$version.tar.gz"
options="!checksum"

build() {
   cd Net-IP-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
