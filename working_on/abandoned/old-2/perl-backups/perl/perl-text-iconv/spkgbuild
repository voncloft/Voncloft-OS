# description : Interface to iconv codeset conversion
# depends : perl

name=perl-text-iconv
version=1.7
release=19

source="https://www.cpan.org/authors/id/M/MP/MPIOTR/Text-Iconv-$version.tar.gz"
options="!checksum" 

build() {
   cd Text-Iconv-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
