# description : Test Module for Alien::Base
# depends : dontpanic perl-alien-build

name=perl-acme-alien-dontpanic
version=2.2900
release=1
url=https://metacpan.org/release/Acme-Alien-DontPanic

source="https://cpan.metacpan.org/authors/id/P/PL/PLICEASE/Acme-Alien-DontPanic-$version.tarsource=.gz"
options="!checksum"

build() {
   cd Acme-Alien-DontPanic-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
