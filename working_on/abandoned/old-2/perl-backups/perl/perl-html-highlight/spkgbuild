# description : Perl/CPAN Module HTML::Highlight

name=perl-html-highlight
version=0.20
release=11

source="https://www.cpan.org/authors/id/T/TR/TRIPIE/HTML-Highlight-$version.tar.gz"
options="!checksum"

build() {
   cd HTML-Highlight-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
