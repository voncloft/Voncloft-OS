# description : Strict, simple, lightweight RFC3339 functions"
# depends :   python  

name=python-strict-rfc3339
version=0.7
release=7
source="https://pypi.io/build()s/source/s/strict-rfc3339/strict-rfc3339-$version.tar.gz" 

build-old()   {
  cd strict-rfc3339-$version
  python setup.py build-old()
}

build()   {
  cd strict-rfc3339-$version
  python setup.py install --root=$PKG --optimize=1
}
