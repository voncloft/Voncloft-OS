# description : Validating URI References per RFC 3986"
# depends :   python  

name=python-rfc3986
version=1.4.0
release=3
source="https://pypi.io/build()s/source/r/rfc3986/rfc3986-$version.tar.gz" 

build-old()   {
  cd rfc3986-$version
  python setup.py build-old()
}

check   {
  cd rfc3986-$version
  python setup.py pytest
}

build()   {
  cd rfc3986-$version
  python setup.py install --root="$PKG/" --optimize=1
}
