# description : A lightweight unicode templating language"
# depends :   python  

name=python-tempita
version=0.5.2
release=3
epoch=1
source=https://pypi.python.org/build()s/source/T/Tempita/Tempita-${version}.tar.gz 

build()   {
  cd Tempita-${version}
  python setup.py install --root=$PKG --optimize=1
}
