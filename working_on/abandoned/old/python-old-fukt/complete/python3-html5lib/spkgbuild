# description : python3 parser tokenizer for html
# depends : python3

name=python3-html5lib
version=1.1
release=5

source="https://github.com/html5lib/html5lib-python/archive/${version}.tar.gz"
options="!checksum"

build()
{
	cd html5lib-python-$version
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
