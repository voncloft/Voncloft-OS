# description : task scheduler for python 
# depends : python3 python3-six python3-pytz python3-tzlocal

name=python3-apscheduler
version=3.6.3
release=3

source="https://pypi.io/packages/source/a/python-apscheduler/python-apscheduler-$version.tar.gz"
options="!checksum"

build()
{
	cd apscheduler-3.6.3
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
