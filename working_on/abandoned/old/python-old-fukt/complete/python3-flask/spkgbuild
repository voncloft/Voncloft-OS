# description : Micro webdevelopment framework for Python
# depends : python3

name=python3-flask
version=1.1.2
release=4

source="https://github.com/pallets/flask/archive/$version.tar.gz""
options="!checksum"

build()
{
	cd flask-$version
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
