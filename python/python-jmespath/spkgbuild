# description : A query language for JSON
# depends : python

name=python-jmespath
version=0.10.0
release=3

source="https://github.com/jmespath/jmespath.py/archive/refs/tags/$version.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
