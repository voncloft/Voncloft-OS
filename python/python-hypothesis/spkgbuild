# description : Advanced Quickcheck style testing library for Python
# depends : python-attrs python-sortedcontainers

name=python-hypothesis
version=6.15.0
release=1

source="https://files.pythonhosted.org/packages/4a/57/842af597a966f65a0dc745d6ed48d449e092d68f09f33ca8f338ad023be0/hypothesis-6.15.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
