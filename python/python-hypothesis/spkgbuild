name=python-hypothesis
version=6.17.1
release=1

source="https://files.pythonhosted.org/packages/cb/07/bac2d38690c3bd2d4f6c76e8fadad5ba1a8b575f5c74423f2e6d8f9836ed/hypothesis-6.17.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
