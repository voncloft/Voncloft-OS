# description : Advanced Quickcheck style testing library for Python
# depends : python-attrs python-sortedcontainers

name=python-hypothesis
version=6.17.3
release=1

source="https://files.pythonhosted.org/packages/c4/bf/3621f25d4ac16162beeab67af0e9949f0312285e44a073afa6d6e607f3fa/hypothesis-6.17.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
