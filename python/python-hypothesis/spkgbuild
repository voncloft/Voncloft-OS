name=python-hypothesis
version=6.18.0
release=1

source="https://files.pythonhosted.org/packages/5a/21/318136a9e473ab1fceaa8c098d3f08df648a9502945976910d8c25256ce9/hypothesis-6.18.0.tar.gz"
