name=python-hypothesis
version=6.21.0
release=1

source="https://files.pythonhosted.org/packages/a7/e2/ec6094232a770d21d6d13fb32dfbb9e4170c467e7e24063c1744a5868adb/hypothesis-6.21.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
