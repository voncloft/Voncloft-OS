# description : Advanced Quickcheck style testing library for Python
# depends : python-attrs python-sortedcontainers

name=python-hypothesis
version=6.14.8
release=1

source="https://files.pythonhosted.org/packages/88/b9/254d56766b318958100fa853779acb5cb3838b2721ed708b9450be54df0b/hypothesis-6.14.8.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
