name=python-hypothesis
version=6.21.3
release=1

source="https://files.pythonhosted.org/packages/b7/86/925d55ce81d6a3f0577990f87af253beddc7551069a9e3deddab24a26847/hypothesis-6.21.3.tar.gz"
