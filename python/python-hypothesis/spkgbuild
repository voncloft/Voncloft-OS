# description : Advanced Quickcheck style testing library for Python
# depends : python-attrs python-sortedcontainers

name=python-hypothesis
version=6.17.0
release=1

source="https://files.pythonhosted.org/packages/4c/17/67c010694e5ab0813005b356fff533b023f54ef7bf39679ae387cbb108ab/hypothesis-6.17.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
