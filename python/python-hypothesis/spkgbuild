# description : Advanced Quickcheck style testing library for Python
# depends : python-attrs python-sortedcontainers

name=python-hypothesis
version=6.14.5
release=1

source="https://files.pythonhosted.org/packages/5f/df/0cc0297e6f350c91d1350710964b1c21cfabaa5d2e154b012a646b33c03e/hypothesis-6.14.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
