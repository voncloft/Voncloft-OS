# description : Advanced Quickcheck style testing library for Python
# depends : python-attrs python-sortedcontainers

name=python-hypothesis
version=6.20.1
release=1

source="https://files.pythonhosted.org/packages/b3/9b/c3de6c8d8942c7991be7821eecdce63e3d9c8ac8e0c362cf09b0914304f8/hypothesis-6.20.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
