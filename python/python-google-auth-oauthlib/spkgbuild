# description : oauthlib integration for Google auth.
# depends : python python-google-auth python-requests-oauthlib

name=python-google-auth-oauthlib
version=0.4.5
release=1

source="https://files.pythonhosted.org/packages/16/92/fd6a1d55cb152e3e9d5fa06eeb1c65ad91195a1a2fceccdef7e6fbc36a00/google-auth-oauthlib-0.4.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
