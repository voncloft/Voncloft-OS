# description : Python module containing verilog files for vexriscv cpu for use with LiteX
# depends : python

name=python-pythondata-cpu-vexriscv
version=2020.08
release=1

source="python-pythondata-cpu-vexriscv-2020.08.tar.gz::https://github.com/litex-hub/pythondata-cpu-vexriscv/archive/2020.08.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
