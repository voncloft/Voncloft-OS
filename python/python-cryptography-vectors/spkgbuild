# description : Test vectors for the cryptography package
# depends : python

name=python-cryptography-vectors
version=3.4.8
release=1

source="https://files.pythonhosted.org/packages/e9/d1/91af613e55eaaba41a2b7daefce86cbb47d2ded527996b4f05294dda313a/cryptography_vectors-3.4.8.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
