# description : A cross-platform clipboard module for Python
# depends : python

name=python-pyperclip
version=1.8.2
release=3

source="python-pyopencl-b3e935355afcfb5240bac5a99707ffecc35772a2.tar.gz::https://github.com/asweigart/pyperclip/archive/b3e935355afcfb5240bac5a99707ffecc35772a2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
