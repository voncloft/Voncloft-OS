# description : Library for setting up self-contained Kerberos 5 environments
# depends : python krb5

name=python-k5test
version=0.10.0
release=1

source="https://files.pythonhosted.org/packages/83/1e/c6b81b1743c5a5b9fbda41db419f10feb25641410b7eefbcf2c666dc131a/k5test-0.10.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
