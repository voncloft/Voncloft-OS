# description : Library for setting up self-contained Kerberos 5 environments
# depends : python krb5

name=python-k5test
version=0.10.0
release=1

source="https://github.com/pythongssapi/k5test/archive/v0.10.0/python-k5test-0.10.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
