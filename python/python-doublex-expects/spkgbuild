# description : Expects matchers for Doublex test doubles assertions
# depends : python-doublex python-expects

name=python-doublex-expects
version=0.7.1
release=5

source="python-doublex-expects-0.7.1.tar.gz::https://github.com/jaimegildesagredo/doublex-expects/archive/v0.7.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
