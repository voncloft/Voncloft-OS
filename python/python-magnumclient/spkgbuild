# description : Client library for Magnum API
# depends : python-pbr python-babel python-keystoneauth1 python-stevedore python-requests

name=python-magnumclient
version=3.5.0
release=1

source="https://github.com/openstack/python-magnumclient/archive/3.5.0/python-magnumclient-3.5.0.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
