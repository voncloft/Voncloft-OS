name=python-httpcore
version=0.13.7
release=3

source="python-httpcore-0.13.6.tar.gz::https://github.com/encode/httpcore/archive/0.13.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
