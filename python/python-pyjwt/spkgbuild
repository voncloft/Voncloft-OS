# description : JSON Web Token implementation in Python
# depends : python-setuptools

name=python-pyjwt
version=2.1.0
release=6

source="python-pyjwt-1.7.1.tar.gz::https://github.com/jpadilla/pyjwt/archive/1.7.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
