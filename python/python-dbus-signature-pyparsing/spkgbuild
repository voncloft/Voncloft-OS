# description : A Parser for a D-Bus Signature
# depends : python-pyparsing

name=python-dbus-signature-pyparsing
_srcname=dbus-signature-pyparsing
version=0.04
release=8

source="https://github.com/stratis-storage/dbus-signature-pyparsing/archive/v$version.tar.gz"
options="!checksum"

build() {
   cd ${_srcname}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
