name=python-dbus-signature-pyparsing
version=0.4
release=8

source="https://files.pythonhosted.org/packages/f0/f6/9373cbe38f2bb71e6bc3f24c246e1fbd3ebcb2cf16a7a5bd0c4b2a3e234f/dbus-signature-pyparsing-0.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
