source="https://files.pythonhosted.org/packages/8e/5e/dc01cdb583a73d255cc35e94e352a77022cb64a1566365e0e22a605326be/matrix-nio-0.18.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
