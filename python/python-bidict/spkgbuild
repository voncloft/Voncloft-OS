# description : Bidirectional mapping library for Python
# depends : python

name=python-bidict
version=0.21.3
release=2

source="https://files.pythonhosted.org/packages/3f/81/7221b28d692af5c5fc180c4850b8e4a48c7db92b3d529b430488f67db74f/bidict-0.21.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
