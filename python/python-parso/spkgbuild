# description : Python parser that supports error recovery and round-trip parsing for different Python versions
# depends : python

name=python-parso
version=0.8.2
release=3

source="https://files.pythonhosted.org/packages/5e/61/d119e2683138a934550e47fc8ec023eb7f11b194883e9085dca3af5d4951/parso-0.8.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
