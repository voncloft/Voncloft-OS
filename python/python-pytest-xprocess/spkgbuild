# description : Pytest plugin to manage external processes across test runs.
# depends : python-psutil python-pytest python-setuptools

name=python-pytest-xprocess
version=0.18.1
release=1

source="https://files.pythonhosted.org/packages/c3/9a/8681347cfbbc073a25ee4e62c2c82f4bbcbbd2976a47da7ab36b8a768d23/pytest-xprocess-0.18.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
