# description : Add Python and JavaScript style comments in your JSON files
# depends : python-lark-parser

name=python-commentjson
version=0.9.0
release=3

source="python-commentjson-0.9.0.tar.gz::https://github.com/vaidik/commentjson/archive/v0.9.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
