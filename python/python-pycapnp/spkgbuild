# description : A cython wrapping of the C++ Capn Proto library
# depends : capnproto python

name=python-pycapnp
version=1.1.0
release=1

source="https://github.com/capnproto/pycapnp/archive/v1.1.0/python-pycapnp-1.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
