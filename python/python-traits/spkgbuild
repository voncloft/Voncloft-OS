# description : Explicitly typed attributes for Python
# depends : python

name=python-traits
version=6.2.0
release=1

source="https://github.com/enthought/traits/archive/refs/tags/o$version.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
