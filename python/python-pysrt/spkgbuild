# description : Python parser for SubRip srt files
# depends : python-chardet

name=python-pysrt
version=1.1.2
release=2

source="python-pysrt-1.1.2.tar.gz::https://github.com/byroot/pysrt/archive/v1.1.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
