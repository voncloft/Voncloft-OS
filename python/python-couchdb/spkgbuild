# description : Python library for working with CouchDB
# depends : python-setuptools

name=python-couchdb
version=1.2
release=4

source="python-couchdb-1.2.tar.gz::https://github.com/djc/couchdb-python/archive/1.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
