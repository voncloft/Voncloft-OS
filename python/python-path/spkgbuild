# description : A module wrapper for os.path
# depends : python

name=python-path
version=16.2.0
release=1

source="https://files.pythonhosted.org/packages/62/2d/652177467df19e8602cad09c080098dd2e16c5fdc1f2c8f366dc38c38790/path-16.2.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
