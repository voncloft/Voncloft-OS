# description : A module wrapper for os.path
# depends : python

name=python-path
version=16.1.0
release=1

source="https://files.pythonhosted.org/packages/49/22/f217df97c2975aa4c2eb6f875931075c4c904e93edcb500d9d3e6ae43e52/path-16.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
