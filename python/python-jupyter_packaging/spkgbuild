# description : Jupyter Packaging Utilities
# depends : python-packaging

name=python-jupyter_packaging
version=0.7.11
release=1

source="https://files.pythonhosted.org/packages/ed/c7/ade159e5481b3140e3a98c78676747915d3b166b35f78e32f9c1ef0568bd/jupyter_packaging-0.10.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
