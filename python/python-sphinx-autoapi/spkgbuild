name=python-sphinx-autoapi
version=1.8.4
release=1

source="https://files.pythonhosted.org/packages/c6/bf/6b02d26ce72e79be8dcf4ab57a9cab05405589825be775a47783dfff07a6/sphinx-autoapi-1.8.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
