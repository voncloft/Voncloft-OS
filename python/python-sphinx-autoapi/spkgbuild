name=python-sphinx-autoapi
version=1.8.2
release=1

source="https://files.pythonhosted.org/packages/b7/5c/80321c378bcb7afa8305a88048a68d2fa32946d72098f38eaa8910059552/sphinx-autoapi-1.8.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
