# description : Simple, fast, extensible JSON encoder/decoder for Python
  # depends : python

name=python-simplejson
version=3.17.5
release=3

source="https://files.pythonhosted.org/packages/01/52/41c71718f941c9a5abebfaa24e3c14e3c0229327b7ebd21348989845ed8f/simplejson-3.17.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
