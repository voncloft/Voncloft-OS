# description : Simple, fast, extensible JSON encoder/decoder for Python
  # depends : python

name=python-simplejson
version=3.17.4
release=3

source="https://files.pythonhosted.org/packages/0b/d1/d1cbd62baf1f1a635a4cd45c8349d235f1d87129e5f9bd9957a2308c424d/simplejson-3.17.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
