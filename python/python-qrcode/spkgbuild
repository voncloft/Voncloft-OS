# description : Python library to generate QR codes
# depends : python-pillow python-six python-setuptools

name=python-qrcode
version=7.3
release=6

source="https://files.pythonhosted.org/packages/12/a5/657b4e6323ddb0f3e25ae9295ac424cfa164b68d9bdd48f86344bea8348e/qrcode-7.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
