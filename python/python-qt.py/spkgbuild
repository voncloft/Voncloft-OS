# description : Python compatibility wrapper around all Qt bindings
# depends : python qt5-python-bindings

name=python-qt.py
version=1.3.4
release=5

source="https://files.pythonhosted.org/packages/0b/35/10e1c8efff66050b724964801a1caca518230bea6d587e62e8ba3b23366d/Qt.py-1.3.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
