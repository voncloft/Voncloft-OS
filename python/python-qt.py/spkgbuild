# description : Python compatibility wrapper around all Qt bindings
# depends : python qt5-python-bindings

name=python-qt.py
version=1.3.6
release=5

source="https://files.pythonhosted.org/packages/20/9d/49353c7cd4812dc8578e954deee7ac0aca4bbbcf434c310c2dbd6772e3b2/Qt.py-1.3.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
