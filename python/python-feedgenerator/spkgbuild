# description : Standalone version of django.utils.feedgenerator python3.
# depends : python python-pytz python-six

name=python-feedgenerator
version=1.9.2
release=3

source="https://files.pythonhosted.org/packages/9b/80/15fd050f2dcb8e81732959cfb7c7192d5181f421c6dc8ef213a9d5da86d1/feedgenerator-1.9.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
