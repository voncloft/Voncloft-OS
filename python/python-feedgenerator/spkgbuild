# description : Standalone version of django.utils.feedgenerator python3.
# depends : python python-pytz python-six

_name=feedgenerator
url=https://pypi.org/project/feedgenerator/
name=python-${_name}
version=1.9.1
release=3

source="https://pypi.python.org/packages/source/${_name:0:1}/$_name/$_name-$version.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
