# description : A general purpose Python retyring library
  # depends : python-six

name=python-retrying
version=1.3.3
release=9

source="https://github.com/rholder/retrying/archive/v1.3.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
