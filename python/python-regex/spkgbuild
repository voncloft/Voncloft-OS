# description : Alternative python regular expression module.
# depends : python

name=python-regex
version=2021.8.28
release=2

source="https://files.pythonhosted.org/packages/source/r/regex/regex-2021.8.28.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
