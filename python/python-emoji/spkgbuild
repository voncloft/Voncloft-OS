# description : Emoji for Python
# depends : python

name=python-emoji
version=1.4.2
release=1

source="https://files.pythonhosted.org/packages/75/eb/e09f7986f83c4bd711f0c81482f12a48742b9a7473a7ac0a97b8bcc9dd4b/emoji-1.4.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
