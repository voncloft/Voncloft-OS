# description : Library for developers to extract data from Microsoft Excel tm spreadsheet files
# depends : python

name=python-xlrd
version=2.0.1
release=1

source="https://files.pythonhosted.org/packages/a6/b3/19a2540d21dea5f908304375bd43f5ed7a4c28a370dc9122c565423e6b44/xlrd-2.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
