# description : Library for developers to extract data from Microsoft Excel tm spreadsheet files
# depends : python

name=python-xlrd
version=2.0.1
release=1

source="python-xlrd-2.0.1.tar.gz::https://github.com/python-excel/xlrd/archive/2.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
