# description : Pure-Python PostgreSQL database driver, DB-API compatible
# depends : python python-scramp

name=python-pg8000
version=1.21.1
release=3

source="https://files.pythonhosted.org/packages/33/10/c3956cf016ca685d0f6db2439c4486d16a348624b3b0c794542cecb25bd7/pg8000-1.21.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
