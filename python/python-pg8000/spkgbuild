# description : Pure-Python PostgreSQL database driver, DB-API compatible
# depends : python python-scramp

name=python-pg8000
version=1.21.0
release=3

source="https://files.pythonhosted.org/packages/5a/9d/6efaa60cbbcaf0ddd7f3ea139e098550b0ce7ae0f91f60f02bd59b8f63fb/pg8000-1.21.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
