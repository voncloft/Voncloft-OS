# description : An SDK for build()ing applications to work with OpenStack
# depends : python-six python-pbr python-yaml python-jsonpatch python-appdirs

name=python-openstacksdk
version=0.59.0
release=1

source="https://files.pythonhosted.org/packages/c3/23/30cd6b7e710980234be264270a60b0b26ed9eca88119d23cd50de404dd1d/openstacksdk-0.59.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
