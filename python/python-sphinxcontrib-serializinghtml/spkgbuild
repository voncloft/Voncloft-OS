# description : Sphinx extension which outputs serialized HTML files json and pickle

name=python-sphinxcontrib-serializinghtml
version=1.1.5
release=3

source="https://files.pythonhosted.org/packages/b5/72/835d6fadb9e5d02304cf39b18f93d227cd93abd3c41ebf58e6853eeb1455/sphinxcontrib-serializinghtml-1.1.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
