# description : Python Build Reasonableness
  # depends : python-setuptools

name=python-pbr
version=5.6.0
release=3

source="python-pbr-5.6.0.tar.gz::https://github.com/openstack-dev/pbr/archive/5.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
