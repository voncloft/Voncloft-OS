# description : A modern parsing library
# depends : python

name=python-lark-parser
version=0.12.0
release=1

source="python-lark-parser-0.11.3.tar.gz::https://github.com/lark-parser/lark/archive/0.11.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
