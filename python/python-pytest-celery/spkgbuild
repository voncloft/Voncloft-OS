# description : A shim pytest plugin to enable celery.contrib.pytest
# depends : python-celery

name=python-pytest-celery
version=0.0.0a1
release=4

source="https://files.pythonhosted.org/packages/46/e5/f6e651d93f0480f298fd1157108132718d75506008034bd932f7ad5cec96/pytest-celery-0.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
