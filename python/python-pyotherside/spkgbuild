# description : Asynchronous Python 3 Bindings for Qt5
# depends : python qt5-base qt5-declarative qt5-svg

name=python-pyotherside
version=1.5.9
release=3

source="python-pyotherside-1.5.9.tar.gz::https://github.com/thp/pyotherside/archive/1.5.9.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
