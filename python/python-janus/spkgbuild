# description : Thread-safe asyncio-aware queue for Python
# depends : python

name=python-janus
version=0.7.0
release=3

source="https://github.com/aio-libs/janus/archive/refs/tags/v$version.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
