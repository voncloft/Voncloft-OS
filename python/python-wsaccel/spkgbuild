# description : Accelerator for ws4py and AutobahnPython
# depends : python

name=python-wsaccel
version=0.6.3
release=2

source="https://github.com/methane/wsaccel/archive/v0.6.3/python-wsaccel-0.6.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
