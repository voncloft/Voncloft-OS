# description : Set of tools to provide lightweight pipelining in Python
# depends : python

name=python-joblib
version=1.0.1
release=1

source="python-joblib-1.0.1.tar.gz::https://github.com/joblib/joblib/archive/1.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
