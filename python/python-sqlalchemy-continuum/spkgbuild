# description : Versioning and auditing extension for SQLAlchemy
# depends : python python-sqlalchemy python-sqlalchemy-utils

name=python-sqlalchemy-continuum
version=1.3.11
release=3

source="python-sqlalchemy-continuum-1.3.11.tar.gz::https://github.com/kvesteri/sqlalchemy-continuum/archive/1.3.11.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
