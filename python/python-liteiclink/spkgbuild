# description : Small footprint and configurable Inter-Chip communication cores
# depends : python-migen python-litex

name=python-liteiclink
version=2020.12
release=1

source="python-liteiclink-2020.12.tar.gz::https://github.com/enjoy-digital/liteiclink/archive/2020.12.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
