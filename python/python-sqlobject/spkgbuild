# description : SQLObject, an object-relational mapper for Python
# depends : python-pydispatcher

name=python-sqlobject
version=3.9.1
release=1

source="python-sqlobject-3.9.1.tar.gz::https://github.com/sqlobject/sqlobject/archive/3.9.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
