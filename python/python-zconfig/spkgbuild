# description : Structured Configuration Library

name=python-zconfig
version=3.6.0
release=5

source="https://github.com/zopefoundation/ZConfig/archive/3.6.0/python-zconfig-3.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
