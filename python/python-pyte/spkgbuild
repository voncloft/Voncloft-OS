# description : Simple VTXXX-compatible terminal emulator
# depends : python-wcwidth

name=python-pyte
version=0.8.0
release=6

source="python-pyte-0.8.0.tar.gz::https://github.com/selectel/pyte/archive/0.8.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
