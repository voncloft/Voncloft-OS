# description : Simple VTXXX-compatible terminal emulator
# depends : python-wcwidth

name=python-pyte
version=0.8.0
release=6

source="https://files.pythonhosted.org/packages/66/37/6fed89b484c8012a0343117f085c92df8447a18af4966d25599861cd5aa0/pyte-0.8.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
