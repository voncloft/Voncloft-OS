name=python-spyder-kernels
version=2.1.0
release=1

source="https://files.pythonhosted.org/packages/d5/37/167a792c02866bd582434c0c0ff74f7350af6cffd8b7abac1f026757cbff/spyder-kernels-2.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
