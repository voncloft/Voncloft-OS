name=python-spyder-kernels
version=2.1.1
release=1

source="https://files.pythonhosted.org/packages/91/b4/bd2ec214ebf4b980fc19e8cd1207d7439b9b80d3532672b4011eed32d930/spyder-kernels-2.1.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
