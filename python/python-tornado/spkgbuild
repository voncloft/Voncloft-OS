# description : open source version of the scalable, non-blocking web server and tools
# depends : python

name=python-tornado
version=6.1.0
release=3

source="python-tornado-6.1.0.tar.gz::https://github.com/tornadoweb/tornado/archive/v6.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
