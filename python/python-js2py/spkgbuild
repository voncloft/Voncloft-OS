# description : JavaScript to Python Translator & JavaScript interpreter written in 100 percent pure Python
# depends : python-tzlocal python-six python-pyjsparser

name=python-js2py
version=0.71
release=3

source="python-js2py-b3e935355afcfb5240bac5a99707ffecc35772a2.tar.gz::https://github.com/PiotrDabkowski/Js2Py/archive/b3e935355afcfb5240bac5a99707ffecc35772a2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
