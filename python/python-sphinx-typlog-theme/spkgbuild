# description : A sphinx theme by Typlog

name=python-sphinx-typlog-theme
version=0.8.0
release=3

source="python-sphinx-typlog-theme-0.8.0.tar.gz::https://github.com/typlog/sphinx-typlog-theme/archive/b3e935355afcfb5240bac5a99707ffecc35772a2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
