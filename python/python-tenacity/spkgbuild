# description : Retry code until it succeeeds
# depends : python-six

name=python-tenacity
version=8.0.1
release=1

source="https://github.com/jd/tenacity/archive/8.0.1/python-tenacity-8.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
