# description : Common interface for data container classes
# depends : python

name=python-itemadapter
version=0.4.0
release=1

source="https://github.com/scrapy/itemadapter/archive/v0.4.0/python-itemadapter-0.4.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
