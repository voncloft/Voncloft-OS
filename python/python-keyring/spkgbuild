# description : Store and access your passwords safely
# depends : python-entrypoints python-secretstorage

name=python-keyring
version=23.2.0
release=1

source="https://files.pythonhosted.org/packages/0a/13/c08a8670b34fe5bf37e10726999077050105f97edbe4edd3038c0357549d/keyring-23.2.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
