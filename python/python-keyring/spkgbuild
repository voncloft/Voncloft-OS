# description : Store and access your passwords safely
# depends : python-entrypoints python-secretstorage

name=python-keyring
version=23.1.0
release=1

source="https://files.pythonhosted.org/packages/cc/ff/89ec0e9c71181f631b7892a290b0a159875354b207b33e5012d3bcdf71b1/keyring-23.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
