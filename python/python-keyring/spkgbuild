# description : Store and access your passwords safely
# depends : python-entrypoints python-secretstorage

name=python-keyring
version=23.2.1
release=1

source="https://files.pythonhosted.org/packages/cc/24/c5402ba0c6380cc058980e2b73f0597ab6875692f185054a94244b7161ab/keyring-23.2.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
