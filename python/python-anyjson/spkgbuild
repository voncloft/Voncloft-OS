# description : Wraps the best available JSON implementation available in a common interface
# depends : python

_pypiname=anyjson
name=python-$_pypiname
version=0.3.3
release=14

source="https://files.pythonhosted.org/packages/source/a/$_pypiname/$_pypiname-${version}.tar.gz"
options="!checksum"

build() {
  cd "$_pypiname-${version}"
  python setup.py build
  python setup.py install --root=$PKG --optimize=1
}
