# description : Easy access to shell commands from python
# depends : python

name=python-whelk
version=3.0
release=2

source="python-whelk-3.0.tar.gz::https://github.com/seveas/whelk/archive/3.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
