# description : SimpleHTTPServer with support for Range requests
# depends : python

name=python-rangehttpserver
version=1.2.0
release=6

source="python-rangehttpserver-1.2.0.tar.gz::https://github.com/danvk/RangeHTTPServer//archive/1.2.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
