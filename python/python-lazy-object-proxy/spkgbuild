# description : A fast and thorough lazy object proxy
# depends : python

name=python-lazy-object-proxy
version=1.6.0
release=2

source="python-lazy-object-proxy-1.6.0.tar.gz::https://github.com/ionelmc/python-lazy-object-proxy/archive/v1.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
