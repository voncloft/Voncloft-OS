# description : A Python Object-Document-Mapper for working with MongoDB
# depends : python-pymongo python-six

name=python-mongoengine
version=0.23.1
release=1

source="python-mongoengine-0.23.1.tar.gz::https://github.com/MongoEngine/mongoengine/archive/v0.23.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
