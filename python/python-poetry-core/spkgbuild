# description : Poetry PEP 517 Build Backend & Core Utilities
# depends : ${_deps[@]/#/python-}

name=python-poetry-core
version=1.1.0
release=2

source="https://files.pythonhosted.org/packages/05/a5/87320d85c550bfaed61bd9bdd6c8b7170147995a7513c9defd8df24982fd/poetry-core-1.0.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
