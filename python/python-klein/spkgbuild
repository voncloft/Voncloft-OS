# description : A web micro-framework built on werkzeug and twisted.web
# depends : python python-attrs python-hyperlink python-incremental python-six

name=python-klein
version=21.8.0
release=3

source="https://files.pythonhosted.org/packages/6c/e3/829c28e1cac2430777d47043b09bc18be25945adf1b6804278a25e7f7af9/klein-21.8.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
