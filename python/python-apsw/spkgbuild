# description : Python wrapper for SQLite
# depends : python sqlite

name=python-apsw
version=3.34.0
release=1
source="https://github.com/rogerbinns/apsw/archive/$version-r1.tar.gz"
options="!checksum"

build() {
  cd apsw-${version}-r1
  python setup.py build
  python setup.py install --root=$PKG --optimize=1 
}
