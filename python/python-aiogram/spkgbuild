# description : A pretty simple and fully asynchronous library for Telegram Bot API written with asyncio and aiohttp
# depends : python-aiohttp python-babel

name=python-aiogram
version=3.0.0
release=3

source="https://files.pythonhosted.org/packages/0c/63/28bd04c7b8ab4796d5c6da4e637cb6ae53b2afac84e421285b5571858a8d/aiogram-3.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
