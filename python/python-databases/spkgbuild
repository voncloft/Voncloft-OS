# description : Async database support for Python
# depends : python

name=python-databases
version=0.5.0
release=1

source="https://files.pythonhosted.org/packages/ef/7f/bb7bd3cc8800bbe11aecef1d67621316fe67ea12a2180422eaef68df5af4/databases-0.5.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
