# description : Async database support for Python
# depends : python

name=python-databases
version=0.5.2
release=1

source="https://files.pythonhosted.org/packages/f1/b3/aca6009596f42113bd5a43055a0bbad82c5f08967094f4b43d5f20300edc/databases-0.5.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
