# description : Async database support for Python
# depends : python

name=python-databases
version=0.5.1
release=1

source="https://files.pythonhosted.org/packages/a3/ac/2bd56de8003b3be921ccfe7918aec580129de3ad51ee95cbfe770a6479cc/databases-0.5.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
