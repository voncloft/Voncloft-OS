# description : A built-package format for Python
  # depends : python-packaging

name=python-wheel
version=0.37.0
release=1

source="python-wheel-0.37.0.tar.gz::https://github.com/pypa/wheel/archive/0.37.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
