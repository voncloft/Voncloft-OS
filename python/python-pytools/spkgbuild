# description : A collection of tools for Python
# depends : python-six python-decorator python-appdirs python-numpy

name=python-pytools
version=2021.2.8
release=1

source="https://files.pythonhosted.org/packages/d9/75/5aa0f6275da839b756e88107ea347d486877d69dd98840c824117d7d6c93/pytools-2021.2.8.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
