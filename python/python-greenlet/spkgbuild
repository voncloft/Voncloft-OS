# description : Lightweight in-process concurrent programming
# depends : python

name=python-greenlet
version=1.1.1
release=1

source="https://files.pythonhosted.org/packages/source/g/greenlet/greenlet-1.1.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
