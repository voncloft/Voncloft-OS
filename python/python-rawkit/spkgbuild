# description : ctypes based libraw bindings 
# depends : libraw python

name=python-rawkit
version=0.6.0
release=9

source="rawkit-0.6.0.tar.gz::https://github.com/photoshell/rawkit/archive/v0.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
