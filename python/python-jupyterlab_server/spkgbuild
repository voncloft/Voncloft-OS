# description : Launch an application built using JupyterLab
# depends : jupyter-server python-requests python-json5 python-babel

name=python-jupyterlab_server
version=2.2.0
release=1

source="https://files.pythonhosted.org/packages/0b/2e/c7935cf8bf13a5766e96277f7ccf051d1f1bf6d7aef7be87e2ed70c71a20/jupyterlab_server-2.6.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
