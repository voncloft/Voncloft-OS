# description : Launch an application built using JupyterLab
# depends : jupyter-server python-requests python-json5 python-babel

name=python-jupyterlab_server
version=2.2.0
release=1

source="https://files.pythonhosted.org/packages/2f/dd/2f87cebca3470c0106cca1dbfac2b47ed6b7c0f653aef89a7cdd8c30e839/jupyterlab_server-2.7.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
