# description : Python bindings for FUSE
# depends : fuse2 python

name=python-fuse
version=1.0.4
release=3

source="python-fuse-1.0.4.tar.gz::https://github.com/libfuse/python-fuse/archive/v1.0.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
