name=python-owslib
version=0.25.0
release=1

source="https://files.pythonhosted.org/packages/e4/47/f8e5b579da7a3a5b44e0a2efa952c1d66e9a13090e1b051fd2203d1f20e2/OWSLib-0.25.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
