# description : Python package for the creation, manipulation, and study of the structure, dynamics, and functions of complex networks.
# depends : python-decorator

name=python-networkx
version=2.6.3
release=3

source="https://files.pythonhosted.org/packages/97/ae/7497bc5e1c84af95e585e3f98585c9f06c627fac6340984c4243053e8f44/networkx-2.6.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
