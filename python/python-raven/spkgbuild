# description : Python client for Sentry
# depends : python

name=python-raven
version=6.10.0
release=7

source="python-pytorch-6.10.0.tar.gz::https://github.com/getsentry/raven-python/archive/6.10.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
