# description : Python object model built on JSON schema and JSON patch.
# depends : python-jsonschema python-jsonpatch python-six

name=python-warlock
version=1.3.3
release=5

source="python-warlock-1.3.3.tar.gz::https://github.com/bcwaldon/warlock/archive/1.3.3.tar.gz"
