# description : Python client for the kubernetes API
# depends : python python-certifi python-six python-dateutil

name=python-kubernetes
version=18.20.0
release=2

source="https://files.pythonhosted.org/packages/9c/f8/0eb10c6939b77788c10449d47d85a4740bb4a5608e1a504807fcdb5babd0/kubernetes-18.20.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
