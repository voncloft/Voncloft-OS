name=python-copr
version=1.112
release=1

source="https://files.pythonhosted.org/packages/1e/69/7ba303232d677b8b59a701ac9930f2715ead186da7ee2abc780419079231/copr-1.112.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
