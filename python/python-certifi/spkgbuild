# description : Python package for providing Mozillas CA Bundle
# depends : python

name=python-certifi
version=2021.5.30
release=3

source="https://pypi.io/packages/source/c/certifi/certifi-2020.12.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
