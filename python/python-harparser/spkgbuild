# description : Python HAR Parser Utility
# depends : python

url=https://github.com/JustusW/harparser
name=python-harparser
version=0.4
release=6

source="https://github.com/JustusW/harparser/archive/$version.tar.gz"
options="!checksum"

build() {
   cd harparser-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
