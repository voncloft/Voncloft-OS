# description : a fork of Python 2 and 3 ast modules with type comment support
# depends : python

name=python-typed-ast
version=1.4.3
release=1

source="https://files.pythonhosted.org/packages/6e/08/c04a49ee26a94c1ec211e7b1e5f2971d692e04818ea67ef70f1e879cf525/typed_ast-1.4.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
