# description : Nose plugin to ignore unicode literal prefixes in doctests
# depends : python-nose

name=python-doctest-ignore-unicode
version=0.1.2
release=3

source="https://github.com/gnublade/doctest-ignore-unicode/archive/b3e935355afcfb5240bac5a99707ffecc35772a2/python-doctest-ignore-unicode-b3e935355afcfb5240bac5a99707ffecc35772a2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
