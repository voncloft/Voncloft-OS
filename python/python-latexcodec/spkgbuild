# description : A lexer and codec to work with LaTeX code in Python
# depends : python-six

name=python-latexcodec
version=2.0.1
release=3

source="python-latexcodec-2.0.1.tar.gz::https://github.com/mcmtroffaes/latexcodec/archive/2.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
