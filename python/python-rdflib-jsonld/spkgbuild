# description : JSON-LD parser and serializer plugins for RDFLib
# depends : python-rdflib

name=python-rdflib-jsonld
version=0.6.0
release=3

source="https://files.pythonhosted.org/packages/64/3f/4616c62eba959507d2898a87940c0ff23ea5592d449031bec5227ada78fe/rdflib-jsonld-0.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
