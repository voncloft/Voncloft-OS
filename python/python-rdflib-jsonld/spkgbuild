# description : JSON-LD parser and serializer plugins for RDFLib
# depends : python-rdflib

name=python-rdflib-jsonld
version=0.6.1
release=3

source="https://files.pythonhosted.org/packages/5a/48/9eaecac5f5ba6b31dd932fbbe67206afcbd24a7a696c03c6c920ac7ddc39/rdflib-jsonld-0.6.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
