# description : Jupyter protocol implementation and client libraries
# depends : python-ipykernel python-traitlets python-pyzmq

name=python-jupyter_client
version=6.1.7
release=3

source="https://files.pythonhosted.org/packages/21/9e/56678f716bbc3af0ebcb9c61809cc71293f9288c5d0813e9667e76924230/jupyter_client-7.0.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
