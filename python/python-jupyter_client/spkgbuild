# description : Jupyter protocol implementation and client libraries
# depends : python-ipykernel python-traitlets python-pyzmq

name=python-jupyter_client
version=6.1.7
release=3

source="https://files.pythonhosted.org/packages/0e/9e/9356c9a71ab5a5355b609b02f4ff939d20d8c8a7fb5edde26bda3207a74c/jupyter_client-7.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
