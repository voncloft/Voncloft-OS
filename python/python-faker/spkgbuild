# description : Faker generates fake data for you.
# depends : python-setuptools python-dateutil python-text-unidecode

name=python-faker
version=8.10.3
release=1

source="https://files.pythonhosted.org/packages/82/5a/111f15e028066932702be98d2457e4aa09fbbd3835f7fe8338db906ca416/Faker-8.10.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
