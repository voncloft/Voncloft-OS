source="https://files.pythonhosted.org/packages/08/9a/a46dc1f0bb5d6a7efdf5f39cda1400af451cc5e8fa850b584b85d1bb3ada/Faker-8.11.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
