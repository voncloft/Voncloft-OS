source="https://files.pythonhosted.org/packages/c0/80/eaa2b420e761b93418be3d1de7cea72e9560a4c5806769916e7c8989ad21/Faker-8.10.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
