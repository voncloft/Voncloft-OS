# description : Faker generates fake data for you.
# depends : python-setuptools python-dateutil python-text-unidecode

name=python-faker
version=8.12.1
release=1

source="https://files.pythonhosted.org/packages/35/23/8f8361df9a1637d85235f3d62d5ab3462c1678cd6d0ccc8869595b728176/Faker-8.13.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
