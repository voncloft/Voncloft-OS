# description : Faker generates fake data for you.
# depends : python-setuptools python-dateutil python-text-unidecode

name=python-faker
version=8.12.0
release=1

source="https://files.pythonhosted.org/packages/78/3a/920c76381204aa39b4c515493908d795108f09e6d520ae37ff34ef31aa27/Faker-8.12.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
