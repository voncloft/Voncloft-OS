# description : Faker generates fake data for you.
# depends : python-setuptools python-dateutil python-text-unidecode

name=python-faker
version=8.12.1
release=1

source="https://files.pythonhosted.org/packages/ee/52/1ce107d0dce28f236ad70c359c0c5fc5c16d70dab6ea3f62e34b3c57e348/Faker-8.12.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
