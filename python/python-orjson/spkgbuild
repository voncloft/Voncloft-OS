# description : Fast, correct Python JSON library supporting dataclasses and datetimes
# depends : python

name=python-orjson
version=3.6.1
release=1

source="https://files.pythonhosted.org/packages/92/97/895dfe0c2e7820fd5453d0efab6a9036de7f97b4edfd157a6a414dd3b0ee/orjson-3.6.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
