# description : MessagePack serializer implementation for Python
# depends : python

name=python-msgpack
version=1.0.2
release=2

source="msgpack-python-1.0.2.tar.gz::https://github.com/msgpack/msgpack-python/archive/v1.0.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
