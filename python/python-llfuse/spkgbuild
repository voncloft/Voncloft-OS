# description : A set of Python bindings for the low level FUSE API.
# depends : python fuse2

name=python-llfuse
version=1.4.1
release=2

source="https://files.pythonhosted.org/packages/b1/d4/44443fbaac6d5b878da99e7c0948ee93c7934fa3b00e48c5363823b583d0/llfuse-1.4.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
