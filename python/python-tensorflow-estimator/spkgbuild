# description : A high-level TensorFlow API that greatly simplifies machine learning programming
# depends : python-wrapt

name=python-tensorflow-estimator
version=2.6.0
release=4

source="python-tensorflow-estimator-2.6.0.tar.gz::https://github.com/tensorflow/estimator/archive/v2.6.0.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
