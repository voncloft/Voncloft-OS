# description : A Python toolbox for build()ing complex digital hardware
# depends : python-colorama

name=python-migen
version=0.9.2
release=3

source="https://files.pythonhosted.org/packages/cc/f0/b38382ac821861c193992817e3687e49122d09f130f6a33a7d01a4f01bc1/migen-0.9.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
