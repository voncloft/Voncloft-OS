# description : A library implementing the SemVer scheme.
# depends : python

name=python-semantic-version
version=2.8.5
release=3

source="python-semantic-version-2.8.5.tar.gz::https://github.com/rbarrois/python-semanticversion/archive/2.8.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
