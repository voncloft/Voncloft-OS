# description : PyYAML-based module to produce pretty and readable YAML-serialized data
# depends : python-yaml

name=python-pyaml
version=21.8.3
release=4

source="https://files.pythonhosted.org/packages/cc/c8/70d6b275de6ab108d9e210d73463e4523b633917e9223a56f8442ce44110/pyaml-21.8.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
