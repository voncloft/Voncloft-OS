# description : Create and publish pure python DBus objects using jeepney
# depends : python-jeepney

name=python-jeepney-objects
version=0.2.0
release=1

source="python-jeepney-objects-0.2.0.tar.gz::https://github.com/ocaballeror/jeepney-objects/archive/v0.2.0.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
