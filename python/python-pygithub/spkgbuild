name=python-pygithub
version=1.55
release=1

source="python-pygithub-1.55.tar.gz::https://github.com/PyGithub/PyGithub/archive/v1.55.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
