# description : Fast HTML/XML Template Compiler
# depends : python

name=python-chameleon
version=3.9.1
release=2

source="https://github.com/malthe/chameleon/archive/3.9.1/python-chameleon-3.9.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
