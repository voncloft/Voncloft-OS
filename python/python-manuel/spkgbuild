# description : Manuel lets you build() tested documentation
# depends : python-setuptools

name=python-manuel
version=1.10.1
release=5

source="python-manuel-1.10.1.tar.gz::https://github.com/benji-york/manuel/archive/1.10.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
