# description : Style-preserving TOML library for Python
# depends : python

name=python-tomlkit
version=0.7.2
release=3

source="https://files.pythonhosted.org/packages/source/t/tomlkit/tomlkit-0.7.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
