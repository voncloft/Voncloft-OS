# description : Python-based project scripting
# depends : python-six

name=python-paver
version=1.3.4
release=4

source="python-paver-1.3.4.tar.gz::https://github.com/paver/paver/archive/Paver-1.3.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
