# description : Official timezone database for Python.
# depends : python

name=python-pytzdata
version=2020.1
release=3

source="python-pytzdata-2020.1.tar.gz::https://github.com/sdispater/pytzdata/archive/2020.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
