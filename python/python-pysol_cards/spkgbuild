# description : Deal PySol FC Cards
# depends : python-pbr

name=python-pysol_cards
version=0.12.0
release=3

source="https://pypi.io/packages/source/p/pysol_cards/pysol_cards-0.12.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
