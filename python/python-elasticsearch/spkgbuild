# description : Python client for Elasticsearch
# depends : python-urllib3

name=python-elasticsearch
version=7.15.0
release=3

source="https://files.pythonhosted.org/packages/f9/31/4004b291edf5eb7f4b81e3cfd453384357a743f6f8bdc61a39ee19474024/elasticsearch-7.14.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
