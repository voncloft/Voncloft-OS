# description : Python client for Elasticsearch
# depends : python-urllib3

name=python-elasticsearch
version=7.14.0
release=3

source="https://files.pythonhosted.org/packages/fb/24/d795d3029833ad4fc7b7b62d55ab17496f7db21e8ff0125076b57be1f4ff/elasticsearch-7.14.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
