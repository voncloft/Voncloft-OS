# description : Python module designed to help make the creation, manipulation and analysis of binary data as simple and natural as possible
# depends : python

name=python-bitstring
version=3.1.8
release=3

source="https://files.pythonhosted.org/packages/c3/fc/ffac2c199d2efe1ec5111f55efeb78f5f2972456df6939fea849f103f9f5/bitstring-3.1.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
