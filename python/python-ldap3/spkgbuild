# description : A strictly RFC 4510 conforming LDAP V3 pure Python client library
# depends : python-pyasn1

name=python-ldap3
version=2.9.1
release=3

source="https://files.pythonhosted.org/packages/43/ac/96bd5464e3edbc61595d0d69989f5d9969ae411866427b2500a8e5b812c0/ldap3-2.9.1.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
