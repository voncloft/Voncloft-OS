# description : A lightweight library for converting complex datatypes to and from native Python datatypes
# depends : python

name=python-marshmallow
version=3.13.0
release=1

source="https://files.pythonhosted.org/packages/88/92/8aac4cb538002d38ae068b8d7bc9fabc2964eb97672e3cb6e16682cdc5be/marshmallow-3.13.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
