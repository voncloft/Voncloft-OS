# description : Python version of Googles common library for parsing, formatting, storing and validating international phone numbers
# depends : python

name=python-phonenumbers
version=8.12.28
release=1

source="https://files.pythonhosted.org/packages/4e/1f/55cb0d522b12808c79747c10ca0ac206c37bd3db9f5e11901657f6aae8ef/phonenumbers-8.12.28.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
