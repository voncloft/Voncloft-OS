# description : Python version of Googles common library for parsing, formatting, storing and validating international phone numbers
# depends : python

name=python-phonenumbers
version=8.12.32
release=1

source="https://files.pythonhosted.org/packages/19/0a/67a2485754fe192d8d7352c97c7fc5d906daae216f0e402056f26bc1ae91/phonenumbers-8.12.31.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
