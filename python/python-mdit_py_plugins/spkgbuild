# description : Collection of core plugins for markdown-it-py
# depends : python-markdown-it-py

name=python-mdit_py_plugins
version=0.2.4
release=1

source="https://files.pythonhosted.org/packages/10/a0/2d3df3cd0c55b4024d153b31b403e02a1af50d78df3a2fb22190100a7004/mdit-py-plugins-0.2.8.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
