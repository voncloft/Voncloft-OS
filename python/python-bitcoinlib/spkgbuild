# description : Python3 library providing an easy interface to the Bitcoin data structures and protocol
# depends: python

name=python-bitcoinlib
version=0.11.0
release=2

source="https://files.pythonhosted.org/packages/c9/37/7e8f81a06f8a6faf175eed697b801f7d544cf6a04d9091fd8a69d3f28fdb/bitcoinlib-0.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
