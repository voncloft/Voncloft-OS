# description : MaxMind GeoIP2 API
# depends : python-aiohttp python-maxminddb python-requests

name=python-geoip2
version=4.4.0
release=3

source="https://github.com/maxmind/GeoIP2-python/archive/v4.2.0/python-geoip2-4.2.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
