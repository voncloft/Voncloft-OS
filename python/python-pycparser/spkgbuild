# description : C parser and AST generator written in Python
  # depends : python-ply

name=python-pycparser
version=2.20
release=3

source="python-pycparser-2.20.tar.gz::https://github.com/eliben/pycparser/archive/release_v2.20.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
