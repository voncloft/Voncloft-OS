# description : File support for asyncio
# depends :

name=python-aiofiles
version=0.7.0
release=3

source="https://github.com/Tinche/aiofiles.git#tag=/archive/refs/tags/$version.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
