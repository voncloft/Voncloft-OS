# description : A WSGI Proxy with various http client backends
# depends : python-webob python-six

name=python-wsgiproxy2
version=0.5.1
release=5

source="https://files.pythonhosted.org/packages/f0/ae/cad3131f771a38b4cdad6ca82bfee1800afc69758c70d0e70483ed1f6b30/WSGIProxy2-0.5.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
