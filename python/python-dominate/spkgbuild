# description : A Python library for creating and manipulating HTML documents using an elegant DOM API
# depends : python

name=python-dominate
version=2.6.0
release=3

source="https://files.pythonhosted.org/packages/29/23/edf8e470f1053245c1aa99d92c8a3da9e83f6c7d3eb39205486965425be5/dominate-2.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
