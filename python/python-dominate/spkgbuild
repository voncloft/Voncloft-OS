# description : A Python library for creating and manipulating HTML documents using an elegant DOM API
# depends : python

name=python-dominate
version=2.6.0
release=3

source="python-dominate-2.6.0.tar.gz::https://github.com/Knio/dominate/archive/2.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
