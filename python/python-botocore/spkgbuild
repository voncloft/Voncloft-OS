# description : A low-level interface to a growing number of Amazon Web Services
# depends : python-dateutil python-jmespath python-docutils python-urllib3

name=python-botocore
version=1.21.7
release=1

source="https://files.pythonhosted.org/packages/29/b1/3818dfdefda8e1e82dbc4d1ca34cbe3eb7cd43d028b7f0e63117635fbc23/botocore-1.21.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
