# description : A low-level interface to a growing number of Amazon Web Services
# depends : python-dateutil python-jmespath python-docutils python-urllib3

name=python-botocore
version=1.21.4
release=1

source="https://files.pythonhosted.org/packages/45/bb/1f429d5d3ef6860782eabb66e3850bc3c665a5bd21088f27eb9771b925fd/botocore-1.21.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
