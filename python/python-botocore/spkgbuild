name=python-botocore
version=1.21.23
release=1

source="https://files.pythonhosted.org/packages/a6/74/c4f3118df00ef7b7d975571ed2f3cbac71e87b396b2171be9879d7b57ffb/botocore-1.21.23.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
