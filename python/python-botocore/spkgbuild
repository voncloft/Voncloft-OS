name=python-botocore
version=1.21.25
release=1

source="https://files.pythonhosted.org/packages/67/82/6b0c667330a91c04d546a4f8f38bac7f73fe4df36b0446b1eb01cc795bf1/botocore-1.21.25.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
