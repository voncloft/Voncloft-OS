name=python-botocore
version=1.21.23
release=1

source="https://files.pythonhosted.org/packages/ad/57/b42c1660647506c1af106f7c1b1fc9eac9de53ce2310089d5f38b8a472cf/botocore-1.21.24.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
