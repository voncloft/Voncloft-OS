# description : A low-level interface to a growing number of Amazon Web Services
# depends : python-dateutil python-jmespath python-docutils python-urllib3

name=python-botocore
version=1.21.1
release=1

source="https://files.pythonhosted.org/packages/66/5c/b88bc63c2b50965abd68abdd9f65b2d58a4d815e46be725a66027e610d37/botocore-1.21.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
