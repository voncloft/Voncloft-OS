name=python-botocore
version=1.21.39
release=1

source="https://files.pythonhosted.org/packages/35/e5/a7e9ee94d0ece4279a68fa042165a9dcc89dad12a0909355885369afa357/botocore-1.21.39.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
