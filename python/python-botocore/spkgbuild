name=python-botocore
version=1.21.28
release=1

source="https://files.pythonhosted.org/packages/72/fb/a11b1794e9687b87ece8be78397a9b029bd321065b49124e85a8548172f2/botocore-1.21.28.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
