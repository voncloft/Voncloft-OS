# description : A low-level interface to a growing number of Amazon Web Services
# depends : python-dateutil python-jmespath python-docutils python-urllib3

name=python-botocore
version=1.21.17
release=1

source="https://files.pythonhosted.org/packages/8d/f4/c98c4b194ac9552de7741372bcbdcd290d93fda377e527546e9868865948/botocore-1.21.17.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
