name=python-botocore
version=1.21.27
release=1

source="https://files.pythonhosted.org/packages/c1/18/b206219f706f6353cee49e3c500688bdcfee79011694bbc8d70e3e22d66b/botocore-1.21.27.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
