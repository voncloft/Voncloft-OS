# description : A low-level interface to a growing number of Amazon Web Services
# depends : python-dateutil python-jmespath python-docutils python-urllib3

name=python-botocore
version=1.21.14
release=1

source="https://files.pythonhosted.org/packages/3d/e2/00260951ceaf88f604568453823fc793f9c2ff1e3484ea09b869946c11e1/botocore-1.21.14.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
