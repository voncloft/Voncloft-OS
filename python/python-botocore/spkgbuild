name=python-botocore
version=1.21.15
release=1

source="https://files.pythonhosted.org/packages/4c/2e/b7c54575c7a9fbe9c9d48195b6c5534c7abef353ea009f9323372bc12294/botocore-1.21.15.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
