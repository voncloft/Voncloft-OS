# description : A low-level interface to a growing number of Amazon Web Services
# depends : python-dateutil python-jmespath python-docutils python-urllib3

name=python-botocore
version=1.21.12
release=1

source="https://files.pythonhosted.org/packages/ec/0f/cf443c142af1e5a7728a5357304e39bfe63ab6a5eb84035eefa509bd843d/botocore-1.21.12.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
