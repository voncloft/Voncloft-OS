name=python-oslo-concurrency
version=4.4.0
release=1

source="https://files.pythonhosted.org/packages/29/98/e494941aea82de0f155e89641ea911ea447eed773dec9ac0f8d8f547209b/oslo.concurrency-4.4.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
