# description : A sphinx extension for embedding sequence diagram using seqdiag
# depends : blockdiag seqdiag python-sphinx

name=python-sphinxcontrib-seqdiag
version=2.0.0
release=3

source="python-sphinxcontrib-seqdiag-2.0.0.tar.gz::https://github.com/blockdiag/sphinxcontrib-seqdiag/archive/2.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
