# description	: Python library for ISO 639 standard
# depends	: python

name=python-iso-639
version=0.4.5
release=1
source="https://files.pythonhosted.org/packages/source/${name:8:1}/${name#*-}/${name#*-}-$version.tar.gz"

build() {
	cd ${name#*-}-$version
	python setup.py build
	python setup.py install --root=$PKG --optimize=1
}
