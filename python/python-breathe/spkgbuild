name=python-breathe
version=4.31.0
release=2

source="https://files.pythonhosted.org/packages/ae/32/146cb3eaea5f4eb4e0aaac78cfa92d4905b33b42fc675c4ac3b59d555aee/breathe-4.31.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
