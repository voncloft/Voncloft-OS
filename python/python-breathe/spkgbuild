# description : An extension to reStructuredText and Sphinx to be able to read and render Doxygen xml output
# depends : python python-sphinx python-docutils python-six

name=python-breathe
version=4.30.0
release=2

source="https://files.pythonhosted.org/packages/95/76/0c9adb4b20082091a0236e558797455ce819b0d9db86570c9a01139c66e2/breathe-4.30.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
