# description : Expressive and extensible TDD/BDD assertion library for Python
# depends : python

name=python-expects
version=0.9.0
release=6

source="python-expects-0.9.0.tar.gz::https://github.com/jaimegildesagredo/expects/archive/v0.9.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
