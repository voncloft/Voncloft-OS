# description : OpenSSH public key parser for Python
# depends : python-ecdsa python-cryptography

name=python-sshpubkeys
version=3.3.1
release=1

source="https://files.pythonhosted.org/packages/a3/b9/e5b76b4089007dcbe9a7e71b1976d3c0f27e7110a95a13daf9620856c220/sshpubkeys-3.3.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
