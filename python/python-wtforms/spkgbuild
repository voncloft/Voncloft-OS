# description : Flexible forms validation and rendering library for python web development
# depends : python

name=python-wtforms
version=3.0.0
release=10

source="https://github.com/wtforms/wtforms/archive/2.3.3/python-wtforms-2.3.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
