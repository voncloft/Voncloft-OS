# description : A compact, flexible and standards-abiding IRC library
# depends : python

name=python-pydle
version=0.9.4
release=2

source="https://github.com/Shizmob/pydle/archive/v0.9.4/python-pydle-0.9.4.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
