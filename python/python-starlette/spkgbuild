# description : The little ASGI framework that shines
# depends : python

name=python-starlette
version=0.16.0
release=3

source="python-starlette-0.14.2.tar.gz::https://github.com/encode/starlette/archive/0.14.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
