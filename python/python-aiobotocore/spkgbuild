# description : asyncio support for botocore library using aiohttp
# depends : python python-aiohttp python-botocore python-wrapt python-aioitertools

name=python-aiobotocore
version=1.4.0
release=1

source="https://files.pythonhosted.org/packages/15/0e/bd904de27b80243cb13b885eb272420a46be2aafa6ba6fe6f085a0f10990/aiobotocore-1.4.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
