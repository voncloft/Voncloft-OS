# description : asyncio support for botocore library using aiohttp
# depends : python python-aiohttp python-botocore python-wrapt python-aioitertools

name=python-aiobotocore
version=1.4.1
release=1

source="https://files.pythonhosted.org/packages/07/60/64d666c149d7087d619c1eef8c23bece6068a61fa3c4c033c7f4f2e6452d/aiobotocore-1.4.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
