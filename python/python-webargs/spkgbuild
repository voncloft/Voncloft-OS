# description : A friendly library for parsing HTTP request arguments, with built-in support for popular web frameworks
# depends : python-marshmallow

name=python-webargs
version=8.0.1
release=3

source="https://files.pythonhosted.org/packages/d9/58/24538fd2bf7164abea0d9e53b3dddb49fda3ff7008e2bfd1309aaf8244f5/webargs-8.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
