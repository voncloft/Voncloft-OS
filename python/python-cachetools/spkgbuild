# description : Extensible memoizing collections and decorators
# depends : python

name=python-cachetools
version=4.2.3
release=1

source="python-cachetools-4.2.2.tar.gz::https://github.com/tkem/cachetools/archive/v4.2.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
