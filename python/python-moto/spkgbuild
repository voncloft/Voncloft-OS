# description : Moto is a library to mock out the boto library.
# depends : python-aws-xray-sdk python-boto python-boto3

name=python-moto
version=2.2.5
release=5

source="https://files.pythonhosted.org/packages/28/af/782477b607386d08284c384fb66ba2c1f5d8c7a60a0f5f5a14ab74cedebc/moto-2.2.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
