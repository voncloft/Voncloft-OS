# description : Moto is a library to mock out the boto library.
# depends : python-aws-xray-sdk python-boto python-boto3

name=python-moto
version=2.2.2
release=5

source="https://files.pythonhosted.org/packages/3c/c3/cd9ee8cf0c520ed2b5e98b11049a281fd1a1bec2871a9ab6e1da6183b64f/moto-2.2.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
