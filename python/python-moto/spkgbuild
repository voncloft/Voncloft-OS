# description : Moto is a library to mock out the boto library.
# depends : python-aws-xray-sdk python-boto python-boto3

name=python-moto
version=2.2.7
release=5

source="https://files.pythonhosted.org/packages/be/e7/ec32d675fdfc22324c6bc7680d7789a382029dd23b483c186e8050e74195/moto-2.2.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
