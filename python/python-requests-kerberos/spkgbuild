# description : A kerberos of useful classes and functions to be used with python-requests.
# depends : python-requests python-pykerberos python-cryptography

name=python-requests-kerberos
version=0.12.0
release=7

source="python-requests-kerberos-0.12.0.tar.gz::https://github.com/requests/requests-kerberos/archive/v0.12.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
