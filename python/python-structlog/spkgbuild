# description : Structured Logging for Python
# depends : python-six

name=python-structlog
version=21.1.0
release=1

source="python-structlog-21.1.0.tar.gz::https://github.com/hynek/structlog/archive/21.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
