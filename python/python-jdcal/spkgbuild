# description : Julian dates, from proleptic Gregorian and Julian calendars
# depends : python

name=python-jdcal
version=1.4.1
release=6

source="https://github.com/phn/jdcal/archive/v1.4.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
