# description : Library for fake certificate authority CA to generate fake TLS certs
# depends : python python-cryptography

name=python-trustme
version=0.9.0
release=3

source="https://files.pythonhosted.org/packages/34/8e/66203d0c8c4557db28772678359e0878610624db8438b1ed9361ceeaad1d/trustme-0.9.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
