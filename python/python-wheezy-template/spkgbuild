# description : lightweight template library
# depends : python

name=python-wheezy-template
version=3.0.3
release=1

source=python-wheezy-template-3.1.0.tar.gz::https://github.com/akornatskyy//archive/3.1.0.tar.gz
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
