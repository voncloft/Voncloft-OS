# description : A pluggable API specification generator - currently supports the OpenAPI Specification
# depends : python

name=python-apispec
version=5.0.0
release=3

source="https://files.pythonhosted.org/packages/e3/d8/6ce8ae00be94079b551243c4ec34a442021634a0a95fb2ca8152a4bbe73d/apispec-5.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
