# description : A pluggable API specification generator - currently supports the OpenAPI Specification
# depends : python

name=python-apispec
version=5.1.0
release=3

source="https://files.pythonhosted.org/packages/53/37/e471e9f6c2c9157059b832f215c12b24be115a236540079eb15516dca270/apispec-5.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
