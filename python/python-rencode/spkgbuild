# description : A Module similar to bencode from the BitTorrent project
# depends : python

name=python-rencode
version=1.0.6
release=5

source="python-pytorch-1.0.6.tar.gz::https://github.com/aresch/rencode/archive/v1.0.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
