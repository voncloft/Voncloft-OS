# description : Email bounce detectors
# depends : python-atpublic python-zope-interface

_name=flufl.bounce
url=https://fluflbounce.readthedocs.io/en/latest/
name=python-flufl.bounce
version=3.0.1
release=3

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="!checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
