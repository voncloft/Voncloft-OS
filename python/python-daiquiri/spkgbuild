# description : Library to configure Python logging easily
# depends : python-json-logger

name=python-daiquiri
version=3.0.1
release=3

source="https://files.pythonhosted.org/packages/39/a5/b81dbb0d1d94eb9b880e068aa827afc7afcd2a2cc5edff837af42df84796/daiquiri-3.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
