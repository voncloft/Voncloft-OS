# description : A library for stubbing in Python
  # depends : python

name=python-pretend
version=1.0.9
release=6

source="python-pretend-1.0.9.tar.gz::https://github.com/alex/pretend/archive/v1.0.9.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
