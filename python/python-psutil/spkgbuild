# description : a cross-platform process and system utilities module for python
# depends : python

name=python-psutil
version=5.8.0
release=1

source="https://github.com/giampaolo/psutil/archive/release-5.8.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
