# description : Read/rewrite/write Python ASTs
# depends : python

name=python-astor
version=0.8.1
release=3

source="python-astor-0.8.1.tar.gz::https://github.com/berkerpeksag/astor/archive/0.8.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
