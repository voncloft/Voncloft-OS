name=python-mediafile
version=0.8.0
release=1

source="https://files.pythonhosted.org/packages/63/88/3a4c88d95123720478abc39f97a39196b3395f52a382b1ee6ad691589bc2/mediafile-0.8.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
