name=python-mediafile
version=0.7.0
release=1

source="https://files.pythonhosted.org/packages/ea/52/daaabff03f831b6e77fe68cac723800c8e124f383338856b1974c0e8d47f/mediafile-0.7.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
