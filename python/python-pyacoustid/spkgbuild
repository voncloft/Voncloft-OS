# depends : 

name=python-pyacoustid
version=1.2.2
release=2

source="https://files.pythonhosted.org/packages/97/e6/aeada0cc05e510d655f17a4336d5a108e4bdfa95eb7d3cc0ca5415bbe568/pyacoustid-1.2.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
