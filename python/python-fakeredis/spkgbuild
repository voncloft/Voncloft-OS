# description : Fake implementation of redis API redis-py for testing purposes
# depends : python-redis python-six python-sortedcontainers

name=python-fakeredis
version=1.6.1
release=3

source="https://files.pythonhosted.org/packages/a6/43/f4f3fb619719df9851a6276d005f20cd9a02142d333ab3591c9c4436bb84/fakeredis-1.6.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
