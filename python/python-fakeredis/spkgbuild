# description : Fake implementation of redis API redis-py for testing purposes
# depends : python-redis python-six python-sortedcontainers

name=python-fakeredis
version=1.6.0
release=3

source="https://files.pythonhosted.org/packages/49/35/c2581218fff43a71e798ce774fbf20d5af4521c45172aa46e5e54eedc77a/fakeredis-1.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
