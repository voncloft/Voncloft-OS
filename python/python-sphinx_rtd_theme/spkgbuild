# description : Read the Docs theme for Sphinx
# depends : python-sphinx

name=python-sphinx_rtd_theme
version=0.5.1
release=2

source="https://files.pythonhosted.org/packages/1c/32/580309c9fd5b1892c6616ce814710c6b14423e98bf1c101bf2c710433cee/sphinx_rtd_theme-1.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
