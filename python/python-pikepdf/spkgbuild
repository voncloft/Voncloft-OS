# description : Read and write PDFs with Python, powered by qpdf
# depends : python-lxml python-pillow qpdf

name=python-pikepdf
version=2.16.0
release=1

source="https://files.pythonhosted.org/packages/4a/26/b0aafc966bc89079ef6ab421ac0849ebd4dd931dcc9367526b531b8d0675/pikepdf-2.16.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
