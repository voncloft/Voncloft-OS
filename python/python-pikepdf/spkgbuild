# description : Read and write PDFs with Python, powered by qpdf
# depends : python-lxml python-pillow qpdf

name=python-pikepdf
version=3.0.0
release=1

source="https://files.pythonhosted.org/packages/a4/5c/6770b064dbe6402397cf87f07b92a252f73099705fcf980030404767bc62/pikepdf-3.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
