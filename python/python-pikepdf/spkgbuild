# description : Read and write PDFs with Python, powered by qpdf
# depends : python-lxml python-pillow qpdf

name=python-pikepdf
version=2.15.0
release=1

source="https://files.pythonhosted.org/packages/07/d6/4fda075ee91a6232e3fe45266412a5023152f9f2d55d41bce7847a016422/pikepdf-2.15.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
