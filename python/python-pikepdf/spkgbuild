# description : Read and write PDFs with Python, powered by qpdf
# depends : python-lxml python-pillow qpdf

name=python-pikepdf
version=2.15.0
release=1

source="https://files.pythonhosted.org/packages/f4/72/8fb625a17f548bc67ef103a830962d0c4af6a45edff8f4d22534b84d44fd/pikepdf-2.15.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
