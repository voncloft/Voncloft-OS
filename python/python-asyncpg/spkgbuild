# description : A fast PostgreSQL Database Client Library for Python/asyncio
# depends : python

name=python-asyncpg
version=0.24.0
release=2

source="https://files.pythonhosted.org/packages/ba/29/cca39b99164d9e99a2b30c207e1a8fb682d062b39cef0f214a293bba113c/asyncpg-0.24.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
