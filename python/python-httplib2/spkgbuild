# description : Comprehensive HTTP client library, supporting many features
# depends : python make-ca

name=python-httplib2
version=0.19.1
release=4

source="https://files.pythonhosted.org/packages/ed/cd/533a1e9e04671bcee5d2854b4f651a3fab9586d698de769d93b05ee2bff1/httplib2-0.19.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
