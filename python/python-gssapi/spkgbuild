# description : Provides both low-level and high level wrappers around the GSSAPI C libraries
# depends : python python-decorator krb5

name=python-gssapi
version=1.7.0
release=2

source="https://files.pythonhosted.org/packages/2d/f2/2a5bf44f878921c92d69f9e8edf6f5f382365e49c34c2764ec2a5bda5639/gssapi-1.7.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
