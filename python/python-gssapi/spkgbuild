# description : Provides both low-level and high level wrappers around the GSSAPI C libraries
# depends : python python-decorator krb5

name=python-gssapi
version=1.7.0
release=2

source="https://github.com/pythongssapi/python-gssapi/archive/v1.6.14/python-gssapi-1.6.14.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
