# description : Python implementation of the SCRAM protocol
# depends : python python-asn1crypto

name=python-scramp
version=1.4.0
release=1

source="https://files.pythonhosted.org/packages/69/d5/69632dfab1806d0dad816311733694768d8f205691cbad2dc7cca4159bb5/scramp-1.4.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
