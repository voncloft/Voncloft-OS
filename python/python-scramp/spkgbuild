# description : Python implementation of the SCRAM protocol
# depends : python python-asn1crypto

name=python-scramp
version=1.4.1
release=1

source="https://files.pythonhosted.org/packages/56/26/7cb510bc22da4e6234e9b90586c43eb4fbece4004b35d5a00987808185f5/scramp-1.4.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
