# description : Simplified, fast RSS parsing library in Python
# depends : python

name=python-podcastparser
version=0.6.7
release=1

source="https://files.pythonhosted.org/packages/4d/a2/a4a17ea6c89ac3b1c82c0210513a961997dc8bb6e792fb6b832db0cb0076/podcastparser-0.6.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
