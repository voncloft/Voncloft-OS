# description : Simplified, fast RSS parsing library in Python
# depends : python

name=python-podcastparser
version=0.6.8
release=1

source="https://github.com/gpodder/podcastparser/archive/0.6.7/python-podcastparser-0.6.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
