# description : An app to handle configuration defaults of packaged Django apps gracefully
# depends : python-django python-six

name=python-django-appconf
version=1.0.4
release=4

source="https://files.pythonhosted.org/packages/11/17/3093cd487d17e54d7d25ede33b0c7694982bf5f2a3d8055059f25eb9f90c/django-appconf-1.0.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
