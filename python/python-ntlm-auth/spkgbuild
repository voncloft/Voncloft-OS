# description : Creates NTLM authentication structures
# depends : python python-six python-cryptography

name=python-ntlm-auth
version=1.5.0
release=3

source="python-ntlm-auth-1.5.0.tar.gz::https://github.com/jborean93/ntlm-auth/archive/v1.5.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
