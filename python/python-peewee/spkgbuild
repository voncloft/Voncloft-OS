# description : a little orm
# depends : python sqlite

name=python-peewee
version=3.14.4
release=3

source="python-peewee-3.14.4.tar.gz::https://github.com/coleifer/peewee/archive/3.14.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
