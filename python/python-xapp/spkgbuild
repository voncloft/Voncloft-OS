# description : Python Xapp Library
# depends : python python-psutil

name=python-xapp
version=2.2.1
release=2

source=python3-xapp-2.2.1.tar.gz::https://github.com/linuxmint/python3-xapp/archive/2.2.1.tar.gz
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
