name=python-fields
version=5.0.0
release=9

source="python-fields-5.0.0.tar.gz::https://github.com/ionelmc/python-fields/archive/v5.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
