# description : Deepin desktop application engine
# depends : python-xlib python-pyqt5

url=https://github.com/linuxdeepin/dae
name=python-dae
version=1.0.2
release=2

source="https://github.com/martyr-deepin/dae/archive/$version.tar.gz"
options="!checksum"

build() {
   cd dae-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
