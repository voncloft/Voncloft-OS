# description : Lazy attributes for Python objects
# depends : python

name=python-lazy
version=1.4
release=5

source="python-lazy-1.4.tar.gz::https://github.com/stefanholek/lazy/archive/1.4.tar.gz"
options="!checksum"

build() {
   cd ${name#python-}-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
