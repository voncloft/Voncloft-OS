# description : Mock/Stub/Spy library for Python
# depends : python

name=python-flexmock
version=0.10.7
release=6

source="https://files.pythonhosted.org/packages/db/0a/90bbc8b570d837afbb9fa3c8612bbcb8a6999f7dbdd2ab997d329de82efe/flexmock-0.10.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
