# description : WSDL parsing services package for Web Services for Python
  # depends : python-six

name=python-wstools
version=0.4.11
release=3

source="https://github.com/pycontribs/wstools/archive/0.4.10/python-wstools-0.4.10.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
