name=python-telegram-bot
version=13.7
release=1

source="https://github.com/python-telegram-bot/python-telegram-bot/archive/b3e935355afcfb5240bac5a99707ffecc35772a2/python-telegram-bot-13.1.20210105.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
