# description : Munkres algorithm for Python
# depends : python

name=python-munkres
version=1.1.4
release=1

source="https://files.pythonhosted.org/packages/fd/41/6a3d0ef908f47d07c31e5d1c2504388c27c39b10b8cf610175b5a789a5c1/munkres-1.1.4.tar.gz"
