# description : Python library for Hawk HTTP authorization
# depends : python-six

name=python-mohawk
version=1.1.0
release=3

source="https://github.com/kumar303/mohawk/archive/1.1.0/python-mohawk-1.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
