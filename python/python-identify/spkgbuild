# description : File identification library for Python
# depends : python

name=python-identify
version=2.2.14
release=1

source="https://files.pythonhosted.org/packages/41/27/01746b4bc5de24e307ba62d1fc637522fb0475e4ecbec947a2e4fdba698e/identify-2.2.14.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
