# description : File identification library for Python
# depends : python

name=python-identify
version=2.2.12
release=1

source="https://files.pythonhosted.org/packages/fa/ae/b2361177e92bcea99d5dded37bfa03f7f48d8a1dca9c14906d3d4b0d0a65/identify-2.2.12.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
