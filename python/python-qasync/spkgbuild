name=python-qasync
version=0.20.0
release=1

source="https://files.pythonhosted.org/packages/f2/d3/b9c1e805cdbdece984fbeb88c2f24cc225d4f7c8075d12bba88f587395ef/qasync-0.20.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
