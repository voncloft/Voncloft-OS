# description : Python library for using asyncio in Qt-based applications
# depends : python qt5-python-bindings

name=python-qasync
version=0.21.0
release=1

source="https://files.pythonhosted.org/packages/58/cf/40bbad97cf8d671fd360b8df85c1842d83ad837730d6b7f328c7daf1e7d4/qasync-0.21.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
