# description : A Python module for decorators, wrappers and monkey patching
# depends : python

name=python-wrapt
version=1.13.0
release=4

source="python-wrapt-1.12.1.tar.gz::https://github.com/GrahamDumpleton/wrapt/archive/1.12.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
