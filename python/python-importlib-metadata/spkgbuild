# description : Read metadata from Python packages
# depends : python-zipp

name=python-importlib-metadata
version=4.8.1
release=1

source="https://github.com/python/importlib_metadata/archive/v4.8.1/python-importlib-metadata-4.8.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
