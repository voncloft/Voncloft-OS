name=python-flake8-isort
version=4.0.0
release=3

source="https://files.pythonhosted.org/packages/62/3d/886cd18de8b407d35130f8a120d1a8a1ce8995900e6463e1176276760101/flake8-isort-4.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
