# description : A Python module for creating Excel XLSX files
# depends : python

name=python-xlsxwriter
version=3.0.1
release=1

source="https://files.pythonhosted.org/packages/5d/36/e943d07af9d26cc2f11861955dbf0031e891f77f3d55f70217fd6a0f4d9f/XlsxWriter-3.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
