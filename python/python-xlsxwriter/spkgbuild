# description : A Python module for creating Excel XLSX files
# depends : python

name=python-xlsxwriter
version=2.0.0
release=1

source="https://files.pythonhosted.org/packages/ea/3b/ed27791a0daf03919f15e30ee3bcda7645e4ad2c0c97eae94c7a70f6e586/XlsxWriter-2.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
