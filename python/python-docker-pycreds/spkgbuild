# description : Python bindings for the docker credentials store API
# depends : python-six

name=python-docker-pycreds
version=0.4.0
release=7

source="python-docker-pycreds-0.4.0.tar.gz::https://github.com/shin-/dockerpy-creds/archive/0.4.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
