# description : Asynchronous network probes for Python
# depends : mtr python

name=python-mtrpacket
version=1.0.0
release=2

source="https://github.com/matt-kimball/mtr-packet-python/archive/1.0.0/python-mtrpacket-1.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
