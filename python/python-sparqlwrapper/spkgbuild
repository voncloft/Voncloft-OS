# description : SPARQL Endpoint interface to Python
# depends : python

name=python-sparqlwrapper
version=1.8.5
release=2

source="python-sparqlwrapper-1.8.5.tar.gz::https://github.com/RDFLib/sparqlwrapper/archive/1.8.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
