# description : News Feed extension for Sphinx
# depends : python-sphinx

name=python-sphinxcontrib-newsfeed
version=0.1.4
release=11

source="https://files.pythonhosted.org/packages/2b/5e/8bc839b5c4ef030bf26eede24208a49f25d00033cbd4969b3895264f14db/sphinxcontrib-newsfeed-0.1.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
