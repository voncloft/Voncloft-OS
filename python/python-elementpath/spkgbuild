# description : XPath 1.0/2.0 parsers and selectors for ElementTree and lxml
# depends : python

name=python-elementpath
version=2.3.1
release=1

source="https://files.pythonhosted.org/packages/b1/36/bf11af8c104af916f7d17b50fe2ebcd45cb2da20312735f802052f4e09e9/elementpath-2.3.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
