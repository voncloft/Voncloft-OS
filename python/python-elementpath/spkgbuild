# description : XPath 1.0/2.0 parsers and selectors for ElementTree and lxml
# depends : python

name=python-elementpath
version=2.3.1
release=1

source="python-elementpath-2.3.1.tar.gz::https://github.com/sissaschool/elementpath/archive/v2.3.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
