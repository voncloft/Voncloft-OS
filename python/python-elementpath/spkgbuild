# description : XPath 1.0/2.0 parsers and selectors for ElementTree and lxml
# depends : python

name=python-elementpath
version=2.3.0
release=1

source="https://files.pythonhosted.org/packages/0d/4c/df4930a41e45c0f0699ff458cb1d6987120a2ffb74394680909d6ef8f0cb/elementpath-2.3.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
