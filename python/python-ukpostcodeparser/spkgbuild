# description : UK Postcode parser
# depends : python

name=python-ukpostcodeparser
version=1.1.2
release=8

source="python-tox-1.1.2.tar.gz::https://github.com/hamstah/ukpostcodeparser/archive/v1.1.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
