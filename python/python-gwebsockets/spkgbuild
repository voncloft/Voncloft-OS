# description : GLib based websocket server written in python
# depends : python-gobject

name=python-gwebsockets
version=0.7
release=3

source="https://files.pythonhosted.org/packages/8a/09/98b91d6435b15a031da57a4d4faba89e022da75f2cb4d3ff1780045cd0ac/gwebsockets-0.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
