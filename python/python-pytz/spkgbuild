# description : Cross platform time zone library for Python
# depends : python

name=python-pytz
version=2021.1
release=1

source="https://pypi.io/packages/source/p/pytz/pytz-2021.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
