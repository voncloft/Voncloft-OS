# description : Braintree Python Library
# depends : python-requests

name=python-braintree
version=4.10.0
release=1

source="https://files.pythonhosted.org/packages/fc/79/8dfb387701a7ff4ec01b1777c7617428739a4d6a289b9a5af7d5f85f516f/braintree-4.10.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
