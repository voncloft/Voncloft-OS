# description : Braintree Python Library
# depends : python-requests

name=python-braintree
version=4.12.0
release=1

source="https://files.pythonhosted.org/packages/24/dc/64fedfd0083e8dbba31e8de8c79b7cebfc6a1e0e7bd21b1bf40db7017ddb/braintree-4.12.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
