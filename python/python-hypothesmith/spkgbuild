# description : Hypothesis strategies for generating Python programs, something like CSmith
# depends : python-hypothesis python-lark-parser python-libcst

name=python-hypothesmith
version=0.1.9
release=1

source="https://files.pythonhosted.org/packages/04/e8/92a2af7005c0f3e37f539c83b9300bd29e07dbd64403739c4f2232bfdd8c/hypothesmith-0.1.9.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
