# description : Base types for calmjs framework
# depends : python

_name=calmjs.types
url=https://github.com/calmjs/calmjs.types
name=python-calmjs.types
version=1.0.1
release=3

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.zip"
options="!checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
