# description : A Python library for the Docker Engine API
# depends : python-requests python-six python-websocket-client python-docker-pycreds python-paramiko

name=python-docker
version=5.0.2
release=1

source="https://files.pythonhosted.org/packages/b9/e4/b902199a501528bae3de060bb3b2e96e3362ec0311dd3e7e38e5e2d66080/docker-5.0.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
