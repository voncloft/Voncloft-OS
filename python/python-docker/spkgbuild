# description : A Python library for the Docker Engine API
# depends : python-requests python-six python-websocket-client python-docker-pycreds python-paramiko

name=python-docker
version=5.0.1
release=1

source="https://files.pythonhosted.org/packages/e6/c9/e0148cb23c2f521d4297458f10c7063743eafbfb78b4490bf9cf731f60be/docker-5.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
