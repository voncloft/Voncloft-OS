# description : Python2s stdlib csv module is nice, but it doesnt support unicode. This module is a drop-in replacement which *does*.

name=python-unicodecsv
version=0.14.1
release=4

source="python-unicodecsv-0.14.1.tar.gz::https://github.com/jdunck/python-unicodecsv/archive/0.14.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
