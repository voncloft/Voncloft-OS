# description : Namespace control and lazy-import mechanism
# depends : python

name=python-apipkg
version=1.5
release=5

source="https://files.pythonhosted.org/packages/source/a/apipkg/apipkg-1.5.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
