# description : Python SDK for Dropbox Core APIs
# depends : python python-urllib3 python-requests

name=python-dropbox
version=11.15.0
release=2

source="https://files.pythonhosted.org/packages/7d/3a/626cb1793da705e76f2458658ba304b5233d497196eb53a04c0cb460c4cd/dropbox-11.15.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
