# description : Python SDK for Dropbox Core APIs
# depends : python python-urllib3 python-requests

name=python-dropbox
version=11.19.0
release=2

source="python-dropbox-10.10.0.tar.gz::https://github.com/dropbox/dropbox-sdk-python/archive/v10.10.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
