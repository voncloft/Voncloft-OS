# description : Python SDK for Dropbox Core APIs
# depends : python python-urllib3 python-requests

name=python-dropbox
version=11.16.0
release=2

source="https://files.pythonhosted.org/packages/9f/57/8089158da9c14a17650842165eabc2cd2cbed8bb632cab3b9bd1577723ea/dropbox-11.17.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
