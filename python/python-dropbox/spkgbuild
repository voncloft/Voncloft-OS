name=python-dropbox
version=11.14.0
release=2

source="https://files.pythonhosted.org/packages/79/30/f69a07ee829ee66973c3105bda5bdcfac3603539a5b4e10a4d4d9bfb36ec/dropbox-11.14.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
