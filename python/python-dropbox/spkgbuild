name=python-dropbox
version=11.16.0
release=2

source="https://files.pythonhosted.org/packages/a3/e4/a79b389dd64310ec39e3809a418f7becc4726cae105a6fea63b1901958e1/dropbox-11.16.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
