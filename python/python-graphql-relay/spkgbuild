# description : A library to help construct a graphql-py server supporting react-relay
# depends : 

name=python-graphql-relay
version=3.1.0
release=3

source="https://files.pythonhosted.org/packages/db/68/c940238ad6c9cf2a5b415ee05b40983b5f686cfaabbdad527adc819f72d4/graphql-relay-3.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
