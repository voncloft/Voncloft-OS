# description : Library for build()ing powerful interactive command lines in Python
# depends : python-pygments

name=python-prompt_toolkit
version=3.0.14
release=1

source="https://files.pythonhosted.org/packages/b4/56/9ab5868f34ab2657fba7e2192f41316252ab04edbbeb2a8583759960a1a7/prompt_toolkit-3.0.20.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
