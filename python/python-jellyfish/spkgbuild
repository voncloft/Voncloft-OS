# description : A python library for doing approximate and phonetic matching of strings
# depends : python

name=python-jellyfish
version=0.8.4
release=3

source="https://files.pythonhosted.org/packages/1a/a7/cbf5298fae14dbcf1a381e882175f14e8099477a091eb3bcf87cbdb33d9d/jellyfish-0.8.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
