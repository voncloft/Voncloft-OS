# description : A python library for doing approximate and phonetic matching of strings
# depends : python

name=python-jellyfish
version=0.8.8
release=3

source="https://files.pythonhosted.org/packages/76/88/e6eba0ebd8a11eb0a03392d827f0a605ad45fbb24234f7db98ca1ecb41b2/jellyfish-0.8.8.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
