# description : A library for parsing ISO 8601 strings
# depends : python

name=python-aniso8601
version=8.1.1
release=1

source="https://bitbucket.org/nielsenb/aniso8601/get/v$version.tar.gz"
options="!checksum"

build() {
  cd aniso8601-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1
}
