# description : Enable git-like did-you-mean feature in click
# depends : python-click

name=python-click-didyoumean
version=0.0.3
release=3

source="https://files.pythonhosted.org/packages/9f/79/d265d783dd022541b744d002745d9e55d84c04a41930e35d8795934f6526/click-didyoumean-0.0.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
