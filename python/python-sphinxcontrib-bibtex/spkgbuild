# description : A Sphinx extension for BibTeX style citations
# depends : python-sphinx python-pybtex python-pybtex-docutils python-docutils

name=python-sphinxcontrib-bibtex
version=2.4.1
release=1

source="https://files.pythonhosted.org/packages/f4/10/ecad27f98f3ba90830c9b0d017a69b8fd491e036c10419e6ff5f042cc91b/sphinxcontrib-bibtex-2.4.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
