# description : asynchronous networking framework in Python
# depends : python

name=python-twisted
version=20.3.0
release=1

source="https://github.com/twisted/twisted/archive/twisted-$version.tar.gz"
options="!checksum"

build()
{
	cd twisted-$version
 	python setup.py build
        python setup.py install --root=$PKG --optimize=1
}
