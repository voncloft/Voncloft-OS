# description : Helper to test WSGI applications
# depends : python-webob python-six python-beautifulsoup4 python-waitress

name=python-webtest
version=3.0.0
release=4

source="https://files.pythonhosted.org/packages/26/c8/8ffba1782700eb06e9b9169156698c6ba95c05b66cda3fc9e025b6b3b649/WebTest-3.0.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
