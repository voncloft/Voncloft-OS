# description : Activate virtual environment for current shell
# depends : python-attrs python-pexpect python-shellingham

_name=dephell_shells
url=https://github.com/dephell/${_name}
name=python-dephell-shells
version=0.1.3
release=5

source="https://files.pythonhosted.org/packages/source/${_name:0:1}/${_name}/${_name}-${version}.tar.gz"
options="!checksum"

build() {
   cd ${_name}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
