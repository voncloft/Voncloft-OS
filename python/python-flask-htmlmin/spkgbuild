# description : Flask html response minifier
# depends : python python-flask python-htmlmin

name=python-flask-htmlmin
version=2.1.0
release=1

source="https://files.pythonhosted.org/packages/03/46/8d3d331ad33f3835c9ad7b42c3da01d52f1fee0365a01545b5f60b74cd67/Flask-HTMLmin-2.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
