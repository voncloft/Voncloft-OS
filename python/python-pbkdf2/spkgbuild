# description : Password-based key derivation function PBKDF2
# depends : python

name=python-pbkdf2
version=1.3
release=8

source="python-pbkdf2-1.3.tar.gz::https://github.com/dlitz/python-pbkdf2/archive/v1.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
