# depends : python-setuptools

name=python-pastedeploy
version=2.1.1
release=3

source="python-pastedeploy-2.1.1.tar.gz::https://github.com/Pylons/pastedeploy/archive/2.1.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
