# description : Hypothesis extension for generating filesystem paths
# depends : python-hypothesis

name=python-hypothesis-fspaths
version=0.1.0
release=6

source="python-hypothesis-fspaths-0.1.0.tar.gz::https://github.com/lazka/hypothesis-fspaths/archive/v0.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
