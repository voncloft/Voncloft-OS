release=1

source="https://files.pythonhosted.org/packages/1f/e3/a26e612df2d7d7adda0944a93e8ca4e3ce3178f95ac2a91a6d34a7241302/josepy-1.9.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
