# description : Data readers extracted from the pandas codebase
# depends : python-pandas python-lxml python-requests

name=python-pandas-datareader
version=0.10.0
release=2

source="https://files.pythonhosted.org/packages/cf/94/b0363da3981da77d3ec7990e89006e4d4f71fd71a82290ce5c85540a7019/pandas-datareader-0.10.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
