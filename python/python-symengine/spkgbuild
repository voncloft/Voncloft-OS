# description : Python wrappers for SymEngine
# depends : python symengine

name=python-symengine
version=0.8.1
release=1

source="https://files.pythonhosted.org/packages/f2/1f/67b68cbf2bc78a521d619bef7ce3fc699aacd48f678fe42058c61a85221e/symengine-0.8.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
