# description : Bundled swagger-ui
# depends : python-jinja

name=python-swagger-ui-bundle
version=0.0.9
release=2

source="python-swagger-ui-bundle-0.0.6.tar.gz::https://github.com/dtkav/swagger_ui_bundle/archive/v0.0.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
