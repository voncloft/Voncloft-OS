# description : Pathlib-compatible object wrapper for zip files
# depends : python

name=python-zipp
version=3.6.0
release=3

source="python-zipp-3.5.0.tar.gz::https://github.com/jaraco/zipp/archive/v3.5.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
