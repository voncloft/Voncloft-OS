# description : Sphinx theme used by Guzzle
# depends : python python-sphinx>=1.1.3

name=python-guzzle-sphinx-theme
version=0.7.11
release=8

source="guzzle_sphinx_theme-0.7.11.tar.gz::https://github.com/guzzle/guzzle_sphinx_theme/archive/0.7.11.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
