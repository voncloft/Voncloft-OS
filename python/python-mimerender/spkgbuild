# description : Transparently select a render function for an HTTP request handlers result
# depends : python-mimeparse

name=python-mimerender
version=0.6.0
release=6

source="python-mimerender-0.6.0.tar.gz::https://github.com/martinblech/mimerender/archive/v0.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
