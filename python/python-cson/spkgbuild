# description : A Python parser for Coffeescript Object Notation CSON
# depends : python python-six python-speg

name=python-cson
version=0.8
release=4

source="https://files.pythonhosted.org/packages/8c/14/461bdde1d712dae3c14e139dd4b37085085fd75f01d44f888767bfc6d313/cson-0.8.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
