# description : A Python parser for Coffeescript Object Notation CSON
# depends : python python-six python-speg

url=https://github.com/avakar/pycson
name=python-cson
version=0.8
release=4

source="https://github.com/avakar/pycson/archive/$version.tar.gz"
options="!checksum"

build() {
   cd pycson-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
