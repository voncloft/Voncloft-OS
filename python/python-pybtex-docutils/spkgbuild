# description : A docutils backend for pybtex
# depends : python-six python-docutils python-pybtex

name=python-pybtex-docutils
version=1.0.1
release=4

source="https://files.pythonhosted.org/packages/41/3b/69c21deab7974b76018124b441c059edc6d6cec970ac038e5f62682eac8a/pybtex-docutils-1.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
