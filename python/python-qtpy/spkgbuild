# description : Provides an uniform layer to support PyQt5, PyQt4 and PySide with a single codebase
# depends : python

name=python-qtpy
version=1.11.1
release=5

source="https://files.pythonhosted.org/packages/0b/09/db517eebc57145c738c280d0555490a4c06594593988f12fef2f3d139bb3/QtPy-1.11.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
