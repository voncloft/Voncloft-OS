# description : Provides an uniform layer to support PyQt5, PyQt4 and PySide with a single codebase
# depends : python

name=python-qtpy
version=1.10.0
release=5

source="https://files.pythonhosted.org/packages/dc/e2/30b27f276ead61a5296d1b36165f5a6147bdac22358768e12c4a1c2b28f5/QtPy-1.10.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
