# description : Provides an uniform layer to support PyQt5, PyQt4 and PySide with a single codebase
# depends : python

name=python-qtpy
version=1.11.0
release=5

source="https://files.pythonhosted.org/packages/7f/28/a76640d928cc74bd6d36791d1e4cccf1c37a4e590d326dbfe437ae49daf5/QtPy-1.11.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
