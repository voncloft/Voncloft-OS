source="https://files.pythonhosted.org/packages/04/cb/f66f77f44929adcc8cb2f162e7c65cab247ce5e2f8d671abf81518a9d79e/pook-1.0.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
