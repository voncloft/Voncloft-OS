# description : Sphinx themes for Pylons Project documentation
# depends : python

name=python-pylons-sphinx-themes
version=1.0.13
release=1

source="https://github.com/Pylons/pylons-sphinx-themes/archive/1.0.13/python-pylons-sphinx-themes-1.0.13.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
