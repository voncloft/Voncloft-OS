# description : Low-level AMQP client for Python fork of amqplib
# depends : python-vine

name=python-amqp
version=5.0.6
release=1

source="https://github.com/celery/py-amqp/archive/v5.0.6/python-amqp-5.0.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
