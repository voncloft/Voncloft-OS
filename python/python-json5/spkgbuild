# description : A Python implementation of the JSON5 data format
# depends : python

name=python-json5
version=0.9.6
release=3

source="https://github.com/dpranke/pyjson5/archive/v0.9.6/python-json5-0.9.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
