# description : General parsing module for Python
  # depends : python

name=python-pyparsing
version=3.0.0
release=3

source="https://github.com/pyparsing/pyparsing/archive/pyparsing_2.4.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
