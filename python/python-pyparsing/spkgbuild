# description : General parsing module for Python
  # depends : python

name=python-pyparsing
version=3.0.6
release=3

source="https://github.com/pyparsing/pyparsing/archive/refs/tags/r$version.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
