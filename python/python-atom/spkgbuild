# description : Memory efficient Python objects
# depends : python python-setuptools

name=python-atom
version=0.6.0
release=3

source="https://files.pythonhosted.org/packages/ae/35/e46cff349b1b16a4e695fcf8e2480b6bf69e00ce2eebfa8276e0693a8c75/atom-0.6.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
