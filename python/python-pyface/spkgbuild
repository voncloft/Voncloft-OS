# description : Traits-capable windowing framework
# depends : python-traits

name=python-pyface
version=7.3.0
release=3

source="pyface-7.3.0.tar.gz::https://github.com/enthought/pyface/archive/7.3.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
