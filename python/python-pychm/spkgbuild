# description : Python bindings for chmlib
# depends : chmlib python

name=python-pychm
version=0.8.6
release=4

source="python-pychm-0.8.6.tar.gz::https://github.com/dottedmag/pychm/archive/v0.8.6.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
