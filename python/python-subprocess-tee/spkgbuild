# description : A subprocess.run that works like tee
# depends : python

name=python-subprocess-tee
version=0.3.4
release=1

source="https://files.pythonhosted.org/packages/20/0a/aae37d9ec3f5688b9d42d95465c9807731f23601d3cfb18da88b9bf45f50/subprocess-tee-0.3.4.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
