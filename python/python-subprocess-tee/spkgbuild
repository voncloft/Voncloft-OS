# description : A subprocess.run that works like tee
# depends : python

name=python-subprocess-tee
version=0.3.3
release=1

source="https://files.pythonhosted.org/packages/82/bb/3b9894590d6d90b89c94cb4734206ace694a23740acaa536d91026933b10/subprocess-tee-0.3.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
