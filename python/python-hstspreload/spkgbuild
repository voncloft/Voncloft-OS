# description : Chromium HSTS Preload list as a Python package
# depends : python

name=python-hstspreload
version=2021.8.1
release=3

source="https://files.pythonhosted.org/packages/ec/27/6c2e82a857a430f57a9800b372ece11ba5ffe11f6eefa8037fb283eb9c3d/hstspreload-2021.8.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
