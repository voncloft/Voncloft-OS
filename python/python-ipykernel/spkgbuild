# description : The ipython kernel for Jupyter
  # depends : python-traitlets python-tornado ipython python-jupyter_client

name=python-ipykernel
version=6.0.2
release=1

source="https://files.pythonhosted.org/packages/e8/33/f964fd8ab35c6f516ea2a6b6327f30c3eb36d3a0b6a73c49006fbf2aee83/ipykernel-6.0.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
