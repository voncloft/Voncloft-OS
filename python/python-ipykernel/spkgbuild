# description : The ipython kernel for Jupyter
  # depends : python-traitlets python-tornado ipython python-jupyter_client

name=python-ipykernel
version=6.3.1
release=1

source="https://files.pythonhosted.org/packages/79/fc/89a1f0f560cf082226ed404d7a876c77519f6af84fd482551c862183c509/ipykernel-6.3.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
