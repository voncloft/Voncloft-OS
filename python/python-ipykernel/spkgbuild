# description : The ipython kernel for Jupyter
  # depends : python-traitlets python-tornado ipython python-jupyter_client

name=python-ipykernel
version=6.0.1
release=1

source="https://files.pythonhosted.org/packages/ed/8e/f652f8db2467782c137cd31b2b9fe4a305500e759d15fb0523fa8eeede29/ipykernel-6.0.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
