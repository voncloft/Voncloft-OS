# description : The ipython kernel for Jupyter
  # depends : python-traitlets python-tornado ipython python-jupyter_client

name=python-ipykernel
version=6.3.0
release=1

source="https://files.pythonhosted.org/packages/2f/1a/0134f80bb8b1f91c63d905871c57ad58488df98a0e05b215bab06d7e3633/ipykernel-6.3.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
