# description : The ipython kernel for Jupyter
  # depends : python-traitlets python-tornado ipython python-jupyter_client

name=python-ipykernel
version=6.1.0
release=1

source="https://files.pythonhosted.org/packages/5e/33/56890c7307c2ea65082df4449cb066ce060ba9ded51b9e9205e6ccb764d6/ipykernel-6.1.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
