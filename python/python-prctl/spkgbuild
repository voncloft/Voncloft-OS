# description : Pythonic interface to the linux prctl syscall
# depends : python libcap

name=python-prctl
version=1.8.1
release=1

source="python-prctl-1.8.1.tar.gz::https://github.com/seveas/python-prctl/archive/v1.8.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
