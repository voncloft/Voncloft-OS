# description : Ctypes-based simple MagickWand API binding for Python
# depends : imagemagick libxml2 python

name=python-wand
version=0.6.7
release=1

source="https://files.pythonhosted.org/packages/a8/74/e544ba1f77d381d76992905c319f3564f7a05a66e18c5490542159833707/Wand-0.6.7.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
