# description : Easy extensible command line tab completion of arguments for your Python script
# depends : python

name=python-argcomplete
version=1.12.3
release=1

source="https://files.pythonhosted.org/packages/6a/b4/3b1d48b61be122c95f4a770b2f42fc2552857616feba4d51f34611bd1352/argcomplete-1.12.3.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
