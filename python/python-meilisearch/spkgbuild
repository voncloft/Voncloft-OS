name=python-meilisearch
version=0.16.0
release=1

source="https://files.pythonhosted.org/packages/e8/5b/123c6ba9d018a92de5df143890b1bfd44b551ce8e832ec3673f93ca7a11e/meilisearch-0.16.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
