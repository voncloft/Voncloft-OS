# description : AST that abstracts the underlying Python version 
# depends : python

name=python-gast
version=0.5.1
release=3

source="https://files.pythonhosted.org/packages/b7/9c/45b706be126aebf3491b07450f3ba419039b6d0b5a9cc0991b5598eedddd/gast-0.5.1.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
