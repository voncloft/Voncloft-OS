# description : Python interfaces to PROJ.4 library
# depends : proj python

name=python-pyproj
version=3.2.0
release=3

source="https://files.pythonhosted.org/packages/2e/9f/217f0cfb328b57a5e471e3d8e1eb58376086ffb50ce74f9977b5bb86f56a/pyproj-3.2.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
