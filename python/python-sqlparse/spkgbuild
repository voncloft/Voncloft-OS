# description : Non-validating SQL parser for Python
# depends : python

name=python-sqlparse
version=0.4.2
release=3

source="python-sqlparse-0.4.2.tar.gz::https://github.com/andialbrecht/sqlparse/archive/0.4.2.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
