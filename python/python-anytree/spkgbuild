name=python-anytree
version=2.8.0
release=3

source="python-anytree::git+https://github.com/c0fec0de/anytree#commit=b3e935355afcfb5240bac5a99707ffecc35772a2"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
