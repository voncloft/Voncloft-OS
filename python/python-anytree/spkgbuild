# description : Powerful and Lightweight Python Tree Data Structure
# depends : python-six

name=python-anytree
version=2.8.0
release=3

source="https://files.pythonhosted.org/packages/d8/45/de59861abc8cb66e9e95c02b214be4d52900aa92ce34241a957dcf1d569d/anytree-2.8.0.tar.gz"
options="!checksum"

build() {
   cd *
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
