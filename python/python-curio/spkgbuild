# description : Concurrent I/O
# depends : python

url=https://github.com/dabeaz/curio
name=python-curio
version=1.4
release=3

source="https://github.com/dabeaz/curio/archive/$version.tar.gz"
options="!checksum"

build() {
   cd curio-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
