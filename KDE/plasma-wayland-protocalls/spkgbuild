# description : Plasma-wayland-protocals
# depends : extra-cmake-modules

name=plasma-wayland-protocals
version=1.1.1
release=1

source=https://download.kde.org/stable/plasma-wayland-protocols/plasma-wayland-protocols-1.1.1.tar.xz

options="!checksums"
build()
{
	cd $name-$version

	mkdir build &&
	cd    build &&

	cmake -DCMAKE_INSTALL_PREFIX=/opt/kf5 ..

	make DESTDIR=$PKG install
	
}
