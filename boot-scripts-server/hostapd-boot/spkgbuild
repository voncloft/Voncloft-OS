# description : daemon to start wifi for router
# dependencies : hostapd

name=hostapd-boot
version=2.9
release=4

source="http://voncloft.dnsfor.me/software/Voncloft-OS/bootscripts.tar.gz"
options="!checksum"

build() {
        cd blfs-bootscripts-20210110
        make DESTDIR=$PKG install-hostapd-boot

}
