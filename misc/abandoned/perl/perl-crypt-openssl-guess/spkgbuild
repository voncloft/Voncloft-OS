# description : Guess OpenSSL include path
# depends : perl

_ddir=Crypt-OpenSSL-Guess-$version
name=perl-crypt-openssl-guess
version=0.11
release=5

source="https://search.cpan.org/CPAN/authors/id/A/AK/AKIYM/Crypt-OpenSSL-Guess-$version.tar.gz"
options="!checksum"

build() {
   cd $_ddir
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
