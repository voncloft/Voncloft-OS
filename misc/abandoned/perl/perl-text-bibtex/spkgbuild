# description : Perl interface to read and parse BibTeX files
# depends : perl

_dist=Text-BibTeX
name=perl-text-bibtex
version=0.85
release=6

source="https://search.cpan.org/CPAN/authors/id/A/AM/AMBS/${_dist}-${version}.tar.gz"
options="!checksum"

build() {
     cd ${_dist}-${version}
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
