# description : Thoroughly testing objects via registered handlers
# depends : perl

name=perl-test-object
version=0.08
release=3

source="https://cpan.metacpan.org/authors/id/E/ET/ETHER/Test-Object-$version.tar.gz"
options="!checksum"

build() {
   cd Test-Object-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
