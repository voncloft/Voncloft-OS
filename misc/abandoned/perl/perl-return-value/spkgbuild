# description : Polymorphic Return Values 
# depends : perl

name=perl-return-value
version=1.666005
release=4

source="https://www.cpan.org/authors/id/R/RJ/RJBS/Return-Value-$version.tar.gz"
options="!checksum"

build() {
   cd  Return-Value-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
