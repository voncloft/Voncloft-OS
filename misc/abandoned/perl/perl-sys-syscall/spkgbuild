# description : Perl/CPAN Module Sys::Syscall
# depends : perl

name=perl-sys-syscall
version=0.25
release=7

source="https://search.cpan.org/CPAN/authors/id/B/BR/BRADFITZ/Sys-Syscall-$version.tar.gz"
options="!checksum"

build() {
   cd Sys-Syscall-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
