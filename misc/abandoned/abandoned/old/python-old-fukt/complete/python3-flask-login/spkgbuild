# description : User session management for Flask 
# depends : python3 python3-nose python3-mock python3-blinker python3-semantic-version

name=python3-flask-login
version=0.5.0
release=3

source="https://github.com/maxcountryman/flask-login/archive/$version.tar.gz"
options="!checksum"

build()
{
	cd flask-login-$version
	python3 setup.py build
	python setup.py install --root=$PKG --prefix=/usr --optimize=1
}
