# description : Quick and small memcached client for Python 
# depends :   libmemcached   python  

name=python-pylibmc
version=1.6.1
release=5



source="https://pypi.org/build()s/source/p/pylibmc/pylibmc-$version.tar.gz" 


build()   {
	cd pylibmc-"$version"
	python setup.py install --root=$PKG
}
