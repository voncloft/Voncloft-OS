# description : Python implementation of Extended Window Manager Hints, based on Xlib"
# depends :   python-xlib  

name=python-ewmh
version=0.1.6
release=3
source="https://github.com/parkouss/pyewmh/archive/v$version.tar.gz" 

build-old()   {
  cd pyewmh-$version
  python setup.py build-old()
}

build()   {
  cd pyewmh-$version
  python setup.py install --root=$PKG --optimize=1
}
