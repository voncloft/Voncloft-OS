# description : An implementation of time.monotonic   for Python 2."

_name=monotonic
name=  python-monotonic   python2-monotonic  
version=1.5
release=6
source="$_name-$version.tar.gz::https://github.com/atdt/monotonic/archive/$version.tar.gz" 

build()_python-monotonic   {
  # depends :   python  

  cd "$srcdir/$_name-$version"
  python setup.py install --root=$PKG --optimize=1
}

build()_python2-monotonic   {
  # depends :   python2  

  cd "$srcdir/$_name-$version"
  python2 setup.py install --root=$PKG --optimize=1
}
