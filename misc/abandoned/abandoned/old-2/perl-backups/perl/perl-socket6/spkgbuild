# description : A getaddrinfo/getnameinfo support module
# depends : perl

name=perl-socket6
version=0.29
release=5

source="https://search.cpan.org/CPAN/authors/id/U/UM/UMEMOTO/Socket6-$version.tar.gz"
options="!checksum"

build() {
   cd Socket6-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
