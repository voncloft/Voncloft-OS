# description : Perl/CPAN Module Object::Event
# depends : perl

name=perl-object-event
version=1.23
release=7

source="https://search.cpan.org/CPAN/authors/id/E/EL/ELMEX/Object-Event-$version.tar.gz"
options="!checksum"

build() {
   cd Object-Event-$version
   perl Makefile.PL
   make
   make DESTDIR=$PKG install
}
