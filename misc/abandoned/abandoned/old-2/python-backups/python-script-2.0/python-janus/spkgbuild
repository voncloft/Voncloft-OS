# description : Thread-safe asyncio-aware queue for Python
# depends : python

url=https://github.com/aio-libs/janus
name=python-janus
version=0.6.1
release=3

source="https://github.com/aio-libs/janus/archive/v$version/$name-$version.tar.gz"
options="checksum"

build() {
   cd janus-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
