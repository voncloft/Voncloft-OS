# description : Python2s stdlib csv module is nice, but it doesnt support unicode. This module is a drop-in replacement which *does*.
# depends : python2

url=https://github.com/jdunck/python-unicodecsv
name=python2-unicodecsv
version=0.14.1
release=4

source="https://github.com/jdunck/python-unicodecsv/archive/$version.tar.gz"

build() {
   cd python-unicodecsv-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
