# description : A high level API for internationalization
# depends : python-atpublic

_name=flufl.i18n
url=https://gitlab.com/warsaw/flufl.i18n
_name=flufl.i18n
name=python-flufl.i18n
version=3.1.4
release=1

source="https://files.pythonhosted.org/packages/source/${_name::1}/${_name}/${_name}-${version}.tar.gz"
options="checksum"

build() {
   cd $name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
