# description : Linux /proc abstraction classes for python
# depends : python-six

url=https://git.kernel.org/pub/scm/libs/python/python-linux-procfs/python-linux-procfs.git/
name=python-linux-procfs
version=0.6.3
release=1

source="https://git.kernel.org/pub/scm/libs/python/${name}/${name}.git/snapshot/${name}-${version}.tar.gz"
options="checksum"

build() {
   cd ${name}-${version}
   cd ${name[0]}-${version}
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
