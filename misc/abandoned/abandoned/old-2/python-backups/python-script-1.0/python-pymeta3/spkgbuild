# description : A Pattern-Matching Language Based on Python
# depends : python

_name=pymeta3
url=https://github.com/wbond/pymeta3
_name=pymeta3
name=python-$_name
version=0.5.1
release=3

source="$url/archive/$version.tar.gz"
options="checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
