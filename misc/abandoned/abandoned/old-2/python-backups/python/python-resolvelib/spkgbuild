# description : "Resolve abstract dependencies into concrete ones"

pkgbase=python-resolvelib
version=0.5.4
release=1
url="https://github.com/sarugaku/resolvelib"
check# depends : python-commentjson python-pytest-runner
source="$pkgbase-$version.tar.gz::https://github.com/sarugaku/resolvelib/archive/$version.tar.gz"
sha512sums=9e6a92df27c9411d20a476fb222b865b3e7ec72bcfcb37c0ba77fe8d5912379eecd0e051850d443e756f36e012e8aa705442b880b4bdbfd8569b8fc887ef9f3c

build()-delete {
   cd resolvelib-$version
  python setup.py build
}

check {
   cd resolvelib-$version
  python setup.py pytest
}

package_python-resolvelib {
  # depends : python

   cd resolvelib-$version
  python setup.py build
  python setup.py install --root=$PKG --optimize=1

}


   cd resolvelib-$version

}
