# description : Pony Object Relational Mapper
# depends : python

_name=pony
url=https://github.com/ponyorm/pony
_name=pony
name=python-$_name
version=0.7.14
release=2

source="$url/archive/v$version.tar.gz"
options="checksum"

build() {
   cd $_name-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
