url=https://github.com/python-hyper/hyperlink
version=21.0.0
release=1

source="https://github.com/python-hyper/hyperlink/archive/v$version.tar.gz"

build() {
   cd hyperlink-$version
   cd hyperlink-$version
   python setup.py build
   python setup.py install --optimize=1 --root=$PKG
 }
