# description : subsonic boot script
# depends :

name=subsonic-boot
version=6.1.6
release=1

build()
{
	cd $name-$version
	mkdir $PKG/etc/init.d
	install $SRC/subsonic $PKG/etc/init.d/
	ln -sfv /etc/init.d/subsonic /etc/rc.d/rc5.d/S99subsonic
	ln -sfv /etc/init.d/subsonic /etc/rc.d/rc4.d/S99subsonic
	ln -sfv /etc/init.d/subsonic /etc/rc.d/rc3.d/S99subsonic

}
