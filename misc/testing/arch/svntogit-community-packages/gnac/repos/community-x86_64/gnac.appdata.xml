  </description>
  <screenshots>
    <screenshot type="default">
      <image>http://gnac.sourceforge.net/images/gnac.png</image>
    </screenshot>
  </screenshots>
  <url type="bugtracker">https://gitlab.gnome.org/GNOME/gnac/issues</url>
  <url type="homepage">http://gnac.sourceforge.net/</url>
  <update_contact>gnac-list@gnome.org</update_contact>
  <translation type="gettext">gnac</translation>
</component>
