
name=python-geoip
version=1.3.2
release=10
# description : "Python bindings for the GeoIP IP-to-country resolver library"
# depends : ('python' 'geoip')
source=(https://pypi.python.org/packages/source/G/GeoIP/GeoIP-${version}.tar.gz)

build-remove() {
  cd "GeoIP-$version"
  python setup.py build
}

package_python-geoip() {
  cd "GeoIP-$version"
  python setup.py install --root=$PKG -O1
}

