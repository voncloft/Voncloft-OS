
_name=libusb1
name=python-$_name
version=1.9.1
release=1
# description : Python ctype-based wrapper around libusb1 
# depends :   python   libusb  
source=" $url/archive/$version.tar.gz" 

build   {
  cd $name-$version

  python setup.py build
}

package   {
  cd $name-$version

  python setup.py install --root=$PKG --optimize=1 --skip-build
}

