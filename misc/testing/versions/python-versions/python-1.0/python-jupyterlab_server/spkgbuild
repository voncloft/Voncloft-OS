
_pipname=jupyterlab_server
name=python-$_pipname
version=2.2.0
release=1
# description : Launch an application built using JupyterLab"
# depends :  jupyter-server python-requests python-json5 python-babel 
conflicts= jupyterlab_server 
replaces= jupyterlab_server 
source=https://pypi.io/packages/source/j/$_pipname/$_pipname-$version.tar.gz 

build   {
  cd $_pipname-$version
  python setup.py build 
}

package   {
  cd $_pipname-$version
  python setup.py install --skip-build --root=$PKG --optimize=1

  install -Dm644 LICENSE $PKG/usr/share/licenses/$name/LICENSE
}
