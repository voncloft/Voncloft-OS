
_name=hawkauthlib
name=python-hawkauthlib
version=2.0.0
release=7
# description : Low-level library for implementing MAC Access Authentication"
# depends :   python-requests   python-webob  
source="https://github.com/mozilla-services/$_name/archive/v$version.tar.gz" 

build   {
  cd $_name-$version
  python3 setup.py build
}

package   {
  cd $_name-$version 
  python3 setup.py install --root=$PKG --optimize=1
}
