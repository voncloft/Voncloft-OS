
name=python-authres
version=1.2.0
release=5
# description : Python Authentication-Results headers generation and parsing 
# depends :   python  
source="https://files.pythonhosted.org/packages/source/a/authres/authres-$version.tar.gz" 
 
build   {
  cd authres-$version
  python setup.py build 
}

check   {
  cd authres-$version
  python -m authres -v
}

package   {
  cd authres-$version
  python setup.py install --root=$PKG --optimize=1 --skip-build
}
