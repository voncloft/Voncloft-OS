
name=python-mypy_extensions
version=0.4.3
release=3
# description : Experimental type system extensions for programs checked with the mypy typechecker 
# depends :   python  
source="https://pypi.org/packages/source/m/mypy_extensions/mypy_extensions-$version.tar.gz" 

build   {
    cd "mypy_extensions-$version"
    python setup.py build
}

package   {
    cd "mypy_extensions-$version"
    python setup.py install --prefix="/usr" --root=$PKG --optimize=1 --skip-build
    install -Dm644 LICENSE "$PKG/usr/share/licenses/$name/LICENSE"
}
