
name=python-pygeoip
version=0.3.2
release=7
# description : Pure Python GeoIP API"
# depends :   python  
source="https://pypi.python.org/packages/source/p/pygeoip/pygeoip-$version.tar.gz" 

build   {
  cd pygeoip-$version
  python setup.py build
}

check   {
  cd pygeoip-$version
  nosetests3
}

package   {
  cd pygeoip-$version
  python setup.py install --root=$PKG --optimize=1 --skip-build
}
