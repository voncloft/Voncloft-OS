
name=python-oset
version=0.1.3
release=7
# description : Ordered sets for Python"
source="https://pypi.io/packages/source/o/oset/oset-$version.tar.gz" 

build   {
  cd "$srcdir/oset-$version"
  python setup.py build
}

package   {
  cd "$srcdir/oset-$version"
  python setup.py install --root=$PKG --optimize=1 --skip-build
  install -Dm644 LICENSE.rst "$PKG/usr/share/licenses/$name/LICENSE"
}
