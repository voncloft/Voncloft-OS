
name=python-repoze.profile
version=2.3
release=4
# description : Aggregate profiling for WSGI requests"
# depends :   python  
source="https://pypi.io/packages/source/r/repoze.profile/repoze.profile-$version.tar.gz" 

build   {
  cd repoze.profile-$version
  python setup.py build
}

check   {
  cd repoze.profile-$version
  nosetests3
}

package   {
  cd repoze.profile-$version
  python setup.py install --root=$PKG

  install -Dm644 LICENSE.txt -t $PKG/usr/share/licenses/$name
}
