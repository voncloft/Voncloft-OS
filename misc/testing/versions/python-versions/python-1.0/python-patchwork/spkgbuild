
name=python-patchwork
version=1.0.1
release=4
# description : Common deployment/sysadmin operations, built on Fabric"
# depends :   fabric  
source="https://github.com/fabric/patchwork/archive/$version/$name-$version.tar.gz" 

build   {
  cd "$srcdir"/patchwork-$version
  python setup.py build
}

package   {
  cd "$srcdir"/patchwork-$version
  python setup.py install -O1 --skip-build --root=$PKG
  install -Dm0644 LICENSE $PKG/usr/share/licenses/$name/LICENSE
}
