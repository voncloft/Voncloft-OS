
name=python-pywinrm
version=0.4.1
release=4
# description : Python library for Windows Remote Management"
# depends :   python   python-requests   python-xmltodict   python-requests-ntlm  
             python-requests-credssp: for CredSSP authenication  
source="https://files.pythonhosted.org/packages/source/p/pywinrm/pywinrm-${version}.tar.gz" 

package   {
    cd pywinrm-${version}
    python setup.py install --root=$PKG/ --optimize=1
}

