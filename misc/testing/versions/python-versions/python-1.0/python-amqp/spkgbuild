
name=python-amqp
version=5.0.5
release=1
# description : Low-level AMQP client for Python  fork of amqplib "
# depends :   python-vine  
source="https://github.com/celery/py-amqp/archive/v$version/$name-$version.tar.gz" 

build   {
  cd py-amqp-$version
  python setup.py build
}

check   {
  cd py-amqp-$version
  pytest
}

package   {
  cd py-amqp-$version
  python setup.py install --root=$PKG --optimize=1
}

# vim:set ts=2 sw=2 et:
