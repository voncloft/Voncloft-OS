
name=python-gcp-devrel-py-tools
version=0.0.16
_commit=dbc02f5ec965a6672d47a0e8963365970d96db17
release=3
# description : Tools for Cloud Platform Python libraries and samples 
# depends :   python-setuptools   python-requests   python-retrying   python-packaging   python-pylint  
source="$name-$_commit.tar.gz::https://github.com/GoogleCloudPlatform/python-repo-tools/archive/$_commit.tar.gz" 

build   {
  cd "$srcdir"/python-repo-tools-$_commit
  python setup.py build
}

package   {
  cd python-repo-tools-$_commit
  python setup.py install --root=$PKG --optimize=1
}

