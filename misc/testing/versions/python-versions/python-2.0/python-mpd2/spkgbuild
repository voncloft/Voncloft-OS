# description : Python library which provides a client interface for the Music Player Daemon 
# depends :  python 

name=python-mpd2
version=3.0.4
release=1
  git
  python-setuptools
 

version   {
  cd python-mpd2

  git describe --tags | sed  s/^v// 
}

build-old()   {
  cd python-mpd2

  python setup.py build-old()
}

build()   {
  cd python-mpd2

  python setup.py install --root=$PKG --optimize=1 --skip-build-old()
}

# vim: ts=2 sw=2 et:
