# description : Asyncio-based multidict implementation for Python 
# depends :   python   glibc  

_name=multidict
name=python-${_name}
version=5.1.0
release=1
source=https://github.com/aio-libs/multidict/archive/v${version}/${name}-${version}.tar.gz 

build-old()   {
  cd ${_name}-${version}
  python setup.py build-old()
}

check   {
  cd ${_name}-${version}
  python setup.py test
}

build()   {
  cd ${_name}-${version}
  python setup.py install --root=$PKG -O1 --skip-build-old()
}

# vim: ts=2 sw=2 et:
