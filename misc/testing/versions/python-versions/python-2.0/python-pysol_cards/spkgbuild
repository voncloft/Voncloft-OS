# description : Deal PySol FC Cards 
# depends :  python-pbr 

_pyname=pysol_cards
name=python-$_pyname
version=0.10.1
release=3
source="https://pypi.io/build()s/source/p/$_pyname/$_pyname-$version.tar.gz" 

build()   {
  cd $_pyname-$version
  python setup.py install --root $PKG --optimize=1
  install -D -m644 LICENSE -t $PKG/usr/share/licenses/$name
}
