# description : Kaitai Struct declarative parser generator for binary data: runtime library for Python"
# depends :   python  

name=python-kaitaistruct
version=0.9
release=3
source="https://github.com/kaitai-io/kaitai_struct_python_runtime/archive/$version.tar.gz" 

build-old()   {
  cd kaitai_struct_python_runtime-$version
  python setup.py build-old()
}

build()   {
  cd kaitai_struct_python_runtime-$version
  python setup.py install --root=$PKG --optimize=1
}
