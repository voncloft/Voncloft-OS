# description : Python module to parse metadata in video files. 
# depends :   python  
name= python-logutils 
_pkgbase="${name#python-}"
version=0.3.5
release=8
source="https://pypi.python.org/build()s/source/l/logutils/logutils-${version}.tar.gz" 

check   {
  cd "${srcdir}/${_pkgbase}-${version}"
  python ./setup.py test
}

build()   {
  cd "${srcdir}/${_pkgbase}-${version}"
  python ./setup.py install --root="$PKG/" --optimize=1
}
