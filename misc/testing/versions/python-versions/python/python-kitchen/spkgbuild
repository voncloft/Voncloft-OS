# description : Useful snippets of python code"
# depends :   python  
name=python-kitchen
version=1.2.6
release=5
source=" $url/archive/$version.tar.gz" 

build-old()   {
    cd "kitchen-$version"
    python setup.py build-old()
}

build()   {
    cd "kitchen-$version"
    python setup.py install --root=$PKG --optimize=1 --skip-build-old()
}
