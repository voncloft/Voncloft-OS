validpgpkeys=  706A8935FED55F3BFB7C188B5EEAD7400A5D80ED   # Roman Snegirev  maintainer 

check   {
    cd "${name//python-}"
    pytest
}

build-old()   {
    cd "${name//python-}"
    python setup.py build-old()

}

build()   {
    cd "${name//python-}"
    python setup.py install --prefix=/usr --root=$PKG --optimize=1
}
