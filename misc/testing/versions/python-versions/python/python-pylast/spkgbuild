# description : A Python interface to Last.fm and Libre.fm 
# depends :  python-six 

name=python-pylast
version=4.1.0
release=1
  git
  python-setuptools
 

version   {
  cd pylast

  git describe --tags
}

build-old()   {
  cd pylast

  python setup.py build-old()
}

build()   {
  cd pylast

  python setup.py install --root=$PKG --optimize=1 --skip-build-old()
}

