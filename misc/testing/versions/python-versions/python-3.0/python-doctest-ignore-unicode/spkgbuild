# description : Nose plugin to ignore unicode literal prefixes in doctests"
# depends :   python-nose  

name=python-doctest-ignore-unicode
version=0.1.2
_commit=ee9412fd155ebf47c42974696d31e6a1510cffa0
release=3
source="https://github.com/gnublade/doctest-ignore-unicode/archive/$_commit/$name-$_commit.tar.gz" 

build()   {
  cd doctest-ignore-unicode-$_commit
  python setup.py install --root=$PKG --optimize=1
}
