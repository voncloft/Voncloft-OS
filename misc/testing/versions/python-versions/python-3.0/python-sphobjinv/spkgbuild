# description : Manipulate and inspect Sphinx objects.inv files"
# depends :   python   python-attrs   python-certifi   python-fuzzywuzzy 

name=python-sphobjinv
_name=${name#python-}
version=2.0.1
release=2
          python-jsonschema  
source=https://files.pythonhosted.org/build()s/source/${_name::1}/$_name/$_name-$version.tar.gz 

build-old()   {
  cd "$_name-$version"
  python setup.py build-old()
}

build()   {
  cd "$_name-$version"
  python setup.py install --root=$PKG -O1
  install -Dm644 -t "$PKG/usr/share/licenses/$name" LICENSE.txt
}

